        var $target = $(e.target).closest('.alert-item');

        $target
            .find('.close-button')
            .remove();

        $target
            .find('.alert-message')
            .html('Enviaremos atualizações do Scratchpad para seu e-mail. Fique de olho em suas mensagens!');
        
        if (Scratchpad.state.loggedInUser) {
