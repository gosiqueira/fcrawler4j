    function isLogged(){
        if ("" != '') {
            return true
        }else{
            return false
        }
    }

    /* Función que pinta los últimos Mensajes */
    function pintarMensajesCabecera(mensajes) {


        /* Update de Nombre de Usuario */        
        if (isLogged()){
            if(newAccount=="0"){
                username = "user"
                if (mensajes['Name'] != ""){
                    username = mensajes['Name']
                }
                $("#usuario").html(username);
                $("#enlaceMensajes").unbind();
                $("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa);  
            }else{
                $('#enlanceMensajes').attr('href', 'https://ssl.logitravel.pt/operativalogi/ResponsiveWeb/Operational/clientsData.aspx?sesion=' + sesion + '&accion=paginar&operaciones=4');
            }

            $("#enlaceIniSesion").unbind();
            $("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa);
        }



        /* DIV: XX mensajes sin leer */
        var divTagQuedan = document.getElementById("sinLeer");
     
        var numMens= 0; 
        if (mensajes['Messages'] != undefined || mensajes['Messages'] != null){
            numMens =Object.keys(mensajes['Messages']).length;
        }
        
        if (numMens== 0){
            divTagQuedan.innerHTML = "Não tem nenhuma mensagem";
            $("#listaMensajes").hide();
            $("#verTodos").hide();
            return;
        }
        $("#listaMensajes").show();
        $("#verTodos").show();

        var noLeidos = mensajes['UnreadMessages'];
        
        var aHref = "https://ssl.logitravel.pt/operativalogi/ResponsiveWeb/operational/clientsData.aspx?sesion=5eed3b61-f54e-4bc4-890d-9de5e9052eda&accion=paginar&operaciones=4";
        

