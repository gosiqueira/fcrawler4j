<script type="text/javascript">
    var cookie = "tokenlogitravelpt" ;
    
    function forzarRefrescoSearchesData() {
        vaciarCacheBusquedas();
        loadSearchesData();
    }
    //function hacerBindBusquedas(){$("#enlaceBusquedas").unbind();$("#enlaceBusquedas").bind("click",{cFlotante: 'cFlotanteBusquedas'}, abrirCapa)}

    function hacerBindSesion(){$("#enlaceIniSesion").unbind();$("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa)}
    function hacerBindMensajes(){$("#enlaceMensajes").unbind();$("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa)}

    function abrirCapa(event) {
            
        switch(event.target.id)
        {
            case "enlaceIniSesion":
                $("#cFlotanteBusquedas").hide();
                $("#cFlotanteMensajes").hide();           
                break;
            case "usuario":
                $("#cFlotanteBusquedas").hide();
                $("#cFlotanteMensajes").hide();           
                break;

            case "enlaceMensajes":
                $("#cFlotantePerfil").hide();
                $("#cFlotanteBusquedas").hide();                    
                break;
            case "enlaceBusquedas":
                $("#cFlotantePerfil").hide();
                $("#cFlotanteMensajes").hide();
                break;
        }

        var boton = event.target.id;
        if(boton=="usuario"){boton="enlaceIniSesion";}

        $("#"+event.data.cFlotante).toggle();
        if ($('#'+event.data.cFlotante).is(':visible')){$("#"+boton).unbind("click");}
        $('#'+event.data.cFlotante).focus();            
                
    }
    function loadSearchesData() {
        var busquedas;
        var topBusquedas = 5;
        var fecha = new Date();
        var timestamp = fecha.getTime();
            
        if ((!localStorage.userBusquedas) || (localStorage.userBusquedas == '') ||
                (!localStorage.userBusquedasLastUpdate) || (new Date().setTime(fecha.getTime() - 300000) > localStorage.userBusquedasLastUpdate)) {

            $.ajax({
                url: "https://api.logitravel.com/account/api/Searches/getSearches",
                    data: {
                        idioma: "pt",
                        USW_codigo: "32557928322",
                        top: topBusquedas
                    },
                    cache: false,
                    dataType: "jsonp",
                    contentType: "charset=iso-8859-1",
                    beforeSend: function (){
                        $("#listaBusquedas").empty();
                        $("#cargaBusquedas").show();
                    },
                    success: function (response) {                    
                        busquedas = JSON.stringify(response);
                        vaciarCacheBusquedas();
                        localStorage.userBusquedas = busquedas;
                        localStorage.userBusquedasLastUpdate = timestamp;
                        pintarUltimasBusquedasCabecera(JSON.parse(busquedas));                    
                    },
                    complete: function () {
                        $("#cargaBusquedas").hide();
                    }              
                });
            
            _gaq.push(['_trackEvent', 'AccountHeader', "BusquedasClick", "LoadSearchesData",0,true]);
            
        }else{                
            pintarUltimasBusquedasCabecera(JSON.parse(localStorage.userBusquedas));
        }
    }
    function showUserMenu() {
    
        if(document.getElementById("cFlotanteUser").style.display == 'block'){
            document.getElementById("cFlotanteUser").style.display ='none';
        }else{
            document.getElementById("cFlotanteUser").style.display = 'block';
            getUserMessages();
            
        } 
    }
    function getUserMessages(){
        $.ajax({
            url: "https://api.logitravel.com/account/api/Message/GetUnreadMessagesCount?token=&sessionCode=5eed3b61-f54e-4bc4-890d-9de5e9052eda",
            dataType: "jsonp",
            contentType: "charset=iso-8859-1",
            success: function (response) {
                $('#numMsg').html(response);
            }
        });
    }
    function getUserData(){
        $.ajax({
            url: "https://api.logitravel.com/account/api/Customer/",
            dataType: "jsonp",
            contentType: "charset=iso-8859-1",
            success: function (response) {
                $('#usuario').html(response.Name);
                $('#UserName').html(response.Name);
            }
        });
    }
    $(document).ready(function () {
        if(newAccount=="1"){
            if (isLogged()){
                $('#userCabecera').bind('click', function(e){ 
                    e.stopPropagation();
                    getUserMessages();
                    $('#cFlotanteUser').show()
                    $('html').click(function (evt) {
                        if (evt.target.id != 'cFlotanteUser') {
                            $('#cFlotanteUser').hide()
                            $('html').unbind()
                        }
                    })
                });
            }
        }else{
            loadSearchesData();

            $("#refrescarBusquedas").unbind();
            $("#refrescarBusquedas").bind("click",function(){
                forzarRefrescoSearchesData();
            });

            $("#enlaceBusquedas").unbind();
            $("#enlaceBusquedas").bind("click",{cFlotante: 'cFlotanteBusquedas'}, abrirCapa);
        
            if (isLogged()) {
                $("#enlaceIniSesion").unbind();
                $("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa);
                $("#enlaceMensajes").unbind();
                $("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa);       
            }

        
            $("#cFlotanteBusquedas").bind("blur", function(event) {
                var parent="body";
            
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }

                if(parent.indexOf("cFlotanteBusquedas")==-1){
                    $("#cFlotanteBusquedas").hide();                        
                    setTimeout(hacerBindBusquedas, 500); 
                }else{
                    $("#cFlotanteBusquedas").focus();                                 
                }
            });


            $("#cFlotantePerfil").bind("blur", function(event) {
                var parent="body";
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }

                if(parent.indexOf("cFlotantePerfil")==-1){
                    $("#cFlotantePerfil").hide();                        
                    setTimeout(hacerBindSesion, 500); 
                }else{
                    $("#cFlotantePerfil").focus();
                }
            });
            $("#cFlotanteMensajes").bind("blur", function(event) {
                var parent="body";
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }
                if(parent.indexOf("cFlotanteMensajes")==-1){
                    $("#cFlotanteMensajes").hide();                        
                    setTimeout(hacerBindMensajes, 500); 
                }else{
                    $("#cFlotanteMensajes").focus();
                }
            });

        


            $.LoadAccountData = function () {
                if(newAccount=="1"){
                    $.ajax({               
                        url:  "https://api.logitravel.com/account/api/Customer//Messages?sessionCode=5eed3b61-f54e-4bc4-890d-9de5e9052eda",
                        dataType: "jsonp",
                        contentType: "charset=iso-8859-1",
                        success: function (response) {
                            var ResNew = { 'Messages': response };
                            mensajes = JSON.stringify(ResNew);
                            vaciarCacheMensajes();
                            localStorage.userMessages = mensajes;
                            localStorage.userMessagesLastUpdate = timestamp;
                            pintarMensajesCabecera(JSON.parse(mensajes));
                        }
                    });
                } else {
                    var mensajes;
                    var fecha = new Date();
                    var timestamp = fecha.getTime();
                    if ((!localStorage.userMessages) || (localStorage.userMessages == '') ||
                        (!localStorage.userMessagesLastUpdate) || (new Date().setTime(fecha.getTime() - 60000) > localStorage.userMessagesLastUpdate)) {
                    $.ajax({
                        url: "https://api.logitravel.com/account/api/user/messages",
                        data: {
                            sessionCode: "5eed3b61-f54e-4bc4-890d-9de5e9052eda",
                            token: "",
                            top: 10,
                            usw_codigo: 32557928322
                                },
                        dataType: "jsonp",
                        contentType: "charset=iso-8859-1",
                        success: function (response) {
                            mensajes = JSON.stringify(response);
                            vaciarCacheMensajes();
                            localStorage.userMessages = mensajes;
                            localStorage.userMessagesLastUpdate = timestamp;
                            pintarMensajesCabecera(JSON.parse(mensajes));
                        }
                    });
                        
                    _gaq.push(['_trackEvent', 'AccountHeader', "Cabecera", "LoadAccountData",0,true]);
                        
                } else {
                    pintarMensajesCabecera(JSON.parse(localStorage.userMessages));
                }
            }
        }

        $.LoadAccountData();


        setInterval(function () { $.LoadAccountData() }, 30000);

        setInterval(function () { 
            $("#listaBusquedas").empty();loadSearchesData() }, 300000);
        

        $.ReloadAccountData = function () {
            vaciarCacheMensajes();
            $.LoadAccountData();
        }     

    }
    });

