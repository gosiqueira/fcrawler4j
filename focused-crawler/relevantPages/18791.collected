<script type="text/javascript">
//<![CDATA[




 var MAX_TRAVELER_NUM=9; 
	
 
     
	function updateAgeFields(divId, count, abDesc) {
		
		var divElement = document.getElementById(divId);
		var ageFields = "";
		for (var i = 0; i < count; i++) {
				if (abDesc == 'child')
					ageFields += "<span><label>Insira a idade da criança:</label> <input type='text' size='3' maxlength='2' name='ageBands["+divId+"].ages["+i+"]'\/><\/span>";
				else if (abDesc == 'infant')
					ageFields += "<span><label>Insira a idade da criança de colo:</label> <input type='text' size='3' maxlength='2' name='ageBands["+divId+"].ages["+i+"]'\/><\/span>";
				else if (abDesc == 'senior')
					ageFields += "<span><label>Insira a idade do idoso:</label> <input type='text' size='3' maxlength='2' name='ageBands["+divId+"].ages["+i+"]'\/><\/span>";
				else if (abDesc == 'youth')
					ageFields += "<span><label>Insira a idade do jovem:</label> <input type='text' size='3' maxlength='2' name='ageBands["+divId+"].ages["+i+"]'\/><\/span>";
				else
					ageFields += "<span><label>Enter <strong>"+abDesc+"'s</strong> age:</label> <input type='text' size='3' maxlength='2' name='ageBands["+divId+"].ages["+i+"]'\/><\/span>";
		}
		
		divElement.innerHTML = ageFields;
		
		if (document.getElementById("agesInfoPopup")) {
			var showAgeInfoLink = false;
			for (var i = 0; i < 10; i++) {
				if (document.getElementById(i) && document.getElementById(i).getAttribute("ageDesc") != "adult" && document.getElementById(i).getElementsByTagName("span").length > 0) {
					showAgeInfoLink = true;
					break;
				}
			}
			if (showAgeInfoLink) { document.getElementById("agesInfoPopup").style.display = "block"; }
			else { document.getElementById("agesInfoPopup").style.display = "none"; }
		}
	}

	
	function updateAgeSelection(currentField) {
		
		var ageBandCount = 3;
		
	    var total = 0;
	    var remain = 0;
	    var field, tmp, length;
	    var max = MAX_TRAVELER_NUM;
	    for (i = 0; i < ageBandCount; i++) {
	        field = document.quoteform['ageBands[' + i + '].count'];
	        if (field != currentField) {
	            for (j = 0; j < ageBandCount; j++) {
	                tmp = document.quoteform['ageBands[' + j + '].count'];
	                total += parseInt(tmp.value);
	            }
	            remain = max - total;
	            if (remain < 0) {
	                remain = 0;
	            }
	            length = parseInt(field.value) + remain;
	            tmp = field.value;
	            field.length = 0;
	            for (j = 0; j <= length; j++) {
	                field[field.length] = new Option(j, j);
	            }
	            field.value = tmp;
	            total = 0;
	            remain = 0;
	        }
	    }
	}
	
	function paxOnChange(divId, count, abDesc, currentField){
		if(abDesc == 'ADULT') {
			updateAgeSelection(currentField);
		} else {
			updateAgeFields(divId, count, abDesc.toLowerCase());
			updateAgeSelection(currentField);
		}
	}
	
	 
	
	function showMap(ID,PRODUCTID,PRODUCTTYPE,CODE) {
		var mapUrl = "";
		mapUrl += "/popups/mapShow.jspa?";
