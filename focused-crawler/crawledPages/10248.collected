    function limitadorCaracteres() {
        var input_de_busca = document.getElementById('busca_autocomplete');
        var value = document.getElementById('busca_autocomplete').value;

        if (value.length < 2) {
            input_de_busca.setCustomValidity("É permitido no minimo 2 caracteres !");
            return false;
        } else {
            input_de_busca.setCustomValidity('');
            return true;
        }
    }
</script>
