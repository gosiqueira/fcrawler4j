    function mudaborda(param1,param2)
    {
        if (param1=="1")
        {
            document.getElementById(param2).style.borderColor="red";
        }
        else if (param1=="2")
        {
            if (document.getElementById(param2).value != "")
            {
                document.getElementById(param2).style.borderColor="#a8a8a8";
            }
        }
    }
    
    function fLimpaMasc(tipo)
    {
        //alert(tipo);
        if (tipo == "dia")
        {
            if (document.getElementById("ctl00_popups1_cad_dtNasc").value == "dd")
            {
                document.getElementById("ctl00_popups1_cad_dtNasc").style.color='#000';
                document.getElementById("ctl00_popups1_cad_dtNasc").value = "";
            }
            else if(document.getElementById("ctl00_popups1_cad_dtNasc").value == "")
            {
                document.getElementById("ctl00_popups1_cad_dtNasc").style.color='#bebdbd';
                document.getElementById("ctl00_popups1_cad_dtNasc").value = "dd";
            }
        }
        else if (tipo == "mes")
        {
            if (document.getElementById("ctl00_popups1_cad_MesNasc").value == "mm")
            {
                document.getElementById("ctl00_popups1_cad_MesNasc").style.color='#000';
                document.getElementById("ctl00_popups1_cad_MesNasc").value = "";
            }
            else if(document.getElementById("ctl00_popups1_cad_MesNasc").value == "")
            {
                document.getElementById("ctl00_popups1_cad_MesNasc").style.color='#bebdbd';
                document.getElementById("ctl00_popups1_cad_MesNasc").value = "mm";
            }
        }
        else if (tipo == "ano")
        {
            if (document.getElementById("ctl00_popups1_cad_AnoNasc").value == "aaaa")
            {
                document.getElementById("ctl00_popups1_cad_AnoNasc").style.color='#000';
                document.getElementById("ctl00_popups1_cad_AnoNasc").value = "";
            }
            else if(document.getElementById("ctl00_popups1_cad_AnoNasc").value == "")
            {
                document.getElementById("ctl00_popups1_cad_AnoNasc").style.color='#bebdbd';
                document.getElementById("ctl00_popups1_cad_AnoNasc").value = "aaaa";
            }
        }
    }
    
