if (overlayHeight > docHeight) {
var filtersDiv = document.getElement(".filtersOverlay .filters");
filtersDiv.setStyle("max-height", (filtersDiv.getSize().y - overlayHeight + docHeight - 20) + "px");
overlay.position();
}
