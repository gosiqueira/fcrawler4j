                $("#mensaje" + i).click(function () {
                    $("#cFlotanteMensajes").unbind();
                    $("#cFlotanteMensajes").hide();
                    tb_show("", "https://ssl.logitravel.fr/operativalogi/getMessage.aspx?sesion=dd659be7-d8a3-4601-9473-1944b6d8c5e5&mensaje=" + $(this).attr("rel") + "&amp;KeepThis=true&amp;TB_iframe=true&amp;height=600&amp;width=820");
                    hacerBindMensajes();
                    
                    _gaq.push(['_trackEvent', 'AccountHeader', "Cabecera", "OpenMessage"]);
                    
                    setTimeout(function () { $.ReloadAccountData() }, 5000)
                });

            }
        } else {
            var divTag = document.createElement("div");
            divTag.id = "mensaje";
            divTag.innerHTML = "<span>Vous n'avez aucun message</span>";
            messages.appendChild(divTag);
        }

        /* DIV: ver todos */
        var divTagTodos = document.getElementById("verTodos");
        divTagTodos.innerHTML = "<a href=\"" + aHref + "\"><span class=\"contenido10 grisflojo txt12negrobold\">Voir tous mes messages</span></a>";

        /* Update de Cantidad Mensajes por leer */
        if (noLeidos > 0) {
            $("#numMensajes").html(noLeidos).addClass("numMensajes br5 txt11blanco")
        } else {
            $("#numMensajes").html("")
        }
    }


