    /* Función que pinta los últimos Mensajes */
    function pintarMensajesCabecera(mensajes) {


        /* Update de Nombre de Usuario */        
        if (isLogged()){
            if(newAccount=="0"){
                username = "user"
                if (mensajes['Name'] != ""){
                    username = mensajes['Name']
                }
                $("#usuario").html(username);
                $("#enlaceMensajes").unbind();
                $("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa);  
            }else{
                $('#enlanceMensajes').attr('href', 'https://ssl.logitravel.it/operativalogi/ResponsiveWeb/Operational/clientsData.aspx?sesion=' + sesion + '&accion=paginar&operaciones=4');
            }

            $("#enlaceIniSesion").unbind();
            $("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa);
        }



        /* DIV: XX mensajes sin leer */
        var divTagQuedan = document.getElementById("sinLeer");
     
        var numMens= 0; 
        if (mensajes['Messages'] != undefined || mensajes['Messages'] != null){
            numMens =Object.keys(mensajes['Messages']).length;
        }
        
        if (numMens== 0){
            divTagQuedan.innerHTML = "Non hai messaggi";
            $("#listaMensajes").hide();
            $("#verTodos").hide();
            return;
        }
        $("#listaMensajes").show();
        $("#verTodos").show();

        var noLeidos = mensajes['UnreadMessages'];
        
        var aHref = "https://ssl.logitravel.it/operativalogi/ResponsiveWeb/operational/clientsData.aspx?sesion=32309a53-9d13-4ebc-a340-f3e318810bf8&accion=paginar&operaciones=4";
        

        /* Caja de los mensajes */
        var caja = document.getElementById("cFlotanteMensajes");

        
        if (noLeidos > 0) {
            divTagQuedan.innerHTML = noLeidos + " Messaggi <span class='txt12negro'>senza leggere</span>";
        } else {
            divTagQuedan.innerHTML = "Messaggio del 1 al " + numMens;
        }
        
        var count = Object.keys(mensajes['Messages']).length;
        /* DIV: los mensajes */
        $("#listaMensajes").html("")
        var messages = document.getElementById("listaMensajes");
        if (count > 0) {
            if (count > 5){
                messages.style.height = "250px";
            }
            /* Cada uno de ellos */

            for (var i = 0; i < count; i++) {
                var code = mensajes['Messages'][i]['Code'];
                var divTag = document.createElement("div");

                var from    = mensajes['Messages'][i]['From'];
                var subject = mensajes['Messages'][i]['Subject'];
                var sentDate = mensajes['Messages'][i]['SentDate'];
                var icoPendientes = mensajes['Messages'][i]['Status'] == 0?"<span class='icoMensajeSinLeer'></span>":"";

                $("#listaMensajes").append("<div id='mensaje" + i + "' rel='" + code + "'class='mensaje Bdashedgris clearfix'><div class='estadoMensaje'>" + icoPendientes + "</div><div class='cuerpoMensaje'><div class='remitente txt12gris'>" + from + "</div><div class='subject txt12azulbold'>" + subject + "</div><div class='txt12gris'>" + sentDate + "</div></div></div>");

                $("#mensaje" + i).click(function () {
                    $("#cFlotanteMensajes").unbind();
                    $("#cFlotanteMensajes").hide();
                    tb_show("", "https://ssl.logitravel.it/operativalogi/getMessage.aspx?sesion=32309a53-9d13-4ebc-a340-f3e318810bf8&mensaje=" + $(this).attr("rel") + "&amp;KeepThis=true&amp;TB_iframe=true&amp;height=600&amp;width=820");
                    hacerBindMensajes();
                    
                    _gaq.push(['_trackEvent', 'AccountHeader', "Cabecera", "OpenMessage"]);
                    
                    setTimeout(function () { $.ReloadAccountData() }, 5000)
                });

            }
        } else {
            var divTag = document.createElement("div");
            divTag.id = "mensaje";
            divTag.innerHTML = "<span>Non hai messaggi</span>";
            messages.appendChild(divTag);
        }

        /* DIV: ver todos */
        var divTagTodos = document.getElementById("verTodos");
        divTagTodos.innerHTML = "<a href=\"" + aHref + "\"><span class=\"contenido10 grisflojo txt12negrobold\">Vedi tutti i miei messaggi</span></a>";

        /* Update de Cantidad Mensajes por leer */
        if (noLeidos > 0) {
            $("#numMensajes").html(noLeidos).addClass("numMensajes br5 txt11blanco")
        } else {
            $("#numMensajes").html("")
        }
    }


