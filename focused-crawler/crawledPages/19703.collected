if (overlay) {
var locId = overlay.container.getAttribute('data-locationid');
if (locId) {
var listing = ta.id('hotel_' + locId);
if (listing) {
var details = listing.getElement('.property_details');
if (details) {
overlay.options.positionSource = details;
if (overlay.visible) {
