/**
* Make the placement extend into the RHR if there is space or contract back from the RHR if the
* sidebar height grows
* Note: This code exists in other placements.  Changes must be made elsewhere
*/
function setHscsRHROverflow() {
var sbElem = document.getElement('.col.sidebar');
var ctrColElem = document.getElement('#BODYCON .gridA .balance') || document.getElement('#BODYCON .gridR .balance');
var hscsElem = document.getElement('#HSCS');
if (!sbElem || !ctrColElem || !hscsElem) return;
var gridTop = ctrColElem.getTop();
var sidebarH = sbElem.offsetHeight;
var trigger = gridTop + sidebarH - 15;
if (hscsElem.getTop() > trigger) {
// There's room to expand into the RHR
hscsElem.addClass('similar_tab_rhr');
$$('div.similar_tab_rhr_yes').show();
$$('span.similar_tab_rhr_yes').show('inline');
$$('.similar_tab_rhr_no').hide();
if (!ta.retrieve('ta.p13n.prodp13n_hr_similar_tab_rhr_is_on')) {
ta.store('ta.p13n.prodp13n_hr_similar_tab_rhr_is_on', true);
ta.trackEventOnPage('prodp13n_hr_similar_xsell', 'expand_into_rhr');
}
} else {
// There is not room to expand into the rhr
hscsElem.removeClass('similar_tab_rhr');
$$('.similar_tab_rhr_yes').hide();
$$('div.similar_tab_rhr_no').show();
$$('span.similar_tab_rhr_no').show('inline');
if (ta.retrieve('ta.p13n.prodp13n_hr_similar_tab_rhr_is_on')) {
ta.store('ta.p13n.prodp13n_hr_similar_tab_rhr_is_on', false);
ta.trackEventOnPage('prodp13n_hr_similar_xsell', 'shrink_from_rhr');
}
}
}
