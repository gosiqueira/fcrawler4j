if(yScroll + 750 >= hscs.getPosition().y){
window.removeEvent('scroll', loadIfScrolledDown);
ta.trackEventOnPage('prodp13n_hr_similar_xsell', 'scrolled_near');
//do ajax poll, for tags and/or price button
load(1, true);
//load the meta view if dates are changed
ta.on('onCalendarRangeSelect', function(){load(1);});
}
}
