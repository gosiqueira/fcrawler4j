<script id="inline-detail-template" type="text/x-handlebars-template">
<div id="{{inlineDetailId}}" class="inline-detail" data-internal-itinerary-id="{{internalItineraryId}}" style="display: none;">
<div class="inline-detail-title-flight" style="display: none;">
<span class="inline-detail-title-text">Detalhes do vôo</span>
<span class="inline-detail-close">&times;</span>
</div>
<span class="inline-detail-title">
<span class="inline-detail-title-text">
{{getRouteDeparture this.segments this.entities}}
<span class="airport-code">({{getRouteDepartureCode this.segments}})</span>
<span class="inline-detail-title-to ux-common-icon-arrow-right"></span>
{{getRouteArrival this.segments this.entities}}
<span class="airport-code">({{getRouteArrivalCode this.segments}})</span>
</span>
<span class="inline-detail-total-duration">
<span class="inline-detail-total-duration-text">DURAÇÃO TOTAL:</span>
<span class="inline-detail-total-duration-time">{{totalDuration.formatted}}</span>
</span>
</span>
<div class="inline-detail-content">
{{#_each this.segments entities=this.entities}}
<div class="inline-detail-airline">
<div class="inline-detail-airline-wrapper">
<span class="inline-detail-airline-logo">
<img src="http://br.staticontent.com/img-versioned/1.23.52/common/airlines/25x25/{{data.carrier.code}}.png" alt="{{data.carrier.description}}" title="{{data.carrier.description}}" onerror="this.src='http://br.staticontent.com/img-versioned/1.23.52/common/airlines/25x25/default.png'" />
</span>
<span class="inline-detail-airline-description review-text">
{{#compare ../isVisibleCarrier false operator="!="}}
{{#get object=_data.extra.entities.airlines key=data.carrier.code}}{{/get}}
{{/compare}}
</span>
{{#showReview reviews=_data.extra.entities.reviews code=data.carrier.code}}
<span class="inline-detail-airline-separator review-text">/</span>
<div class="review-summary review-text">
{{#if _data.review.overallScore.score}}
<a href="#" class="review-description flight-review-description" data-review-info='{ "code" : "{{data.carrier.code}}" }'>
{{_data.review.overallScore.score}} {{_data.review.overallScore.description}}
</a>
{{/if}}
</div>
{{/showReview}}
</div>
</div>
<div class="inline-detail-data">
<span class="inline-detail-route-data">
<span class="inline-detail-airport">
<span class="inline-detail-airport-date">
{{#if data.departure.date.formatted}}
<span class="inline-detail-airport-date-text">Sai:</span>
{{data.departure.date.formatted}}
{{/if}}
</span>
<span class="inline-detail-airport-code-wrapper">
<span class="inline-detail-airport-code">{{data.departure.location.airport.code}}</span>
<span class="inline-detail-airport-hour">{{data.departure.hour.formatted}}</span>
{{#if data.nightFlight}}
<span class="night-flight ux-common-icon-night flights-title-tooltip"
data-title="Vôo noturno" data-title-id="night-flight-tooltip">
</span>
{{/if}}
</span>
<span class="inline-detail-airport-description">
{{#get object=_data.extra.entities.cities key=data.departure.location.city.code}}{{/get}} - {{#get object=_data.extra.entities.airports key=data.departure.location.airport.code}}{{/get}} ({{data.departure.location.airport.code}}) </span>
</span>
<span class="inline-detail-airport-separator ux-common-icon-arrow-right"></span>
<span class="inline-detail-airport">
<span class="inline-detail-airport-date">
{{#if data.arrival.date.formatted}}
<span class="inline-detail-airport-date-text">Chega:</span>
{{data.arrival.date.formatted}}
{{/if}}
</span>
<span class="inline-detail-airport-code-wrapper">
<span class="inline-detail-airport-code">{{data.arrival.location.airport.code}}</span>
<span class="inline-detail-airport-hour">{{data.arrival.hour.formatted}}</span>
{{#compare data.daysDifference 0 operator=">"}}
{{#compare data.daysDifference 1 operator="=="}}
<span class="days-difference">+{{data.daysDifference}} dia</span>
{{else}}
<span class="days-difference">+{{data.daysDifference}} dias</span>
{{/compare}}
{{/compare}}
</span>
<span class="inline-detail-airport-description">
{{#get object=_data.extra.entities.cities key=data.arrival.location.city.code}}{{/get}} - {{#get object=_data.extra.entities.airports key=data.arrival.location.airport.code}}{{/get}} ({{data.arrival.location.airport.code}}) </span>
</span>
</span>
<span class="inline-detail-flight">
<span class="inline-detail-flight-data ">
{{#if data.duration.formatted}}
<span class="inline-detail-flight-data-title">DURAÇÃO</span>
<span class="inline-detail-flight-data-description">{{data.duration.formatted}}</span>
{{/if}}
</span>
<span class="inline-detail-flight-data ">
{{#if data.flightNumber}}
<span class="inline-detail-flight-data-title">VÔO N°</span>
<span class="inline-detail-flight-data-description">{{data.flightNumber}}</span>
{{/if}}
</span>
<span class="inline-detail-flight-data inline-detail-flight-cabinType">
<span class="inline-detail-flight-data-title">CLASSE</span>
<span class="inline-detail-flight-data-description">{{#get object=_data.extra.entities.cabinTypes key=data.marketingCabinType}}{{/get}}</span>
</span>
</span>
</div>
{{#if data.aircraftModel}}
<div class="inline-detail-flight-airline">
<span class="airplane-name">
<span class="ux-common-icon-airplane-going"></span>
{{data.aircraftModel.manufacturer}} {{data.aircraftModel.model}}
</span>
{{#compare data.carrier.code data.operatingCarrier.code operator="!=" }}
<span class="inline-detail-flight-airline-separator">/</span>
<span class="inline-detail-operated">
Operado por {{#get object=_data.extra.entities.airlines key=data.operatingCarrier.code}}{{/get}}
</span>
{{/compare}}
</div>
{{/if}}
{{#hasAncillaries data showAncillaries=false}}
<div class="inline-detail-ancillaries">
<div class="inline-detail-ancillaries-content">
<span class="inline-detail-ancillaries-text">Inclui:</span>
</div>
</div>
{{/hasAncillaries}}
{{#showConnection segments=../segments}}
<span class="time detail-time">
<span class="detail-time-wrapper">
<span class="connection">
<span class="text">
{{#if _data.showWaitDuration}}
{{#if _data.showAirportChange}}
<span class="ux-common-icon-info"></span>
{{else}}
<span class="ux-common-icon-last-buy"></span>
{{/if}}
Espera de <span class='connection-text-duration'>{{_data.waitDuration}}</span>&nbsp;em {{_data.locationDescription}} ({{_data.locationCode}})
{{else}}
Espera de {{_data.cityDescription}} ({{_data.cityCode}})
{{/if}}
</span>
<span class="connection-type">
{{#if _data.showAirportChange}}
<span class="icons airport-change">
<span class="main-sprite icon-detail-marker icon"></span>
<span class="main-sprite icon-detail-next-arrow icon"></span>
<span class="main-sprite icon-detail-marker icon"></span>
</span>
(Troca de aeroporto)
{{/if}}
{{#if _data.showAirplaneChange}}
<span class="icons">
<span class="main-sprite icon-detail-plane icon"></span>
<span class="main-sprite icon-detail-next-arrow icon"></span>
<span class="main-sprite icon-detail-plane icon"></span>
</span>
(Troca de avião)
{{/if}}
{{#if _data.showSameAirplane}}
<span class="icons">
<span class="main-sprite icon-detail-plane icon"></span>
</span>
(Mesmo avião)
{{/if}}
</span>
</span>
{{#segmentHasAirportChange ../../segments index}}
<span class="bus-policy">Traslado entre aeroportos não incluso neste bilhete</span>
{{/segmentHasAirportChange}}
{{/showConnection}}
</span>
</span> {{/_each}}
<div id="itinerary-select-button">
<span class="itinerary-select-button flights-button-no">Selecione vôo</span>
</div>
</div>
<div class="detail-footer">
<div class="top-box">
<span class="detail-local-hour">Horários em hora local de cada cidade</span>
</div>
</div>
</div>
</script>
