<script>
	if ( $body.hasClass( 'is-portada' ) ) {
		var scrollTimeStart = new Date;
		var scrollEvent100=0;
		var scrollEvent90 = 0;
		var scrollEvent75 = 0;
		var scrollEvent50 = 0;
		var scrollEvent25 = 0;

		function IniciarGoogleAnalyticsTrackEvents(){
			TrackingScroll();
		}

		function TrackingScroll() {
			$(window).scroll(function(){
				var scrollPercent = GetScrollPercent();
				//fin home
				if(scrollPercent==100 && scrollEvent100!=1){
					scrollEvent100=1;
					var scrollTime = (new Date - scrollTimeStart )/1000;
					var roundTimeScroll = Math.round(scrollTime,1);
					TrackEvent("Portada-SoyChile", "Scroll en 100%", document.location.href, roundTimeScroll);
				}
				//fin ultimo minuto
				if(scrollPercent>90 && scrollEvent90!=1){
					scrollEvent90=1;
					var scrollTime = (new Date - scrollTimeStart )/1000;
					var roundTimeScroll = Math.round(scrollTime,1);
					TrackEvent("Portada-SoyChile", "Scroll en 90%", document.location.href, roundTimeScroll);
				}
				// noticias de último minuto
				if(scrollPercent>75 && scrollEvent75!=1){
					scrollEvent75=1;
					var scrollTime = (new Date - scrollTimeStart )/1000;
					var roundTimeScroll = Math.round(scrollTime,1);
					TrackEvent("Portada-SoyChile", "Scroll en 75%", document.location.href, roundTimeScroll);
				}
				//deporte y economia
				if(scrollPercent>50 && scrollEvent50!=1){
					scrollEvent50=1;
					var scrollTime = (new Date - scrollTimeStart )/1000;
					var roundTimeScroll = Math.round(scrollTime,1);
					TrackEvent("Portada-SoyChile", "Scroll en 50%", document.location.href, roundTimeScroll);
				}
				//noticias destacadas
				if(scrollPercent>25 && scrollEvent25!=1){
					scrollEvent25=1;
					var scrollTime = (new Date - scrollTimeStart )/1000;
					var roundTimeScroll = Math.round(scrollTime,1);
					TrackEvent("Portada-SoyChile", "Scroll en 25%", document.location.href, roundTimeScroll);
				}
			});
		}
		function GetScrollPercent() {
			var bottom = $(window).height() + $(window).scrollTop();
			var height = $(document).height();
			return Math.round(100*bottom/height);
		}
		function TrackEvent(Category, Action, Label, Value){
			_gaq.push(['_trackEvent', Category, Action, Label, Value, true]);
		}
		//]]>
	    $(document).ready(function () {
	        IniciarGoogleAnalyticsTrackEvents();
	    });
	}
</script>
