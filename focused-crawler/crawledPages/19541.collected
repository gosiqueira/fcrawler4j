<script type="text/javascript">
ta.commerce.urgencygauge = (function(ta) {
var _container
, _getClosestPercent = function(dataPercent) {
var allowedPercents = [5, 10, 20, 30, 40, 50, 60]; // lowest to highest to keep the for loop simple
var percent;
if (dataPercent) {
dataPercent = parseInt(dataPercent);
}
if (!dataPercent || dataPercent <= 0 || dataPercent > allowedPercents[allowedPercents.length - 1]) { // only allow 1% - 60%
return;
}
for (i = 0; i < allowedPercents.length; i++) { // assumes percents are ordered lowest to highest
if (!percent || Math.abs(allowedPercents[i] - dataPercent) <= Math.abs(percent - dataPercent)) {
percent = allowedPercents[i];
}
}
return percent;
};
return {
init: function() {
if (_container) {
if(!ta.retrieve('sd_hh.urgency_gauge')) { // SD/HH use MetaJson and do not refresh the Velocity. Allow for container re-use.
return; // already initialized
}
}
_container = document.getElement('.urgency_gauge');
if (!_container) {
return; // can't find notifications container
}
var viewing = _container.getElement('.viewing');
var booked = _container.getElement('.booked');
var availability = _container.getElement('.availability');
var pricesUp = _container.getElement('.prices_up');
var pricesDown = _container.getElement('.prices_down');
var popMsgs = [];
if (viewing) {
popMsgs.push(1);
}
if (booked) {
popMsgs.push(2);
}
if (availability) {
popMsgs.push(3);
}
var whichPopMsg;
if (window.pageServlet === 'Hotels') {
whichPopMsg = 3;
}
else if (popMsgs.length > 0) {
whichPopMsg = popMsgs[Number.random(0, popMsgs.length - 1)]; // Select a popularity message at random
}
var notifications = [];
if (viewing && whichPopMsg === 1) {
var viewingVal = viewing.getAttribute('data-viewing');
notifications.push({'el': viewing, 'trackingLabel': 'people_viewing', 'trackingValue': viewingVal});
}
if (booked && whichPopMsg === 2) {
var bookedVal = booked.getAttribute('data-booked');
notifications.push({'el': booked, 'trackingLabel': 'people_booked', 'trackingValue': bookedVal});
}
if (availability && whichPopMsg === 3) {
var availabilityVal = availability.getAttribute('data-percent');
if (!availabilityVal) {
availabilityVal = Cookie.read('urgencyAvailability_' + window.modelLocId);
}
if (!availabilityVal) {
availabilityVal = Number.random(1, 60); // Generate a random integer between 1 and 60
Cookie.write('urgencyAvailability_' + window.modelLocId, availabilityVal, {domain:cookieDomain, path:'/'});
}
var closestPercent = _getClosestPercent(availabilityVal);
if (closestPercent) {
availability.set('html', availability.get('html').toString().replace('(0)', availabilityVal));
availability.addClass('percent' + _getClosestPercent(availabilityVal));
notifications.push({'el': availability, 'trackingLabel': 'percent', 'trackingValue': 100 - availabilityVal});
}
}
if (pricesUp && pricesDown) {
var pricesChangedVal = Cookie.read('urgencyPricesChanged_' + window.modelLocId);
if (!pricesChangedVal) {
pricesChangedVal = Number.random(0, 1) === 1 ? 1 : -1; // Generate a random value, either +1 or -1
Cookie.write('urgencyPricesChanged_' + window.modelLocId, pricesChangedVal, {domain:cookieDomain, path:'/'});
}
var pricesChanged;
if (pricesChangedVal > 0) {
pricesChanged = pricesUp;
}
else if (pricesChangedVal < 0) {
pricesChanged = pricesDown;
}
pricesChanged.set('html', pricesChanged.get('html').toString().replace('(0)', pricesChangedVal));
notifications.push({'el': pricesChanged, 'trackingLabel': 'prices_' + (pricesChangedVal > 0 ? 'up' : 'down')});
