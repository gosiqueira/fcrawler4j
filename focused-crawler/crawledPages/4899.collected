            e.preventDefault();
            //Guarda una copia de referencia
            var $this = $(this);

            if (overlayOpened != null)
                overlayOpened.close();

            //Crea un tooltip
            var overlay = new Despegar.UX.Common.Modules.Overlay();

            overlayOpened = overlay;


            overlay.open({
