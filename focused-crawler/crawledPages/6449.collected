          <span class="item-img-icon">
            {{#if thumbnailUrl}}
<figure class="image aspect-ratio1-1" data-media-type="image" data-src="//images.trvl-media.com{{thumbnailUrl}}" data-alt="{{hotelName}}" data-aspect-ratio="1-1" data-class="tile-media"><img src="//images.trvl-media.com{{thumbnailUrl}}" alt="{{hotelName}}" class="tile-media"/></figure>
            {{else}}
              <figure class="aspect-ratio16-9 image no-image" data-media-type="image" data-lazy="true" data-late="false" data-downgrade="false" data-aspect-ratio="16-9" data-class="tile-media" style="background-size:110%"></figure>
            {{/if}}
          </span>
          <h4 class="item-title">{{hotelName}}</h4>
          <p class="item-date">{{checkInDate}} - {{checkOutDate}}</p>
          <p class="item-watches item-nested">{{viewership viewCount}}</p>
          <div class="item-price-urgency"><strong class="price">{{price}}</strong><p class="item-nested">{{lastSeenDate}}</p></div>
        </div>
      </a>
    </li>
</script>


