function checkfrmNewsletter()
{
	if (document.frmNewsletter.txf_t_mailing.value == "")
	{
		document.frmNewsletter.txf_t_mailing.focus();
		alert("Informe seu e-mail para cadastro!");
		return false;
	}

	if ((document.frmNewsletter.txf_t_mailing.value.indexOf("'") > 0) || (document.frmNewsletter.txf_t_mailing.value.indexOf(",") > 0) || (document.frmNewsletter.txf_t_mailing.value.indexOf("/") > 0) || (document.frmNewsletter.txf_t_mailing.value.indexOf("&ccedil;") > 0)) 
	{
		document.frmNewsletter.txf_t_mailing.focus();
		alert("O e-mail digitado possui caracteres incorretos");
		return false;
	}

	if (document.frmNewsletter.txf_t_mailing.value.indexOf("@") <= 0 || document.frmNewsletter.txf_t_mailing.value.indexOf(".") <= 0)
	{
		document.frmNewsletter.txf_t_mailing.focus();
		alert("Desculme, mas o e-mail digitado parece estar incorreto.")
		return false;
	}
}	
