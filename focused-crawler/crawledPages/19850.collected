* INITIALIZERS
***********************************************/
ta.store('hotels_checkbox_filters_price_slider', true);
ta.queueForLoad(function() {
getOrCreatePriceSlider();
registerHandlers();
} , 'initialize ' + widget.name);
return {
togglePriceSlider : togglePriceSlider
};
});
})();
