if (!slider || !slider.priceSlider) {
return;
}
slider.priceSlider.resetValues();
togglePriceSliderTag(false);
updateSponsoredCoupon();
ta.call('ta.p13n.wizard.selectTags', event, elmt, [{'name': 'j', 'val': -1},{'name': 'k', 'val': -1}], true, 'filter');
}
/**
* Set minimum filter heights
*/
function setMinFilterHeight() {
var widgetDiv = widget.element;
var sortBar = widgetDiv && widgetDiv.getElement('.JFY_sort_wrap_in_bar');
var sortBarSize = sortBar && sortBar.getSize();
var sortBarHeight = sortBarSize ? sortBarSize.y : 49;
var filtersWrap = widgetDiv && widgetDiv.getElement('.checkbox_wrap');
var filtersSize = filtersWrap && filtersWrap.getSize();
var filtersHeight = filtersSize && filtersSize.y;
var bodyWrap = ta.id('BODYCON');
if (filtersHeight && bodyWrap) {
bodyWrap.setStyle('min-height', filtersHeight + 600 - sortBarHeight + 24);
}
}
/**
* Reset saved sponsored coupons
*/
function updateSponsoredCoupon() {
if (ta.has('h_sponsored_coupon_respect_price_slider')) {
ta.remove('sponsoredCouponData');
}
}
/**
* Register all the handlers for this particular widgets
*/
function registerHandlers() {
ta.on('update-'+widget.name, onHacUpdate);
ta.on('onLeftFiltersUpdate', onLeftFiltersUpdate);
ta.on('hotels.resetFilters', resetPriceSliderRange);
}
/**
* Unregister all the handlers for this particular widget
*/
function unregisterHandlers() {
ta.off('update-'+widget.name, onHacUpdate);
ta.off('onLeftFiltersUpdate', onLeftFiltersUpdate);
ta.off('hotels.resetFilters', resetPriceSliderRange);
}
/***********************************************
* PUBLIC FUNCTIONS
***********************************************/
/**
* Collapse or expand the price slider dependending on the
* current state of the price slider.
*/
function togglePriceSlider(event) {
// Grab widget div to toggle
var widgetDiv = widget.element;
var wrap = widgetDiv && widgetDiv.getElement('.JFY_hotel_filter_wrap');
