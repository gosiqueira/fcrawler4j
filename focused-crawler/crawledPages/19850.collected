}
/**
* Handles the event when price slider is changed. This is used
* as a callback by PriceSlider object and called when the price
* on the slider has changed.
*/
function priceSliderChanged(event, elmt, textChanged) {
ta.trackEventOnPage('priceSlider', textChanged ? 'textChanged' : 'sliderMoved');
event && event.stop();
var slider = ta.id('priceSlider');
if (!elmt || !slider || !slider.priceSlider) {
return;
}
var min = slider.getAttribute('data-min');
var max = slider.getAttribute('data-max');
var startInput = ta.id('jfy_filter_j');
var endInput = ta.id('jfy_filter_k');
if (!min || !max || !startInput || !endInput) {
return;
}
// get integer value from data attribute since actual value contains currency
var start = startInput.getAttribute('data-value');
var end = endInput.getAttribute('data-value');
if (!start || !end) {
return;
}
var label = slider.priceSlider.getFormattedInputValueRange(startInput, endInput);
var isPriceSelected = start !== min || end !== max;
togglePriceSliderTag(isPriceSelected, label);
start = start === min ? -1 : start; // don't send actual value if minimum is selected
end = end === max ? -1 : end; // don't send actual value if maximum is selected
updateSponsoredCoupon();
ta.call('ta.p13n.wizard.selectTags', event, this, [{'name': 'j', 'val': start},{'name': 'k', 'val': end}], true, 'filter');
}
/**
* Toggle the price slider tag in the cloud on or off depending
* on the variable 'show', using the label variable as the tag.
*/
function togglePriceSliderTag(show, label) {
var tag = document.getElement('.jfy_filter_j_k');
if (tag) {
if (show) {
tag.addClass('selected');
tag.removeClass('hidden');
retainSlider = false;
}
else {
tag.removeClass('selected');
tag.addClass('hidden');
retainSlider = true;
}
if (label) {
var tagLabel = tag.getElement('.label');
if (tagLabel) {
tagLabel.innerHTML = label;
}
}
}
ta.HotelsCheckboxFilters.updateTagCloudClass();
setMinFilterHeight();
}
/**
* Called when removing the price slider tag from the tag cloud
*/
function resetPriceSliderRange(event, elmt) {
