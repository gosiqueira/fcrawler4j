/**
* Create a ta.commerce.PriceSlider object and set it on element
* with id priceSlider. This element will be associated with the
* object going forward and will be retrieved after created.
*/
function getOrCreatePriceSlider() {
if (ta.has('hotels_checkbox_filters_price_slider') && ta.commerce.PriceSlider) {
var elmt = ta.id('priceSlider');
if (elmt) {
if(!elmt.priceSlider) {
elmt.priceSlider = new ta.commerce.PriceSlider('priceSlider', priceSliderChanged);
}
return elmt.priceSlider;
}
}
return null;
}
/**
* Check whether the new slider is different from the old slider
* and therefore should be updated. This is to prevent excessive
* updating on every poll even when there is no change to the
* price slider.
*/
function shouldUpdate(oldSliderDiv, newSliderDiv) {
if (!oldSliderDiv || !newSliderDiv) {
return false;
}
if (retainSlider) {
return false;
}
var oldData = oldSliderDiv.getElement(".sliderContainer");
var newData = newSliderDiv.getElement(".sliderContainer");
if (!oldData || !newData) {
return false;
}
var oldValues = getPriceValues(oldData);
var newValues = getPriceValues(newData);
return oldValues.min !== newValues.min ||  oldValues.max !== newValues.max;
}
/**
* Extract price values from the data element and stuff this
* data into an object with values for min, max, start, and
* end
*/
function getPriceValues(data) {
return {
min: data.getAttribute("data-min"),
max: data.getAttribute("data-max")
};
}
/**
* Replace the old price slider with the new one, but only if there
* has been changes to the price slider. In addition, update the
* tag cloud with prices from price slider and toggle them on or
* off depending on values.
*/
function onHacUpdate(newSliderDiv) {
// Grab the new and old divs to replace and detect changes
var oldSliderDiv = widget.element;
if (!shouldUpdate(oldSliderDiv, newSliderDiv)) {
return;
}
// Replace the old price slider with the new one and recreate slider
newSliderDiv.replaces(oldSliderDiv);
widget.element = newSliderDiv;
var priceSlider = getOrCreatePriceSlider();
// Toggle the price slider tag cloud on or off depending on values
var startInput = ta.id('jfy_filter_j');
var endInput = ta.id('jfy_filter_k');
if (startInput && endInput) {
var label = priceSlider.getFormattedInputValueRange(startInput, endInput);
var isPriceSelected = priceSlider.isPriceSelected();
togglePriceSliderTag(isPriceSelected, label);
}
// Initialize all the widgets under this one
if (ta.prwidgets) {
unregisterHandlers();
var parentElmt = newSliderDiv.getParent();
ta.prwidgets.initWidgets(parentElmt);
}
}
/**
* Handle event when the left filter has been updated. We want
* correctly replace data in widget variable to reflect the new
* elements on the page after the left filters is replaced.
*/
function onLeftFiltersUpdate(newLeftFilters) {
if (!newLeftFilters) {
return;
}
var widgetElmt = newLeftFilters.getElement("[data-prwidget-name='" + widget.name + "']");
if (widgetElmt) {
widget.element = widgetElmt;
}
