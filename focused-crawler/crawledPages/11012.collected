<script type="text/javascript">
    var cookie = "tokenlogitraveluk" ;
    
    function forzarRefrescoSearchesData() {
        vaciarCacheBusquedas();
        loadSearchesData();
    }
    //function hacerBindBusquedas(){$("#enlaceBusquedas").unbind();$("#enlaceBusquedas").bind("click",{cFlotante: 'cFlotanteBusquedas'}, abrirCapa)}

    function hacerBindSesion(){$("#enlaceIniSesion").unbind();$("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa)}
    function hacerBindMensajes(){$("#enlaceMensajes").unbind();$("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa)}

    function abrirCapa(event) {
            
        switch(event.target.id)
        {
            case "enlaceIniSesion":
                $("#cFlotanteBusquedas").hide();
                $("#cFlotanteMensajes").hide();           
                break;
            case "usuario":
                $("#cFlotanteBusquedas").hide();
                $("#cFlotanteMensajes").hide();           
                break;

            case "enlaceMensajes":
                $("#cFlotantePerfil").hide();
                $("#cFlotanteBusquedas").hide();                    
                break;
            case "enlaceBusquedas":
                $("#cFlotantePerfil").hide();
                $("#cFlotanteMensajes").hide();
                break;
        }

        var boton = event.target.id;
        if(boton=="usuario"){boton="enlaceIniSesion";}

        $("#"+event.data.cFlotante).toggle();
        if ($('#'+event.data.cFlotante).is(':visible')){$("#"+boton).unbind("click");}
        $('#'+event.data.cFlotante).focus();            
                
    }
    function loadSearchesData() {
        var busquedas;
        var topBusquedas = 5;
        var fecha = new Date();
        var timestamp = fecha.getTime();
            
        if ((!localStorage.userBusquedas) || (localStorage.userBusquedas == '') ||
                (!localStorage.userBusquedasLastUpdate) || (new Date().setTime(fecha.getTime() - 300000) > localStorage.userBusquedasLastUpdate)) {

            $.ajax({
                url: "https://api.logitravel.com/account/api/Searches/getSearches",
                    data: {
                        idioma: "en",
                        USW_codigo: "32557956407",
                        top: topBusquedas
                    },
                    cache: false,
                    dataType: "jsonp",
                    contentType: "charset=iso-8859-1",
                    beforeSend: function (){
                        $("#listaBusquedas").empty();
                        $("#cargaBusquedas").show();
                    },
                    success: function (response) {                    
                        busquedas = JSON.stringify(response);
                        vaciarCacheBusquedas();
                        localStorage.userBusquedas = busquedas;
                        localStorage.userBusquedasLastUpdate = timestamp;
                        pintarUltimasBusquedasCabecera(JSON.parse(busquedas));                    
                    },
                    complete: function () {
                        $("#cargaBusquedas").hide();
                    }              
                });
            
            _gaq.push(['_trackEvent', 'AccountHeader', "BusquedasClick", "LoadSearchesData",0,true]);
            
        }else{                
            pintarUltimasBusquedasCabecera(JSON.parse(localStorage.userBusquedas));
        }
    }
    function showUserMenu() {
    
        if(document.getElementById("cFlotanteUser").style.display == 'block'){
            document.getElementById("cFlotanteUser").style.display ='none';
        }else{
            document.getElementById("cFlotanteUser").style.display = 'block';
            getUserMessages();
            
        } 
    }
    function getUserMessages(){
        $.ajax({
            url: "https://api.logitravel.com/account/api/Message/GetUnreadMessagesCount?token=&sessionCode=13a5c794-f576-4130-a713-8a9372a9b463",
            dataType: "jsonp",
            contentType: "charset=iso-8859-1",
            success: function (response) {
                $('#numMsg').html(response);
            }
        });
    }
    function getUserData(){
        $.ajax({
            url: "https://api.logitravel.com/account/api/Customer/",
            dataType: "jsonp",
            contentType: "charset=iso-8859-1",
            success: function (response) {
                $('#usuario').html(response.Name);
                $('#UserName').html(response.Name);
            }
        });
    }
    $(document).ready(function () {
        if(newAccount=="1"){
            if (isLogged()){
                $('#userCabecera').bind('click', function(e){ 
                    e.stopPropagation();
                    getUserMessages();
                    $('#cFlotanteUser').show()
                    $('html').click(function (evt) {
                        if (evt.target.id != 'cFlotanteUser') {
                            $('#cFlotanteUser').hide()
                            $('html').unbind()
                        }
                    })
                });
            }
        }else{
            loadSearchesData();

            $("#refrescarBusquedas").unbind();
            $("#refrescarBusquedas").bind("click",function(){
                forzarRefrescoSearchesData();
            });

            $("#enlaceBusquedas").unbind();
            $("#enlaceBusquedas").bind("click",{cFlotante: 'cFlotanteBusquedas'}, abrirCapa);
        
            if (isLogged()) {
                $("#enlaceIniSesion").unbind();
                $("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa);
                $("#enlaceMensajes").unbind();
                $("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa);       
            }

        
            $("#cFlotanteBusquedas").bind("blur", function(event) {
                var parent="body";
            
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }

                if(parent.indexOf("cFlotanteBusquedas")==-1){
                    $("#cFlotanteBusquedas").hide();                        
                    setTimeout(hacerBindBusquedas, 500); 
                }else{
                    $("#cFlotanteBusquedas").focus();                                 
                }
            });


            $("#cFlotantePerfil").bind("blur", function(event) {
                var parent="body";
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }

                if(parent.indexOf("cFlotantePerfil")==-1){
                    $("#cFlotantePerfil").hide();                        
                    setTimeout(hacerBindSesion, 500); 
                }else{
                    $("#cFlotantePerfil").focus();
                }
            });
            $("#cFlotanteMensajes").bind("blur", function(event) {
                var parent="body";
                if(typeof event.originalEvent.explicitOriginalTarget!="undefined")
                {
                    if(typeof event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id!="undefined")
                    {
                        parent = event.originalEvent.explicitOriginalTarget.parentElement.offsetParent.id;
                    }
                }else{
                    if(document.activeElement.offsetParent!=null){
                        parent=document.activeElement.offsetParent.id;
                    }
                }
                if(parent.indexOf("cFlotanteMensajes")==-1){
                    $("#cFlotanteMensajes").hide();                        
                    setTimeout(hacerBindMensajes, 500); 
                }else{
                    $("#cFlotanteMensajes").focus();
                }
            });

        


            $.LoadAccountData = function () {
                if(newAccount=="1"){
                    $.ajax({               
                        url:  "https://api.logitravel.com/account/api/Customer//Messages?sessionCode=13a5c794-f576-4130-a713-8a9372a9b463",
                        dataType: "jsonp",
                        contentType: "charset=iso-8859-1",
                        success: function (response) {
                            var ResNew = { 'Messages': response };
                            mensajes = JSON.stringify(ResNew);
                            vaciarCacheMensajes();
                            localStorage.userMessages = mensajes;
                            localStorage.userMessagesLastUpdate = timestamp;
                            pintarMensajesCabecera(JSON.parse(mensajes));
                        }
                    });
                } else {
                    var mensajes;
                    var fecha = new Date();
                    var timestamp = fecha.getTime();
                    if ((!localStorage.userMessages) || (localStorage.userMessages == '') ||
                        (!localStorage.userMessagesLastUpdate) || (new Date().setTime(fecha.getTime() - 60000) > localStorage.userMessagesLastUpdate)) {
                    $.ajax({
                        url: "https://api.logitravel.com/account/api/user/messages",
                        data: {
                            sessionCode: "13a5c794-f576-4130-a713-8a9372a9b463",
                            token: "",
                            top: 10,
                            usw_codigo: 32557956407
                                },
                        dataType: "jsonp",
                        contentType: "charset=iso-8859-1",
                        success: function (response) {
                            mensajes = JSON.stringify(response);
                            vaciarCacheMensajes();
                            localStorage.userMessages = mensajes;
                            localStorage.userMessagesLastUpdate = timestamp;
                            pintarMensajesCabecera(JSON.parse(mensajes));
                        }
                    });
                        
                    _gaq.push(['_trackEvent', 'AccountHeader', "Cabecera", "LoadAccountData",0,true]);
                        
                } else {
                    pintarMensajesCabecera(JSON.parse(localStorage.userMessages));
                }
            }
        }

        $.LoadAccountData();


        setInterval(function () { $.LoadAccountData() }, 30000);

        setInterval(function () { 
            $("#listaBusquedas").empty();loadSearchesData() }, 300000);
        

        $.ReloadAccountData = function () {
            vaciarCacheMensajes();
            $.LoadAccountData();
        }     

    }
    });

$(window).load(function () {
             
    if (!isLogged()) {
        /* Enlaces Iniciar Sesión */
            
                $("#userCabecera").click(function () {
                    tb_show("", "/operativalogi/ResponsiveWeb/Account/Login.aspx?KeepThis=true&amp;TB_iframe=true&amp;height=540&amp;width=490");
                });
                $("#enlaceIniSesion").click(function () {
                    tb_show("", "/operativalogi/ResponsiveWeb/Account/Login.aspx?KeepThis=true&amp;TB_iframe=true&amp;height=540&amp;width=490");
                });
                $(".enlaceIniSesion").click(function () {
                    tb_show("", "/operativalogi/ResponsiveWeb/Account/Login.aspx?KeepThis=true&amp;TB_iframe=true&amp;height=540&amp;width=580");
                });
                $("#enlacePerfil").click(function () {
                    tb_show("", "/operativalogi/ResponsiveWeb/Account/Login.aspx?KeepThis=true&amp;TB_iframe=true&amp;height=540&amp;width=490");
                });
                $("#enlaceMisReservas").click(function () {
                    tb_show("", "/operativalogi/ResponsiveWeb/Account/Login.aspx?KeepThis=true&amp;TB_iframe=true&amp;height=540&amp;width=490");
                });
            
            }

        if (typeof String.prototype.startsWith != 'function') {
            String.prototype.startsWith = function (str){
                return this.slice(0, str.length) == str;
            };
        }

    });    

  
    function isLogged(){
        if ("" != '') {
            return true
        }else{
            return false
        }
    }

    /* Función que pinta los últimos Mensajes */
    function pintarMensajesCabecera(mensajes) {


        /* Update de Nombre de Usuario */        
        if (isLogged()){
            if(newAccount=="0"){
                username = "user"
                if (mensajes['Name'] != ""){
                    username = mensajes['Name']
                }
                $("#usuario").html(username);
                $("#enlaceMensajes").unbind();
                $("#enlaceMensajes").bind("click",{cFlotante: 'cFlotanteMensajes'}, abrirCapa);  
            }else{
                $('#enlanceMensajes').attr('href', 'https://ssl.logitravel.co.uk/operativalogi/ResponsiveWeb/Operational/clientsData.aspx?sesion=' + sesion + '&accion=paginar&operaciones=4');
            }

            $("#enlaceIniSesion").unbind();
            $("#enlaceIniSesion").bind("click",{cFlotante: 'cFlotantePerfil'}, abrirCapa);
        }



        /* DIV: XX mensajes sin leer */
        var divTagQuedan = document.getElementById("sinLeer");
     
        var numMens= 0; 
        if (mensajes['Messages'] != undefined || mensajes['Messages'] != null){
            numMens =Object.keys(mensajes['Messages']).length;
        }
        
        if (numMens== 0){
            divTagQuedan.innerHTML = "You have no messages";
            $("#listaMensajes").hide();
            $("#verTodos").hide();
            return;
        }
        $("#listaMensajes").show();
        $("#verTodos").show();

        var noLeidos = mensajes['UnreadMessages'];
        
        var aHref = "https://ssl.logitravel.co.uk/operativalogi/ResponsiveWeb/operational/clientsData.aspx?sesion=13a5c794-f576-4130-a713-8a9372a9b463&accion=paginar&operaciones=4";
        

        /* Caja de los mensajes */
        var caja = document.getElementById("cFlotanteMensajes");

        
        if (noLeidos > 0) {
            divTagQuedan.innerHTML = noLeidos + " Messages <span class='txt12negro'>not read</span>";
        } else {
            divTagQuedan.innerHTML = "Posts 1 to " + numMens;
        }
        
        var count = Object.keys(mensajes['Messages']).length;
        /* DIV: los mensajes */
        $("#listaMensajes").html("")
        var messages = document.getElementById("listaMensajes");
        if (count > 0) {
            if (count > 5){
                messages.style.height = "250px";
            }
            /* Cada uno de ellos */

            for (var i = 0; i < count; i++) {
                var code = mensajes['Messages'][i]['Code'];
                var divTag = document.createElement("div");

                var from    = mensajes['Messages'][i]['From'];
                var subject = mensajes['Messages'][i]['Subject'];
                var sentDate = mensajes['Messages'][i]['SentDate'];
                var icoPendientes = mensajes['Messages'][i]['Status'] == 0?"<span class='icoMensajeSinLeer'></span>":"";

                $("#listaMensajes").append("<div id='mensaje" + i + "' rel='" + code + "'class='mensaje Bdashedgris clearfix'><div class='estadoMensaje'>" + icoPendientes + "</div><div class='cuerpoMensaje'><div class='remitente txt12gris'>" + from + "</div><div class='subject txt12azulbold'>" + subject + "</div><div class='txt12gris'>" + sentDate + "</div></div></div>");

                $("#mensaje" + i).click(function () {
                    $("#cFlotanteMensajes").unbind();
                    $("#cFlotanteMensajes").hide();
