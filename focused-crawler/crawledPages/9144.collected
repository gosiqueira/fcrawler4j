		$.each(tab.find("dt") , function(index,value){
			var abr = $(this).children('p').first();
			abr.append(abreviatedAnswers[index]);
			if(0 == index) {
				$(this).addClass("faq_open");
				$(this).next().show();
				abr.hide();
			}
		});
		
		$('#faq_link_to_help').click(function(){
			GAQPushTransition("_trackEvent", "FE-1843", "follow help link", "Click", 1, true);
		});
		
		
		$('#faq_link_to_contact').click(function(){
			GAQPushTransition("_trackEvent", "FE-1843", "follow contact us link", "Click", 1, true);		
		});
		
		tab.find("dt").click(function(){
			
			var messageClicked = 1 + tab.find("dt").index( this );
			var trackText = "clicked FAQ question number "+messageClicked;
			
			GAQPushTransition("_trackEvent", "FE-1843", trackText, "Click", 1, true);
			
			if('faq_open' != $(this).attr('class')){
				tab.find("dt").find('p').show();
				$(this).find('p').hide();
				$(this).addClass("faq_open");
				$(this).siblings("dt").removeClass("faq_open");
				$(this).siblings("dd").slideUp();
				$(this).next().slideDown();
			}
		});
	});
</script>



