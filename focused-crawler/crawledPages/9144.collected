    function droplistPopulate( data, droplistJSON, callback ) {
        
        $.ajax({
                url: '/AjaxDroplists.do;jsessionid=16B75FEBA044974944DB1E01F22A2AA3.node170a',
                
                data: data + '&wrapNonAirports=true' ,
                	
                dataType: 'json',
                success: function( result ) {            
                    $( droplists ).each( function() {                 
                        if( this.index >= droplistJSON.index ) {                  
                            var element; 
                            element = $( this.jqSelector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) );
                        }
                        if( this.index != droplistJSON.index )
                            $( element ).attr('disabled',true);
                    });
                    
                    
                   
                   
                    	if(fillTopLocsOptions( droplistJSON.jqSelector, result )){
                    	
                    		$(droplistJSON.jqSelector).attr('disabled', false);
                    	}
                    
                    	else {
                    		
                   
                    		$( result ).each(function() {      
                    			if( typeof this.timeZone != 'undefined' ) {
                                    
                                    $( droplistJSON.jqSelector ).append( 
                                            $( document.createElement('option') )
                                                .html( this.name )
                                                .attr('timeZone',this.timeZone)
                                                .val( this.id ) )
                                            .attr('disabled',false);
                                } else {
                                    
                                    $( droplistJSON.jqSelector ).append( 
                                            $( document.createElement('option') )
                                                .html( this.name )
                                                .val( this.id ) )
                                            .attr('disabled',false);                      
                                }
                            }); 
                    		
                     } 
                        
                    if( typeof callback != 'undefined' ) callback.call(); 
                }
            });
        }
    
        function droplistPopulateWithData( data ) {
            
            $.ajax({
                url: '/AjaxDroplists.do;jsessionid=16B75FEBA044974944DB1E01F22A2AA3.node170a',
                
                data: data + '&mode=fillAll&wrapNonAirports=true' ,
                	
                dataType: 'json',
                success: function( result ) {
                    $( droplists ).each(function() {                  
                        var selector = this.jqSelector;
                        var enable = false;  
                        var element; 
                        element = $( this.jqSelector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) ).attr('disabled',true);
                       
                       // $( selector ).empty().append( $( document.createElement('option') ).html( pleaseSelectTxt ).val( 0 ) ).attr('disabled',true);
                            
                            
                            
                            	var enableDefaultSelection = false;
								
                            	if(this.fillAllName === 'locationList' && fillTopLocsOptions(selector, result[this.fillAllName])) {
                            		
                            		enable = true;
                            		enableDefaultSelection = true;
                            	}
                            	else {
                            		
                            
                                                        
	                            if (typeof(this)=='object') { 
	                            	
		                            $( result[ this.fillAllName ] ).each(function() {
		                            	if( typeof this.timeZone != 'undefined' ) {
		                                    
		                                    $( selector ).append(
		                                            $( document.createElement('option') )
		                                                .html( this.name )
		                                                .attr('timeZone',this.timeZone)
		                                                .val( this.id )
		                                        ); 
		                                        enable = true;
		                                    
		                                } else {
		                                    
		                                    $( selector ).append(
		                                            $( document.createElement('option') )
		                                                .html( this.name )
		                                                .val( this.id )
		                                        ); 
		                                        enable = true;                              
		                                }
		                                
		                            });
	                                
	                            }
                            		 
                           		
                            		} 
	                            	
	                            		if(enableDefaultSelection) {
			                    			$('#pu-location').val('0');
			                    			$('#do-city').attr('disabled', true);
	                            		}
	                    			
							var JSEXPtj_landing_page_city_default_location = false;
							
                            
	                        
                            if( enable == true ) $( selector ).attr('disabled', false);
                            if( typeof result[ this.fillAllSelect ] != 'undefined' && result[ this.fillAllSelect ] != ''  ) {
                            	if (!JSEXPtj_landing_page_city_default_location) {
                            			$(selector).val(result[this.fillAllSelect]);
                            			if ($(selector).val() == result[this.fillAllSelect]) {
									$( selector ).val( result[ this.fillAllSelect ] ).find('option[value=0]').attr('disabled',true);
                            			} else {
                            				$(selector).val(result[this.fillAllSelect]).find('option[value=0]');
                            			}
                            	}
                            }
                    });                
                }
            
            });
        }
        
      
       
        function hasTopLocsAttributes(selectorName, list){
        	
        	var returnValue = false;
        	if(selectorName && list) {
        		
        		switch(selectorName){
        		case 'location':
        		case 'dropLocation':
        			break;
        			
        			default: return false;
        		}
        		
        		$(list).each(function(){
        			
        			if(this.type) {
        				switch(this.type) {
        				case 'other':
        				case 'railstation':
        					returnValue = true; break;
        				}
        			}
        			
        			if(returnValue) return false;
        			
        		});
        	}
        	
        	return returnValue;
        }
        
        function fillTopLocsOptions(selector, json) {
        	
        	if(!hasTopLocsAttributes($(selector).attr('name'), json)) return false;
        	
        	var foundAirport = false, foundcity = false;
        	var airportGroup = $('<optgroup></optgroup>').attr('label', 'No Aeroporto');
        	var citiesGroup = $('<optgroup></optgroup>').attr('label', 'Na Cidade');
        	
        	$(json).each(function (){
        		
        		var appender = null;
        		
        		if (this.type === 'airport') {
        			appender = airportGroup;
        			foundAirport = true;
        		}
        		else if (this.type === 'other' || this.type === 'railstation' || this.type === 'allareas') {
        			appender = citiesGroup;
        			foundcity = true;
        		}
       			
        		if(appender){
        			var option = $('<option></option>').html(this.name).attr('value', this.id);
        			$( appender ).append( option );
        		}
        	});
        	
        	if(foundAirport) $(selector).append(airportGroup);
       	
        	if(foundcity) $(selector).append(citiesGroup);
        	
        	return foundAirport || foundcity;
        }
        
   

