	<script type="text/javascript">
	$(function(){
		var CICLE_INTERVAL=7000, TRANSITION_TIME=250,
			templateSelector='#single-review-tmpl', reviewContainerSelector='.reviews-list>.review-item';
		var ReviewModel = function(id, title, starRating, flag, flagLC, customerName){
			this.id = id;
			this.title = title;
			this.starRating = starRating;
			this.flag = flag;
			this.flagLC = flagLC;
			this.customerName = customerName;
		};
		
		var ReviewView = function(model){
			var reviewTmpl = $(templateSelector).html();
			var $container = $(reviewContainerSelector);
			this.render = function(ignorefixedHeight){
				$container.html(RCTemplate.apply(reviewTmpl, model));
				if($container.css('visibility')=='hidden') {
					$container.css('visibility','visible');
					$container.animate({ opacity: 1 },TRANSITION_TIME);
				}
			};
			this.destroy = function(done){
				$container.animate({ opacity: 0 },TRANSITION_TIME,function(){
					$container.css('visibility','hidden');
					$container.empty();
					done();
				});
			};
		};
		
		var ReviewsController = (function(){
			var reviewView = null;
			var cicleIndex = 0;
			return {
				init: function(){
					var model = reviewCollection.at(0);
					reviewView = new ReviewView(model);
					reviewView.render();
					this.startCicle();
				},
				startCicle: function(){
					var that = this;
					cicleID = setInterval(function(){
						that.switchReview();
					},CICLE_INTERVAL);
				},
				switchReview: function(){
					reviewView.destroy(function(){
						cicleIndex+=1;
						if (cicleIndex>=reviewCollection.elements.length)
							cicleIndex=0;
						var model = reviewCollection.at(cicleIndex);
						reviewView = new ReviewView(model);
						reviewView.render();
					});
				}
			};
		})();
		
		var RCTemplate = {
				// Very simple method to apply a template to a model
				apply: function(template,model){
					for (var i in model) {
						var replaceRegex = new RegExp('{{'+i+'}}','g');
						template = template.replace(replaceRegex, model[i]);
						// Managing conditional code
						if (model[i]==false||model[i]=='') {
							// If the property is empty or false remove the whole part between {{#property}} and {{/property}}
							replaceRegex = new RegExp('{{#'+i+'}}.*{{\\/'+i+'}}','g');
							template = template.replace(replaceRegex,'');
						}
						else {
							// If not just remove the {{#property}} and {{/property}}
							replaceRegex = new RegExp('{{[#\\/]'+i+'}}','g');
							template = template.replace(replaceRegex,'');
						}
					}
					return template;
				}
			};
		
		var RCCollection = function(){
			var elements = this.elements = [];
			this.add = function(model){
				elements.push(model);
			};
			this.get = function(id){
				var element = null;
				for (var i in elements) {
					if (elements[i].id==id) {
						element = elements[i];
						break;
					}
				}
				return element;
			};
			this.at = function(index){
				return (elements[index])?elements[index]:null;
			};
			this.indexOf = function(element){
				return this.indexByID(element.id);
			};
			this.indexByID = function(id){
				var index = 0;
				for (var i in elements) {
					if (elements[i].id==id) {
						index = parseInt(i);
						break;
					}
				}
				return index;
			};
		};
		
		var reviewCollection = new RCCollection();
		
		reviewCollection.add(new ReviewModel(1,
				'ILHA VERDE - A RECOMENDAR',
				5,
				'Portugal','portugal',
				'Sr&nbsp;J. Abreu'));
		
		reviewCollection.add(new ReviewModel(2,
				'Very positive, make you feel very easeful',
				5,
				'Portugal','portugal',
				'Sr&nbsp;R. Ferraz'));
		
		reviewCollection.add(new ReviewModel(3,
				'Excelente custo-beneficio',
				5,
				'Portugal','portugal',
				'Sr&nbsp;J. Mendonca'));
		
		reviewCollection.add(new ReviewModel(4,
				'Excellent backoffice and follow up process',
				5,
				'Portugal','portugal',
				'Sr&nbsp;R. Caricchio'));
		
		reviewCollection.add(new ReviewModel(5,
				'Wonderful website!!',
				5,
				'Portugal','portugal',
				'Sr&nbsp;J. Corceiro'));
		
		reviewCollection.add(new ReviewModel(6,
				'Good deal in Santiago airport.',
				4,
				'Portugal','portugal',
				'Sr&nbsp;F. Rostock'));
		
		reviewCollection.add(new ReviewModel(7,
				'Excelente',
				5,
				'Portugal','portugal',
				'Sr&nbsp;F. Oliveira'));
		
		reviewCollection.add(new ReviewModel(8,
				'CARRO CONFORTÁVEL E BAIXO CONSUMO.',
				5,
				'Portugal','portugal',
				'Sr&nbsp;J. Gomes'));
		
		
		if (reviewCollection.elements.length>=5) {
			$('#reviews').css('display','block');
			ReviewsController.init();
		}
	});
	</script>
