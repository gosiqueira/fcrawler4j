/* Sub nav */
.rc-nav-sub-item {
  display: none;
  padding: 15px 25px;
  position: absolute;
  background-color: #FFF;
  right: 0;
  left: inherit !important;
  border-top: 4px solid #1B65A4;
  box-shadow: -2px 2px 3px rgba(0,0,0,0.4);
  cursor: auto;
  pointer-events: initial;
  min-height: 302px;
  z-index: 9999;
}
.rc-nav-item.active .rc-nav-sub-item {
  display: block;
}

.menu-fade {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 40px;
  background: -moz-linear-gradient(top, rgba(255,255,255,0) 0%, rgba(255,255,255,0.35) 35%, rgba(255,255,255,0.65) 65%, rgba(255,255,255,1) 100%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,0)), color-stop(35%,rgba(255,255,255,0.35)), color-stop(65%,rgba(255,255,255,0.65)), color-stop(100%,rgba(255,255,255,1)));
