<script>

	function sortList(ulSelector,valSelector){
	    var ul = $(ulSelector);
	    var list = $.makeArray(ul.children());
	    
	    list.sort(function(a,b){
	    	
	    	var a = $(a).find(valSelector).text();
	    	if(!a) a = 0;
	    	var b = $(b).find(valSelector).text();
	    	if(!b) b = 0;

	    	var output =parseFloat(b) - parseFloat(a); 
	    	if(isNaN(output)){
	    		output = 0;
	    	}
	        return output;
	    });
	    ul.empty().append(list);
	}

	$(function(){
		
		var tabs = $('.more_destination_info_tabs li');
		var panes = $('div.more_destination_info_box > div.info_text');

		
			sortList('#brand_links_logos','.avg');
		

		 $('#carCatUl li:odd').addClass('even');
		 $('#nearUl li:odd').addClass('even');
		 $('#popCityUl li:odd').addClass('even');
		 $('#popAirportUl li:odd').addClass('even');
		 $('#brand_links_logos li:odd').addClass('even');
		 
		 
		function selectPane(tabNum){
			
			tabs.removeClass('destinations_active_tab');
			tabs.eq(tabNum).addClass('destinations_active_tab');
			panes.hide();
			panes.eq(tabNum).show();
		}
		
		selectPane(0);
		
		tabs.bind('click',function(){
			selectPane($(this).index());
		});
		
	});
</script>

