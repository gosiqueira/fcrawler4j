		else if (item.placeType == "T") { icon = "<i class=\"autocomplete-icons autocomplete-station\"></i>"; }
		else if (item.placeType == "W") { icon = "<i class=\"autocomplete-icons autocomplete-country\"></i>"; }
		else if (item.placeType == "Z") { icon = "<i class=\"autocomplete-icons autocomplete-place\"></i>"; }
		return icon;
	}
	
