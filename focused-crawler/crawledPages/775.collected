		// enable after user allow geolocation
		disableGeo();
		showLoading(true);

		if (navigator.geolocation) {
			navigator.geolocation.getCurrentPosition(function(position) {

				geoInputLat.val(position.coords.latitude);
				geoInputLng.val(position.coords.longitude);

