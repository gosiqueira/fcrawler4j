<script>

function viewShoppingCart() {
    var cartSUrl = "//br.viator.com/pt/8328/booking/view-cart";
    document.location.href = cartSUrl;
    return false;
}

function showErrorMsg(title, body, list) {
	
	var $errorMsg = $('#errorMsg');
	var $errorMsgTitle = $('#errorMsgTitle');
    var $errorMsgBody = $('#errorMsgBody');
    var $errorMsgList = $('#errorMsgList');
    
    $errorMsgTitle.html(unescape(title));
    if(body != null) {
    	$errorMsgBody.html(unescape(body));
    }
    $errorMsgList.find('li').remove();
    if (list) {
    	$.each(list, function(index, value) {
    		$errorMsgList.append('<li>' + value + '</li>');
    	});
    	$errorMsgList.find('li:empty').remove();
    }
    
    $errorMsg.show();
}

$('#submitCart').on('click', function(e) {
    e.preventDefault();

    var $this = $(this);
    var $form = $('#quoteform');
    var $errorMsg = $('#errorMsg');
    var $errorMsgTitle = $('#errorMsgTitle');
    var $errorMsgBody = $('#errorMsgBody');
    var $errorMsgList = $('#errorMsgList');

    if ($this.data('clicked')) {
        return;
    }

    $this.data('clicked', true);
    $this.prop('disabled', true);

    $errorMsg.hide();

    $.ajax({
        type : 'POST',
        data : $form.serialize(),
        url : $form.attr('action'),
        success : function(data) {
            if (!data.status) {
                if(!data.tourAvailable){
                    showErrorMsg("Lamentamos, mas esta excursão ou atividade não está disponível no momento.", "Tente selecionar outra data e/ou um número diferente de viajantes.");
                } else if (data.ageInRange) {
                	showErrorMsg("Lamentamos, mas não foi possível confirmar os preços devido ao seguinte:", null, data.ageInRange);
                }

                $this.data('clicked', false);
                $this.prop('disabled', false);
            }

            if (data.tourOptionsRedirection) {
                
                $("#productCodeByPost").remove();                 
                var yearMonthComp = $("#yearMonth").val().split("-");
                var redirectUrl = '/pt/8328/product/tour-options/2484IGUHELI/?year=' + yearMonthComp[0]  + '&month=' + yearMonthComp[1] + "&day=" + $("#day").val();
                $("select[name^='ageBandCounts']").each(function(index) {
                    redirectUrl = redirectUrl + '&' + ('ageBandCounts%5B%27' + $(this).prop('name').split('.')[1] + '%27%5D=' + $(this).val()); 
                });
                
                $form.attr('action', redirectUrl);                                
                $form.submit();
            } else if (data.addItemToShoppingCartSuccess) {
                window.location.replace('/pt/8328/booking/shopping-cart');
                $this.prop('disabled', true);
            }
        },
        error : function( jqXHR, textStatus, errorThrown) {
            $this.data('clicked', false);
            $this.prop('disabled', false);

            showErrorMsg("Ocorreu um erro", "Lamentamos! Encontramos uma falha temporária e por isso não foi possível processar sua solicitação.<br/>Nossa equipe está investigando o problema. Enquanto isso, tente pressionar o botão de retorno, atualizar a página e retornar a http://br.viator.com");

            Viator.Common.logError('Error adding or updating item to cart: ' + textStatus + ' (' + errorThrown + ')');
        }
    });
});

$(document).on('hotelPickupOptionSelected', function(event, params) {
    $("input[name='selectedHotelId']").val(params['hotelId']);
});

</script>


                     
