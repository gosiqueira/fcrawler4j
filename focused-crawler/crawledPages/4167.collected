<h1>{{ "shopping.cart.cross-selling.module.title" | i18n | placeholder : underscore.last(shoppingCart.cart).destination.name }}</h1>
</div>
<div ng-repeat="offer in model.offers | limitTo : SHOPPING_CART_OFFERS_QT" >
<div class="ds-resume-cluster-content ds-resume-cluster-more-activities ds-resume-recommended-activities">
<div class="ds-resume-cluster" ng-click="trackRecommendedViewDetailEvt(offer.data); promotionGoToDetail(offer.data)">
<div class="ds-resume-cluster-figure"
style="background-image: url('{{ offer.data.thumb.key | mediaAkamaiURL : model.home }}/162x148'), url('/tickets/library/css/images/activities/default-cluster-image.jpg')"
title="{{ title }}">
<span class="providers">Tracking ID: {{ offer.data.trackingId }}</span>
</div>
<div class="ds-resume-cluster-text">
<div class="ds-resume-cluster-title" ng-init="TITLE_MAX_CHARS = 82">
<span class="ds-resume-cluster-rating" ng-if="offer.data.reviewInformation">
<span title="Basado en {{ offer.data.reviewInformation.totalComments }} comentarios">
{{ offer.data.reviewInformation.rating }}
<em>puntos</em>
</span>
<span class="ds-resume-cluster-rating-comments">
Basado en {{ offer.data.reviewInformation.totalComments }} comentarios
</span>
</span>
<span ds-bind-html="offer.data.title" title="{{ offer.data.title }}"></span>
</div>
<div class="ds-resume-cluster-description">
<p ds-bind-html="offer.data.description"></p>
</div>
<div class="ds-resume-cluster-fade-overlay"></div>
</div>
<span class="ds-discount-ribbon"
ng-if="offer.data.promotion"
ng-switch="offer.data.promotion.type">
<span ng-switch-when="OFFER">{{offer.data.promotion.label}}</span>
<span ng-switch-when="DISCOUNT">{{offer.data.promotion.percentage}}% OFF</span>
<span class="ribbon-tail"></span>
<span class="ribbon-behind"></span>
</span>
<div class="ds-resume-cluster-pricebox" ng-class="{'-with-ribbon': offer.data.promotion}">
<div class="ds-resume-cluster-pricebox-inner">
<div class="ds-resume-cluster-pricebox-center">
<div class="ds-resume-cluster-price-total-container" ng-switch="offer.data.type">
<ul>
<li class="ds-resume-cluster-price-disclaimer" ng-switch-when="TICKET">{{ "filter.price.singular" | i18n }} {{ "by.ADULT " | i18n | lowercase }}</li>
<li class="ds-resume-cluster-price-disclaimer" ng-switch-when="TOUR">{{ "filter.price.singular" | i18n }}</li>
<li class="ds-resume-cluster-price-strikethrough" ng-if="offer.data.price.originalAmount">
<em>{{ "currency.symbol" | i18nEnum : offer.data.price.currency }}</em>
{{ offer.data.price.originalAmount }}
</li>
<li class="ds-resume-cluster-price-total">
<em>{{ "currency.symbol" | i18nEnum : offer.data.price.currency }}</em>
{{ offer.data.price.amount | localePrice : 'BO' }}
</li>
</ul>
</div>
<div class="ds-resume-cluster-price-button-container">
<a class="eva-button-primary-medium"><em>{{"see.detail" | i18n}}</em></a>
</div>
</div>
</div>
</div>
</div>
</div> </div>
</div>
</div>
</div>
