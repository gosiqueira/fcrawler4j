<div class="ds-pricebox-item activity" ng-repeat="item in shoppingCart.cart track by item.hash" ng-if="!item.bundle && item.prices">
<!-- pricebox item :: name -->
<div class="ds-item-text">
<p class="product"> <i ng-class="{'ux-destination-services-icons-adventure': item.category == 'ADVENTURE_TOUR', 'ux-destination-services-icons-parks': item.category == 'ATTRACTION_THEME_PARK' || !item.category, 'ux-destination-services-icons-culture': item.category == 'CULTURE_TOUR',
'ux-destination-services-icons-nightlife': item.category == 'FOOD_NIGHTLIFE', 'ux-destination-services-icons-other': item.category == 'OTHER', 'ux-destination-services-icons-interest': item.category == 'POI', 'ux-destination-services-icons-transport': item.category == 'TRANSFER', 'ux-destination-services-icons-shopping': item.category == 'SHOPPING', 'ux-destination-services-icons-shows': item.category == 'SHOW_EVENT', 'ux-destination-services-icons-ski': item.category == 'SKI', 'ux-destination-services-icons-circuit': item.category == 'TRAVEL_ROUTES'}" ></i>
<span></span>
{{ item.name }}</p>
</div>
<!-- pricebox item :: price -->
<div class="ds-item-price">
<span class="eva-tooltip-top ds-resume-pricebox-tooltip">
<em>{{ model.currency | currencySymbol }}</em>
{{ price(item).basePrice | localePrice : model.home }}
<span class="tooltip-content">
<span class="content-inset">
<dl class="tooltip-item">
<dt class="tooltip-product" ng-pluralize
count="adults(item.distribution)"
when="{
'1':' 1' + ' ' + ('shopping.cart.adult.singular'|i18n|lowercase),
'other': ' {{adults(item.distribution)}}' + ' ' + ('shopping.cart.adult.plural'|i18n|lowercase)
}">
</dt>
<dd class="tooltip-price">{{ model.currency | currencySymbol }} {{ price(item).totalPrice.adult.disaggregatedPrice.sellingPrice.gross | localePrice : model.home }}</dd>
</dl>
<dl class="tooltip-item" ng-if="childrens(item.distribution) != 0">
<dt class="tooltip-product" ng-pluralize
count="childrens(item.distribution)"
when="{
'1':' 1' + ' ' + ('shopping.cart.child.singular'|i18n|lowercase),
'other': ' {{childrens(item.distribution)}}' + ' ' + ('shopping.cart.child.plural'|i18n|lowercase)
}">
</dt>
<dd class="tooltip-price" ng-show="price(item).totalPrice.child.disaggregatedPrice.sellingPrice.gross">{{ model.currency | currencySymbol }} {{ price(item).totalPrice.child.disaggregatedPrice.sellingPrice.gross | localePrice : model.home }}</dd>
<dd class="tooltip-price" ng-show="!price(item).totalPrice.child.disaggregatedPrice.sellingPrice.gross">-</dd>
