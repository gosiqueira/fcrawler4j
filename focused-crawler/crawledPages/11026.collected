					    <option value="22">Canary Islands</option>
					    <option value="5">Caribbean / Bahamas</option>
					    <option value="2">Eastern Mediterranean /Black Sea</option>
					    <option value="3">N. Europe and Norwegian Fjords</option>
					    <option value="1">Western Mediterranean</option>
				</select>
			</div>
		</div>

        <!-- Por Fecha de salida -->
        <div class="col-xs-12 col-sm-6 col-md-4">
			<label for="salidaInicioChollosCruceros" class="txtGrande bold mb-5">For departures between</label>
			<div class="row row-narrow mb-xs-10">
				<div class="col-xs-5 pr-0">
					<div class="inputFake contSalidaInicioChollosCruceros">
						<div class="input-group">
