      <script type="text/javascript">
      $(document).ready(function(){
        $('.homepage-slick-unique').slick({
          dots: true,
          infinite: true,
          speed: 600,
          autoplay: true,
          autoplaySpeed: 4500,
          onInit : addNextSlideTitle,
          onAfterChange : updateSlideTitle
        });
      });

      // As the carousel is initialised add the container for the next slide title
      // Also get the title value for the first slide by calling the same function
      // that will be called to update the text each time
      function addNextSlideTitle(slick) {
          
          var nextTitleContainer = $(document.createElement('div'));
          var nextHeader = $(document.createElement('p')).addClass('head').text('Next');
          var nextTitle = $(document.createElement('p')).addClass('body');
          nextTitleContainer
              .attr('id', 'next-slide-title').addClass('next-slide-title')
              .append(nextHeader).append(nextTitle)
              .appendTo($('.homepage-slick-unique .slick-list'));
          updateSlideTitle(slick, 0);
      }

      // Set the text of the next slide title
      // Callback function for slide change
      function updateSlideTitle(slick, currentIndex) {
          var currentSlide = $('.home-carousel-item[index=' + currentIndex + ']');
          var nextSlideTitle = $(currentSlide).next().find('.slick-caption h2').text();
          // If there is no text in the h2 then get the h3
          if(nextSlideTitle.length === 1) {
              nextSlideTitle = $(currentSlide).next().find('.slick-caption h3').text();
          }

          $('#next-slide-title .body').text(nextSlideTitle);         
      }

      </script>
