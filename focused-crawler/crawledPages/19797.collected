/**
* Private javascript for hotels_price_slider_histogram widget
*/
define(["widget"], function(widget) {
/***********************************************
* PRIVATE FUNCTIONS
***********************************************/
/**
* Handler called when values on the price slider changed.
* We want to correctly change the set of histograms that
* are marked as selected after the user changed prices.
*/
function onValuesChanged() {
var min = getSelectedMin();
var max = getSelectedMax();
var histBars = document.getElements(".price_histogram .histogramBar");
if (!histBars || !min || !max) {
return;
}
histBars.each(function(curBar) {
var curMinPrice = parseInt(curBar.getAttribute("data-minPrice"), 10);
var curMaxPrice = parseInt(curBar.getAttribute("data-maxPrice"), 10);
if (!curMinPrice || !curMaxPrice) {
return;
}
if (curMaxPrice >= min && curMinPrice <= max) {
curBar.addClass("selected");
} else {
curBar.removeClass("selected");
}
});
}
/**
* Get the current minimum price user has selected
*/
function getSelectedMin() {
var minFilter = document.getElement("#jfy_filter_j");
if (!minFilter) {
return;
}
return parseInt(minFilter.getAttribute("data-value"), 10);
}
/**
* Get the current maximum price user has selected
*/
function getSelectedMax() {
var maxFilter = document.getElement("#jfy_filter_k");
if (!maxFilter) {
return;
}
return parseInt(maxFilter.getAttribute("data-value"), 10);
}
/**
* Handle the event where the filter is reset. We want to
* select all the histograms on the page in this case since
* the prices are now expanded to include everything
*/
function onResetFilters() {
var histBars = document.getElements(".price_histogram .histogramBar");
histBars.each(function(curBar) {
curBar.addClass("selected");
});
}
/***********************************************
* INITIALIZERS
***********************************************/
ta.queueForLoad(function() {
ta.on("priceSlider.valuesChanged", onValuesChanged);
ta.on("priceSlider.mouseMove", onValuesChanged);
ta.on('hotels.resetFilters', onResetFilters);
} , 'initialize ' + widget.name);
return { };
});
})();
