	<script>
	//Preenchimento de variáveis ENVIRONMENT
	//preenche variaveis tc_vars["env_template"] e tc_vars["xt_s2"] de acordo com a pagina que estao
	//Preenchimento de variáveis ENVIRONMENT
		//preenche variaveis tc_vars["env_template"] e tc_vars["xt_s2"] de acordo com a pagina que estao
		tc_vars["env_work"]= 'PRD';
		var jsonPages='{"pages": [{"id": "1","name": "HOME_PAGE"},{"id": "1001","name": "HOME_PAGE"},{"id": "6","name": "UNIVERS"},{"id": "1006","name": "UNIVERS"},{"id": "7","name": "CATEGORY"},{"id": "1007","name": "CATEGORY"},{"id": "8","name": "UNIVERS"},{"id": "1008","name": "UNIVERS"},{"id": "16","name": "PRODUCT"},{"id": "1016","name": "PRODUCT"},{"id": "1018","name": "PRODUCT"},{"id": "18","name": "PRODUCT"},{"id": "19","name": "PRODUCT"},{"id": "1019","name": "PRODUCT"},{"id": "30","name": "INTERNAL_SEARCH"},{"id": "1030","name": "INTERNAL_SEARCH"}],"UNIVERS":[{"name":"montanha---aventura","id":"2"},{"name":"esporte-aquaticos","id":"3"}, {"name":"roller","id":"4"}, {"name":"running","id":"5"}, {"name":"raquetes---golfe-37703","id":"6"}, {"name":"fitness","id":"7"}, {"name":"suplementos-e-saude","id":"8"},{"name":"esportes-coletivos","id":"9"}, {"name":"rodas","id":"10"}, {"name":"natureza","id":"11"}, {"name":"esportes-de-combate","id":"12"}, {"name":"outlet","id":"13"}, {"name":"eletronicos-e-oculos","id":"22"}, {"name":"pesca-37790","id":"23"}, {"name":"equitacao-37627","id":"24"}, {"name":"natacao-37170","id":"25"}, {"name":"default","id":"20"}]}';
		jsonPages=$.parseJSON(jsonPages);
		var pages = jsonPages.pages;
		var univers=jsonPages.UNIVERS;
		var showcase=jsonPages.SHOWCASE;
		var cat1 ='';
		var cat2 = '';
		var cat3 ='';
		var homeId = '1';
		var founds2=false;
		var s2default='';
		var levelList=new Array('');	
		var urlProdName = window.location.pathname.indexOf('_') >= 0 ? window.location.pathname : '';
		urlProdName = urlProdName.substring(urlProdName.lastIndexOf('/')+1,urlProdName.indexOf('_'));
                tc_vars["page_cat1"]= cat1;		
                tc_vars["page_cat2"]= cat2;
                tc_vars["page_cat3"]= cat3;
		
		for(i=0; i < pages.length; i++){
			if(homeId==pages[i].id){
				tc_vars["env_template"]=pages[i].name;
				if(homeId==1 || homeId==1001){
					tc_vars["xt_s2"]="1";
					tc_vars["page_name"]="Homepage";	

				} else if(homeId==30 || homeId==1030){
					tc_vars["xt_s2"]="18";
					tc_vars["page_name"]="Resultado_de_busca";
				} 				
				//<![CDATA[
				if(pages[i].name=='UNIVERS' || pages[i].name=='CATEGORY' || pages[i].name=='PRODUCT'){
					for(j=0; j < univers.length; j++){
						if(univers[j].name.toLowerCase()==cat1){
							tc_vars["xt_s2"]=univers[j].id;
							founds2=true;
						} else if(univers[j].name.toLowerCase()=='default'&& founds2==false){
							s2default=univers[j].id;
						}
					}
					if(tc_vars["xt_s2"]==''){
						tc_vars["xt_s2"]=s2default;
					}
				}
					
				
				if(pages[i].name=='SHOWCASE'){
					var hotsite='';
					if(homeId=='34' || homeId==1034){
						if('' == 4){
							hotsite='hotsite2';
						} else {
							hotsite='hotsite1';
						}
					} else {
						hotsite='hotsite3';
					}
					for(j=0; j < showcase.length; j++){
						if(showcase[j].name.toLowerCase()==hotsite){
							tc_vars["xt_s2"]=showcase[j].id;
						} 
					}
				}
				//]]>
			}
		}
		
	
		//Preenchimento de variáveis Tree structure
		if(homeId==6 || homeId==1006 || homeId==7 || homeId==1007 || homeId==8 || homeId==1008){
			tc_vars["page_cat1"] = cat1;
			tc_vars["page_name"] = pageNameFormat();
		}else if(homeId==16 || homeId==1016){
			tc_vars["page_name"] = pageNameFormat()+'::produto_'+ urlProdName;
		}else if(homeId==33 || homeId==1033){
			tc_vars["xt_s2"]="16";
			tc_vars["page_name"]="Nossas_lojas";
		}
	
		
		//Monta variavel page_name no formato Escalada_e_alpinismo::Mochilas::Vitrine_Mochilas
		function pageNameFormat(){
			var pageName ='';
			if(cat3!=''){
				pageName=formatCategory(cat2)+'::'+formatCategory(cat3)+'::Vitrine_'+formatCategory(cat3);
			}else if(cat2!=''){
				pageName='Vitrine_'+formatCategory(cat2);
			} else if(cat1!=''){
				pageName='Vitrine_'+formatCategory(cat1);
			}
			return pageName;
		}
		
		function formatCategory(category){
			category = category.replace(/\-/g,'_');
			category = category.replace(/\//g,'');
			category = category.replace(/\_?\d/g,'');
			category = category[0].toUpperCase() + category.substring(1);
			return category;
		}
		
			//Preenchimento das tags users
		//<![CDATA[
		var store = $.cookie('st');
					
		var nomeCookie = ('USER_INFO' + (store != null ? store : ''));
		var dadosCookie = $.cookie(nomeCookie);  
	
		if (dadosCookie!= null && dadosCookie!= "") {
			tc_vars["user_id"] = splitCookie(1);
			tc_vars["user_gender"] = splitCookie(2);
			tc_vars["user_recency"] = splitCookie(3);
			tc_vars["user_logged"] = splitCookie(4);
			tc_vars["user_age"] = splitCookie(5);
			tc_vars["user_postalcode"] = splitCookie(6);
			tc_vars["user_email"] = splitCookie(7);
			tc_vars["user_status"] = userStatus();
                        tc_vars["user_cookie_acceptation"] =  navigator.cookieEnabled;

			optIn = splitCookie(8);
			if(optIn=='true'){
				tc_vars["user_email_optin"] ='1'
			}else {
				tc_vars["user_email_optin"] ='0'
			}
			tc_vars["user_newcustomer"] = splitCookie(9);
			if(tc_vars["user_newcustomer"]=='false'){
				tc_vars["user_newcustomer"]='no';
			}else{
				tc_vars["user_newcustomer"]='yes';
			}
			tc_vars["user_firstname"] = splitCookie(10);
			tc_vars["user_lastname"] = splitCookie(11).replace("}", "");
		} 
		
		if('false' == 'false'){
			tc_vars["user_logged"]='no';
			tc_vars["user_newcustomer"] = '';
		}
		//]]>
		function splitCookie(position){
			var dado=dadosCookie.split('|')[position];
			if (dado != undefined) {
				var index = dado.indexOf('=')+1;
				dado = dado.substring(index);
			} else {
				dado = "";
			}
			return dado;
		}

	       function userStatus(){
                   if(tc_vars["user_logged"]="no")
                      return 1;
                   else
                     return 0;
               }
	
    </script>
