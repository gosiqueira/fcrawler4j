                $(".csOtherLinks .toggle-inner").slideToggle("slow", function () { });
            });

            // ----------------------------------
            // Infinite Scroll Manual Trigger as Twitter functionality
            // ----------------------------------
            var $tumblelog = $('#GridContainer');

            // Chrome Bug - Quick fix: http://stackoverflow.com/questions/7256695/jquery-masonry-height-issue
            $tumblelog.masonry({
                columnWidth: function (containerWidth) {
                    return containerWidth / 100;
                }
            }).masonry('reload');

            $tumblelog.infinitescroll({
                navSelector: '.navigation',
                nextSelector: '.navigation .next',
                itemSelector: 'article.post',
                behavior: 'twitter',
                loading: {
                    msgText: 'Espere...',
                    finishedMsg: 'Loaded tudo!'
                }
            },
              // call Isotope as a callback
              function (newElements) {
                  var $newElems = $(newElements).css({
                      opacity: 0
                  });
                  $newElems.imagesLoaded(function () {
                      $newElems.animate({
                          opacity: 1
                      });
                      $tumblelog.masonry('appended', $newElems);
                  });
              });

            
            $tumblelog.imagesLoaded(function () {
                $tumblelog.masonry({
                    columnWidth: function (containerWidth) {
                        return containerWidth / 100;
                    }
                });
            });

        }); //Domready
    </script>
