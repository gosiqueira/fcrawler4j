    var nextImage = function(img){
        var parentData = img.parentNode;
        while ( parentData.nodeName != 'LI' ){
            parentData = parentData.parentNode;
        }

        var dataImg = parentData.getAttribute('data-img').split(','),
            indexErrorImg = dataImg.indexOf(img.src),
            nextImgIndex = (indexErrorImg == dataImg.length - 1) ? 0 : indexErrorImg + 1;

        dataImg.splice(indexErrorImg, 1);
        parentData.setAttribute('data-img', dataImg.join(","));
        parentData.setAttribute('data-index', nextImgIndex);
        img.src = dataImg[nextImgIndex] || 'http://br.staticontent.com/vacation-rentals/static/search/2.2.4/images/error/error-img-275.gif';
    }
