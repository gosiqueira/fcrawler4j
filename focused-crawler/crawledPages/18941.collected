/**
* Reposition/remove flyout when hotel list updates
*/
function updateDealsFlyout(ieFix) {
if(ta.retrieve('metaCheckRatesOverlay')) {
var overlay = ta.retrieve('metaCheckRatesOverlay');
if (overlay) {
var locId = overlay.container.getAttribute('data-locationid');
if (locId) {
var listing = ta.id('hotel_' + locId);
if (listing) {
var details = listing.getElement('.property_details');
if (details) {
overlay.options.positionSource = details;
if (overlay.visible) {
// reposition flyout
overlay.position();
if (ieFix) {
scrollToDealsFlyout(overlay);
}
var seeAll = listing.getElement('.seeAll');
if (seeAll) {
seeAll.addClass('on');
}
}
}
}
else {
// hotel is no longer included in the list, so close the flyout
closeDealsFlyout();
}
}
}
}
}
/**
* Close the deals flyout
*/
