}
/**
* Scrolls down the page until the deals flyout is visible if it is not already.
* Only runs on first load (not for progressive hac).
*/
function scrollToDealsFlyout(overlay) {
if (!overlay) {
return;
}
var container = overlay.container;
if (container) {
var locationId = container.getAttribute('data-locationid');
if (locationId) {
var listing = ta.id('hotel_' + locationId);
if (listing) {
var overlayCoordinates = container.getCoordinates()
, overlayBottom = overlayCoordinates.top + overlayCoordinates.height
, listingTop = listing.getCoordinates().top
, windowTop = window.getScroll().y
, windowBottom = windowTop + window.getHeight()
, pdp = ta.id('PERSISTENT_DATE_PICKER')
, pdpHeight = pdp && pdp.getHeight()
;
if (windowBottom < overlayBottom && (windowTop + pdpHeight) < listingTop) {
if (Fx && Fx.Scroll) {
new Fx.Scroll(window).start(0, listingTop - pdpHeight);
}
else {
window.scrollTo(0, listingTop - pdpHeight);
}
}
}
}
}
}
