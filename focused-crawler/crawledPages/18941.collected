function checkForOverflow() {
var tagsWrap = widget.element && widget.element.getElement('.p13n_inline_info'),
tags = tagsWrap && tagsWrap.getElements('.matchedTagWrap .matchedTag'),
more = widget.element && widget.element.getElement('.p13n_inline_info .more');
if (tagsWrap && tags && more) {
var revealMore = false;
var numTags = tags.length;
for (var i = 0; i < numTags; i++) {
if (tags.length > 0 && tagsWrap.getScrollWidth() > tagsWrap.getWidth() + 1) { // subpixel accuracy is a problem here
tags.pop().dispose();
revealMore = true;
} else {
if (tags.length > 0) {
var lastTag = tags[tags.length-1];
if (lastTag) {
lastTag.innerHTML = lastTag.innerHTML.replace(",", "");
}
}
break;
}
}
if (revealMore) {
more.innerHTML = "+" + i;
more.removeClass('invisible');
tagsWrap.removeClass('preTruncation');
}
