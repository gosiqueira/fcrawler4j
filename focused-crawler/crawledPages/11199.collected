</div><!-- #route-one | data-tab: Route | Días 1 y 2 -->
<div class="swiper-slide pageSection" id="934e843a-2424-40be-beba-ea3f93b75533" data-tab="route" data-pageSectionName="route">
    <div class="content">
        <!-- Jumbotron -->
        <div class="jumbotron LazyBackground " data-backgroundimage="http://cdn.logitravel.pt/microsites/circuitos/circuito/day_7_8.jpg">
            <!-- jumbo Box -->
            <section class="jumbo-box positive bottom right hidden-xs">
                <h3 class="lettering-lines name-before">Itiner&#225;rio<br />Dia 8</h3>
            </section>

        </div>

        <div class="breadcrumb-container hidden-xs">
            <div class="container">
                <ol class="breadcrumb">
                    <li class="level1">Costa Rica: natureza completa</li>
                    <li class="level2">3. Itiner&#225;rio</li>
                    <li class="active">Dia 8</li>
