<script type="text/html" id="new-alert-template" >
<div>
<div class="price-alert">
    <div class="popup-body">
			<div class="tooltip-container">
				<div class="tooltip-content">

	<div class="new-alert basic-form {{if product}}${product}-{{else}}generic-{{/if}}alert" id="alert-price-popup">
	{{if product }}
			{{if  typeof modify != "undefined" && modify == true}}

				{{if typeof renew != "undefined" && renew  == true}}
					<h3 class="typo-title-bg">Renovar mi alerta de {{if product=='fl' }}vuelos{{else product=='ho' }}hoteles {{else product=='pkg' }}paquetes {{/if}}
				{{else}}
					<h3 class="typo-title-bg">Modificar mi alerta de {{if product=='fl' }}vuelos{{else product=='ho' }}hoteles {{else product=='pkg' }}paquetes {{/if}}
				{{/if}}

			{{else}}
				<h3 class="typo-title-bg">Crear mi alerta de {{if product=='fl' }}vuelos{{else product=='ho' }}hoteles {{else product=='pkg' }}paquetes {{/if}}
			{{/if}}

		<img src="http://py.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
		<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
		{{if typeof adviceId != 'undefined' && adviceId == 'CUSTOM'}}
			<p class="nibbler-advice">${adviceText}</p>
		{{/if}}
	{{else typeof from != 'undefined' && from == "newsletter"  }}
	 	{{if typeof adviceId != 'undefined'}}
			{{if adviceId == 'UPDATED_SUBSCRIPTION'}}
				<h3 class="typo-title-bg">Ya est&aacute; suscripto a nuestras ofertas<img src="http://py.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">Su direcci&oacute;n ${mail} ya está suscripta a nuestras ofertas.</p>

			{{else adviceId == 'ALREADY_SUBSCRIBED'}}
				<h3 class="typo-title-bg">Ya est&aacute; suscripto a nuestras ofertas<img src="http://py.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">Su direcci&oacute;n ${mail} ya estaba suscripta a nuestras ofertas. &iquest;Prob&oacute; revisar su Correo no deseado?</p>

			{{else adviceId == 'SENT_CONFIRMATION'}}
				<h3 class="typo-title-bg">&iexcl;Gracias por suscribirse a despegar.com!<img src="http://py.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">Confirme su suscripci&oacute;n desde el email enviado a ${mail}. Recuerde, si no lo encuentra, que puede estar en Correo no deseado.</p>
			{{else adviceId == 'SENT_THANKS'}}
				<h3 class="typo-title-bg">&iexcl;Gracias por suscribirse a despegar.com!<img src="http://py.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">A partir de este momento recibir&aacute; las mejores ofertas exclusivas en su casilla ${mail}</p>
