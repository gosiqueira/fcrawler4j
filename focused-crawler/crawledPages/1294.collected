    <section class="module circuit-route">
        <h5 class="titularDestacado gris mt-xs-10 mb-15"><i class="fa fa-map-marker"></i>Itiner&#225;rio Completo </h5>
        <article class="txtcol-xs-2 txtcol-sm-2 txtcol-md-3 txtcol-lg-3">

            <!-- mapa ampliado-->
            <div class="view-details">
                <div class="galleryPhone hidden">
                    <a data-fancybox-group="gallery_mapa_01" class="fancybox" href="http://cdn.logitravel.pt/contenidosshared/images/staticMaps/72965_400x720.png"></a>
                </div>
                <figure class="mb-10">
                    <img class="lazy img-responsive" alt="Ver Mapa" src="http://cdn.logitravel.pt/comun/images/shim.gif" data-original="http://cdn.logitravel.pt/contenidosshared/images/staticMaps/72965_214x305.png" />
                </figure>
                <a data-fancybox-group="gallery_mapa_01" title="Mapa" href="http://cdn.logitravel.pt/contenidosshared/images/staticMaps/72965_400x720.png" class="more-details fancybox bg-normal transparent hidden-sm hidden-xs">
                    <div class="center ampliarMapa"><span class="txtGrande x2 blanco shadow-black"><i class="fa fa-map-marker fa-lg mr-7"></i>Ver Mapa</span></div>
                </a>
