			//$('#reactions_liste').prepend('<div class="resultnav"></div>');
			$('#reactions_liste').append('<div class="resultnav"></div>');
			
			var p = 1;// jQuery.url.attr("anchor");
			page = 1;
			/*
			if(p && p.indexOf('page_') >= 0) {
				page = Math.round(p.replace('page_',''));
			}  else {
				page = 1;
			}
			*/
			showPageReaction(page);
			
		});

		
		function showPageReaction(page) {
		
			var pageMax = Math.ceil($(window.listeRes).length / window.nbPerPage);
		
		    if(pageMax <= 1) {
		        $(window.listeRes).show();
		        return;
		    }
			if(page < 0) {
				page = 1;
			} else if(page >pageMax) {
				page = pageMax;
			}
			var first = (window.nbPerPage * (page-1) ) + 1;
			var last = (window.nbPerPage * (page) );
			var i = 0;
			
			$('#reactions_liste div.reaction:visible').slideUp('fast');
			$(window.listeRes).each(function() {
				i++;
				if(i >= first  && i<= last) {
					$(this).slideDown('fast');
				}
				
			});
			var html = '';
			if( page > 1) {
			//	html += '<a onclick="showPageReaction(1)" href="#page_1">&lt;&lt;</a> | ';
				html += '<a onclick="showPageReaction('+(page - 1)+')" href="#page_'+(page - 1)+'">Page précédente</a> | ';
			} else {
				html += '<span>Page suivante</span> | '; //<span>&lt;&lt;</span> | 
			}

			var pageSelect = page;
		
			html += '<span>'+window.Trads['page']+' '+pageSelect+'/ '+pageMax+' </span>';
			
			if (page < pageMax) {
				html += ' | <a onclick="showPageReaction('+(page + 1)+')" href="#page_'+(page + 1)+'">Page suivante</a>';
			//	html += ' | <a onclick="showPageReaction('+(pageMax)+')" href="#page_'+(pageMax)+'">&gt;&gt;</a>';
			} else {
				html += ' | <span>Page précédente</span>';	 // | <span>&gt;&gt;</span> 
			}
			
			$('.resultnav').html(html);
			
		}



		
		
	</script>
