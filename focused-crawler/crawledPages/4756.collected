              <input required type="email" class="nibbler-header-input" name="username" placeholder="Ingresa tu email">
              <label for="password">Contraseña</label>
              <input required data-minConditions="4" type="password" class="nibbler-header-input" name="password" placeholder="Ingresa una contraseña">

