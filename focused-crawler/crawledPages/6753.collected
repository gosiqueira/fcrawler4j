        <script type="text/javascript">
        function reloadCalendarUI(form)
        {
            if (!form)
                form = getSelectedForm(cleanSelectedOptions(getSelectedOptions(true)));

            // Obtenho o tipo de compra
            var tipoCompra = getSelectedOptions(false).toUpperCase();

            // url da página que monta os scripts
            var url = "/apt/jsp/reloadCalendar.jhtml";

            // Nome do campo de ida
            var inputIda = form +"-date1";

            // Nome do campo de volta
            var inputVolta = form +"-date2";

            // Dados para postar para a action
            var data = {
                dsTipoCompra:                   tipoCompra,
                dsCalendarUiCampoIda:           inputIda,
                dsCalendarUiCampoVolta:         inputVolta,
                dsCalendarUiMaxCalendar:        '1',
                dsCalendarUiTypCalendar:        'IdaVolta',
                dsCalendarUiDateIni:            '',
                dsCalendarUiDateFim:            '',
                dsCalendarUiDateFormat:         'dd/mm/y',
                cdEntCod:                       '0107130',
                nmBotaoCalendarioLojinha:       '/apt/jsp/pacoteDinamico/HomeNew/images/lojinha_calendario_17x18.png',
                nmBotaoHoverCalendarioLojinha:  '/apt/jsp/pacoteDinamico/HomeNew/images/lojinha_calendario_17x18.png'
            };

            // Div destino do ajax
            var target = "scriptCalendario";

            // Função que recarrega os scripts
            reloadCalendar(url, data, target, inputIda, inputVolta);
        }


        function fillNav(form)
        {
			var dsCobrandSomenteAereo = "";

                var icChecked = $("#"+ form +"-multiplus").attr("checked") == "checked";
                $("#nav").val(icChecked ? "multiplus" : "brasilPT");

        }
		

