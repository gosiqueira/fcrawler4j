                 $("#form_aereo_hotel-date1").attr('maxLength','10');
                 $("#form_aereo_hotel-date2").attr('maxLength','10');
            }
            else
            {
                $("#form_aereo_hotel-date1").attr('maxLength','8');
                $("#form_aereo_hotel-date2").attr('maxLength','8');
            }

        });


        function onIdaSelect(dateText, inst)
        {
            // Crio as variaveis com a data selecionado pelo user
            var minDate = getMinDate(dateText);
            var maxDate = getMinDate(dateText);

            var minStay = parseInt(10);
            var maxStay = parseInt(30);

            if (minDate != null && maxDate != null)
            {

                    minDate.setDate(minDate.getDate() + minStay);

                     maxDate.setDate(maxDate.getDate() + maxStay);


                // Adiciono ao calendario as datas calculadas
                $( "#form_aereo_hotel-date2" ).datepicker("option", "minDate", minDate);
                $( "#form_aereo_hotel-date2" ).datepicker("option", "maxDate", maxDate);
            }
        }

        // Função que vai validar se a data digitada pelo usuário compreende o período especificado nas configurações
        $('#form_aereo_hotel-date1').unbind('blur');
		$('#form_aereo_hotel-date1').bind('blur', function(event) {
            var dtCampo = getMinDate($('#form_aereo_hotel-date1').val());
            if (dtCampo != null)
            {
                var minDate = $( '#form_aereo_hotel-date1' ).datepicker( "option", "minDate" );
                var maxDate = $( '#form_aereo_hotel-date1' ).datepicker( "option", "maxDate" );
                if (dtCampo < minDate)
                {
                    alert("Data não permitida, verifique!");
                    $( '#form_aereo_hotel-date1' ).val("");
                    $( '#form_aereo_hotel-date1' ).focus();
                }
                else if (dtCampo > maxDate)
                {
                	alert("Data não permitida, verifique!");
                	$( '#form_aereo_hotel-date1' ).val("");
                    $( '#form_aereo_hotel-date1' ).focus();
                }
                else
                {
                   onIdaSelect($('#form_aereo_hotel-date1').val(), null);
                }
            }
            else
            {
            	$('#form_aereo_hotel-date1').val("");
            }
        });

        // Função que vai validar se a data digitada pelo usuário compreende o período especificado nas configurações
