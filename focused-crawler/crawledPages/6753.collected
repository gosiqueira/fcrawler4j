<script type="text/javascript" charset="utf-8">
    // Função que devolve um objeto data a partir de uma string
    function getMinDate(date)
    {
        try
        {
	        // Configuraçao com o formato da data
	        var dateFormat = 'dd/mm/y';
	        var dateRet = new Date();

	        if (date != null && date != "" && (date.length == 8 || date.length == 10))
	        {
	            var data = date.split("/");
	            if (dateFormat == "dd/mm/y")
                {
	            	dateRet.setFullYear(parseFloat("20"+data[2]), parseFloat(data[1])-1, parseFloat(data[0]));
                	return dateRet;
	            }
	            else if (dateFormat == "dd/mm/yy")
                {
	            	dateRet.setFullYear(parseFloat(data[2]), parseFloat(data[1])-1, parseFloat(data[0]));
                	return dateRet;
	            }
	        }

	        return null;
        }
        catch(err)
        {
        	return null;
        }
    }

    // Função que limpa o campo quando ele é selecionado
    function limpaCampo(input, inst)
    {
        input.value = "";
    }

	// Função que carrega via ajax o script do calendário
	function reloadCalendar(url, data, target, inputIda, inputVolta)
    {
		$.ajax({
			type: 'POST',
			url: url,
			contentType: "application/x-www-form-urlencoded; charset=UTF-8",
	        data: data,
	        cache: false,
			async: false,
			/*dataType: "text/html",*/
			scriptCharset: "UTF-8",
			// Se houve sucesso vamos carregar o resultado para o argumento
			success: function(conteudo){
				// Fecho os calendários que estiverem abertos
				$("#" + inputIda).datepicker( "hide" );
				$("#" + inputVolta).datepicker( "hide" );
				// Removo as regras atuais
				$("#" + inputIda).datepicker( "destroy");
				$("#" + inputVolta).datepicker( "destroy");
				// Limpo o div com os scripts
				$("#" + target).html("");
				// Insiro o novo script
				$("#" + target).html(conteudo);
				// Atualizo o calendário com as novas regras
				$("#" + inputIda).datepicker( "refresh");
				$("#" + inputVolta).datepicker( "refresh");
			}
		});
    }
</script>

    <script type="text/javascript" charset="utf-8">
		
        $(function(){
            // Configuração com a quantidade de dias a partir da data atual para a compra
            var minStay       = parseInt(10);
            // Configuração com a quantidade maxima de dias a partir da data atual para compra
            var maxStay       = parseInt(30);
         	// Configuração com a quantidade de dias a partir da data atual para a compra
            var minBuy        = parseInt(10);
            // Configuração com a quantidade maxima de dias a partir da data atual para compra
            var maxBuy        = parseInt(365);
            // Configuraçao com o formato da data
            var dateFormat    = 'dd/mm/y';
            // Configuração com a quantidade máxima de meses do calendário
            var maxMounths    = parseInt(1);
            // Variaveis que irao armazenar as datas
            var today         = new Date();
            var minDateStay   = new Date();
            var maxDateStay   = new Date();
            var minDateBuy    = new Date();
            var maxDateBuy    = new Date();
            var dateIniParam  = getMinDate('');
            var dateFimParam  = getMinDate('');

            // Calculando a data minima e a data máxima para estadia
            if (dateIniParam != null)
            {
                minDateBuy.setDate(dateIniParam.getDate() + minBuy);
                maxDateBuy.setDate(dateIniParam.getDate() + maxBuy);
            }
            else
            {
                minDateBuy.setDate(today.getDate() + minBuy);
                maxDateBuy.setDate(today.getDate() + maxBuy);
            }

            // Calculando a data minima e a data máxima para estadia
            if (dateFimParam != null)
            {
              minDateStay.setDate(dateIniParam.getDate() + minStay);
              maxDateStay.setDate(dateIniParam.getDate() + maxStay);
            }
            else
            {
            	minDateStay.setDate(minDateBuy.getDate() + minStay);
            	maxDateStay.setDate(minDateBuy.getDate() + maxStay);
            }


			/* Verifico se existe uma imagem para colocar no botão do calendário */
			if( ('' != null && '' != "") || (('' != null && '' != "")) )
            {
				$("#form_aereo_hotel-date1").datepicker({
					minDate: minDateBuy,
					maxDate: maxDateBuy,
					constrainInput: true,
					gotoCurrent: true,
					onSelect: onIdaSelect,
					beforeShow: limpaCampo,
					numberOfMonths: maxMounths,
					dateFormat: dateFormat,
                    highlightWeek : true,
					showOn: "button",
                    buttonText: "",
                    buttonImage: '', 
                    buttonImageOnly: true
				});

				$("#form_aereo_hotel-date2").datepicker({
					minDate: minDateStay,
					maxDate: maxDateStay,
					constrainInput: true,
					gotoCurrent: true,
					numberOfMonths: maxMounths,
					beforeShow: limpaCampo,
					dateFormat: dateFormat,
                    highlightWeek : true,
					showOn: "button",
                    buttonText: "",
                    buttonImage: '', 
                    buttonImageOnly: true
				});
				
				jQuery(".ui-datepicker-trigger").bind("mouseover", function(){
					jQuery(this).attr({src:""});
				});
				jQuery(".ui-datepicker-trigger").bind("mouseout", function(){
					jQuery(this).attr({src:""});
				});
			}
			else
			{
				$("#form_aereo_hotel-date1").datepicker({
					minDate: minDateBuy,
					maxDate: maxDateBuy,
					constrainInput: true,
					gotoCurrent: true,
					onSelect: onIdaSelect,
					beforeShow: limpaCampo,
					numberOfMonths: maxMounths,
					dateFormat: dateFormat,
                    highlightWeek : true
				});

				$("#form_aereo_hotel-date2").datepicker({
					minDate: minDateStay,
					maxDate: maxDateStay,
					constrainInput: true,
					gotoCurrent: true,
					numberOfMonths: maxMounths,
					beforeShow: limpaCampo,
					dateFormat: dateFormat,
                    highlightWeek : true
				});			
			}
			
			
            // Limito o tamanho do campo de acordo com a mascara informada
            if (dateFormat == 'dd/mm/yy')
