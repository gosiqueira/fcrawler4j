// For each tag on this widget, look up the url and replace the elemnt.  One sample neighborhood url is
// represented under id '12345', which is then swapped with the real neighborhood id.
function doTheSwap() {
widget.element.getElements('span.tag').each(function(elmt) {
var tagId = elmt.getAttribute('data-tag-id');
var url;
if (tagId.charAt(0) === 'd') {
url = ta.retrieve('ta.p13n.prodp13nCrossSellUrls')['d12345'].replace('zfn12345', 'zfn' + tagId.substring(1));
}
else {
url = ta.retrieve('ta.p13n.prodp13nCrossSellUrls')[tagId];
}
if (url) {
var newElmt = document.createElement('a');
newElmt.setAttribute('class', 'tag');
newElmt.setAttribute('href', url);
newElmt.setAttribute('target', '_blank');
newElmt.setAttribute('onclick', elmt.getAttribute('data-onclick'));
newElmt.innerHTML = elmt.innerHTML;
elmt.parentNode.replaceChild(newElmt, elmt);
}
});
}
ta.queueForLoad(function() {
// If there are tags to swap...
if (widget.element.getElements('span.tag').length > 0 ) {
