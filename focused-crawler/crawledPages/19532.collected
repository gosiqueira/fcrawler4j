// fail to update all prices and recommendations
if (!updateResult){
parentElm.show();
parentElm.setStyle('height', 0);
parentElm.setStyle('opacity', '0');
// If there was a response, use it and save off the predates html.
// If no response, swap in the predates html.
if (responseText.indexOf('recTbl') != -1) {
if (predatesHtml == null) {
predatesHtml = parentElm.innerHTML;
}
parentElm.innerHTML = responseText;
}
else {
parentElm.innerHTML = predatesHtml;
predatesHtml = null;
}
var innerElmt = parentElm.getElementById("HSCS_ROW");
if(innerElmt) {
var opacityTween = new Fx.Tween(parentElm, {
property: 'opacity'
});
var targetHeight = innerElmt.getSize().y;
