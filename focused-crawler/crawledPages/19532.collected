function updateEntirePlacement( responseText ) {
// update the tags, as initial page load will not have them
updateTags(responseText);
// update the price buttons
var updateResult = updateIndividualFirst(responseText);
var parentElm = ta.id(placement.id);
var dataToShow = (responseText.indexOf('recTbl') != -1 || predatesHtml);
//Else put in the original placement near the map.
// If there is no recTbl (presumably due to availability), leave what was there.
