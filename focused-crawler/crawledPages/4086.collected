var define = ta.prwidgets.define.bind(ta.prwidgets,'prodp13n_recommendation_carousels_related_footer_xsell','handlers.js');
define(["widget"], function(widget) {
var carousel = widget.element.getElement('.carousel');
var carouselInner = carousel.getElement('.carouselInner');
var cells = carouselInner.getChildren();
var numPages = parseInt(carousel.getAttribute('data-num-pages'), 10);
var pageSize = parseInt(carousel.getAttribute('data-page-size'), 10);
var id = carousel.getAttribute('data-id');
var arrows = widget.element.getElements('.arrow');
var leftArrow = widget.element.getElement('.arrow.left');
var rightArrow = widget.element.getElement('.arrow.right');
var isRtl = !!document.getElement('.rtl');
function _setPage(page, element) {
carousel.setAttribute('data-page-number', page);
if (page == 1) {
leftArrow.addClass('disabled');
}
else {
leftArrow.removeClass('disabled');
}
if (page == numPages) {
rightArrow.addClass('disabled');
}
else {
rightArrow.removeClass('disabled');
}
var cell = cells[(page - 1) * pageSize];
var cellLeft = cell.getPosition(carouselInner).x;
var transform;
if (!element) { // not in response to a click
arrows.addClass('instant');
transform = 'setStyle';
} else {
arrows.removeClass('instant');
carousel.addClass('scrolling');
carouselInner.set('tween', {onComplete: function() {
carousel.removeClass('scrolling');
}});
transform = 'tween';
ta.fireEvent('prodp13n.carousel.setPage', id, page);
ta.trk('SetCarouselPage', false, element, ["N:" + page]);
}
if (isRtl) {
carouselInner[transform]('right', '-' + (carouselInner.getSize().x - (cellLeft + cell.getSize().x)) + 'px');
}
else {
carouselInner[transform]('left', '-' + cellLeft + 'px');
}
}
return {
left: function(element) {
var page = parseInt(carousel.getAttribute('data-page-number'), 10);
if (page > 1) {
_setPage(page - 1, element);
}
},
right: function(element) {
var page = parseInt(carousel.getAttribute('data-page-number'), 10);
if (page < numPages) {
_setPage(page + 1, element);
}
},
setPage: function(page) {
if (page < 1) {
page = 1;
}
if (page > numPages) {
page = numPages;
}
if (page != parseInt(carousel.getAttribute('data-page-number'), 10)) {
_setPage(page, true);
}
}
};
});})();
(function(){
var define = ta.p13n.placements.define.bind(ta.p13n.placements,'prodp13n_hr_similar_xsell','handlers.js');
//Private javascript for prodp13n_hr_similar_xsell
/*jshint -W004 */
/*jshint -W018 */
define(["placement"], function(placement) {
var lastreqts = null;
var predatesHtml = null;
// after a new request, if the result contains the same hotel as the last request, replace the tags with new tags only
function updateTags(responseText){
var oldRecsContainer = {},  // id -> tags, body
newRecsContainer = {},
parentElm = ta.id(placement.id),
i;
// get rec ids on the page
var oldRecs = parentElm.getElements('.rec');
var recId, oldTagsElmt, rec, newTagsElmt;
for (i = 0; i < oldRecs.length; i++){
recId = oldRecs[i].getAttribute("data-id");
oldTagsElmt = null;
oldTagsElmt = oldRecs[i].getElement('.tags');
rec = {
tagsElmt: oldTagsElmt,
body:oldRecs[i]
};
oldRecsContainer[recId] = rec;
}
// has available tags
if( parentElm && responseText.indexOf('recTbl') != -1 ) {
var newResult = document.createElement('div');
newResult = ta.id(newResult);
newResult.innerHTML = responseText;
var newRecs = newResult.getElements('.rec');
for (i = 0; i < newRecs.length; i++){
recId = newRecs[i].getAttribute("data-id");
newTagsElmt = null;
newTagsElmt = newRecs[i].getElement('.tags');
rec = {
tagsElmt: newTagsElmt,
body: newRecs[i]
};
newRecsContainer[recId] = rec;
}
// for each old hotel, search if the same hotel exists in the new hotel list
// if so, replace the tags
for (recId in oldRecsContainer){
// has new tags
if (recId in newRecsContainer ){
rec = oldRecsContainer[recId];
if (rec.tagsElmt) {
rec.tagsElmt.innerHTML = newRecsContainer[recId].tagsElmt.innerHTML;
}
else {
var picStyle = rec.body.getElement('.picStyle');
if (picStyle) {
picStyle.appendChild(newRecsContainer[recId].tagsElmt);
}
}
}
}
}
}
// after a new request, if the result contains the same hotel as the last request, replace the price buttons with new prices only
// if some hotels in the last request does not have prices for the new request, replace those hotels with new hotels which have prices
// if the number of new hotels does not match the number of olde hotels, refresh the entire placement
function updateIndividualFirst(responseText){
var oldRecsContainer = {},  // id -> button, replaced, body
newRecsContainer = {},
parentElm = ta.id(placement.id),
i;
if (predatesHtml == null) {
predatesHtml = parentElm.innerHTML;
}
// get rec ids on the page
var oldRecs = parentElm.getElements('.rec');
var recId, oldPriceBtn, rec, newPriceBtn;
for (i = 0; i < oldRecs.length; i++){
recId = oldRecs[i].getAttribute("data-id");
oldPriceBtn = null;
oldPriceBtn = parentElm.getElement('.crBtn_'+recId);
rec = {
priceBtn: oldPriceBtn,
replaced: false,
body:oldRecs[i]
};
oldRecsContainer[recId] = rec;
}
// has available prices
if( parentElm && responseText.indexOf('recTbl') != -1 ) {
var newResult = document.createElement('div');
newResult = ta.id(newResult);
newResult.innerHTML = responseText;
var newRecs = newResult.getElements('.rec');
// if new results have more hotels than the old one, refresh the entire page
if (newRecs.length != oldRecs.length){
return false;
}
for (i = 0; i < newRecs.length; i++){
recId = newRecs[i].getAttribute("data-id");
newPriceBtn = null;
newPriceBtn = newResult.getElement('.crBtn_'+recId);
rec = {
priceBtn: newPriceBtn,
replaced: false,
body: newRecs[i]
};
newRecsContainer[recId] = rec;
}
var leftOver = [];
// for each old hotel, search if the same hotel exists in the new hotel list
// if so, replace the price button
for (recId in oldRecsContainer){
// has a new price
if (recId in newRecsContainer ){
rec = oldRecsContainer[recId];
showDivWithAnimation(rec.priceBtn, newRecsContainer[recId].priceBtn);
rec.replaced = true;
newRecsContainer[recId].replaced = true;
} else {
// does not have a new price
leftOver.push(recId);
}
}
//dealing with those who do not have a new price
for ( i = 0; i < leftOver.length; i++ ){
// if the old rec does not have a price, replace it with a new hotel
rec = oldRecsContainer[leftOver[i]];
for (recId in newRecsContainer){
if (newRecsContainer[recId].replaced === false){
showDivWithAnimation(rec.body,newRecsContainer[recId].body);
showDivWithAnimation(rec.priceBtn, newRecsContainer[recId].priceBtn);
newRecsContainer[recId].replaced = true;
rec.replaced = true;
break;
}
}
// do not have enough replacements
if (!rec.replaced){
return false;
}
}
return true;
}else{
//no rec has availability at all
return false;
}
}
/**
* update the placement
*/
function updateEntirePlacement( responseText ) {
// update the tags, as initial page load will not have them
updateTags(responseText);
// update the price buttons
var updateResult = updateIndividualFirst(responseText);
var parentElm = ta.id(placement.id);
var dataToShow = (responseText.indexOf('recTbl') != -1 || predatesHtml);
//Else put in the original placement near the map.
// If there is no recTbl (presumably due to availability), leave what was there.
if (parentElm && dataToShow) {
// fail to update all prices and recommendations
if (!updateResult){
parentElm.show();
parentElm.setStyle('height', 0);
parentElm.setStyle('opacity', '0');
// If there was a response, use it and save off the predates html.
// If no response, swap in the predates html.
if (responseText.indexOf('recTbl') != -1) {
if (predatesHtml == null) {
predatesHtml = parentElm.innerHTML;
}
parentElm.innerHTML = responseText;
}
else {
parentElm.innerHTML = predatesHtml;
predatesHtml = null;
}
var innerElmt = parentElm.getElementById("HSCS_ROW");
if(innerElmt) {
var opacityTween = new Fx.Tween(parentElm, {
property: 'opacity'
});
var targetHeight = innerElmt.getSize().y;
var heightTween = new Fx.Tween(parentElm, {
property: 'height',
onComplete: function() {
parentElm.setStyle('height', 'auto');
opacityTween.start(0, 1);
}
}
);
heightTween.start(0, targetHeight);
}
}
}
}
/**
* swap the content of the target elemt
*/
function showDivWithAnimation(elmt, replaceElmt){
elmt.show();
elmt.setStyle('opacity', 0);
elmt.innerHTML = replaceElmt.innerHTML;
// update price button class
if (elmt.hasClass('crBtn')){
var oldId = elmt.getAttribute("data-id");
var newId = replaceElmt.getAttribute("data-id");
if (oldId && newId){
elmt.removeClass('crBtn_'+oldId);
elmt.addClass('crBtn_'+newId);
}
}
elmt.setAttribute("data-id", replaceElmt.getAttribute("data-id"));
var opacityTween = new Fx.Tween(elmt, {property:'opacity'});
opacityTween.start(0, 1);
}
// This function is invoked to load new hotels in the
// also-viewed module on the top right of /H_R.
function load(requestNumber, forceAlsoIfDatesAreNotSet) {
try {
if(ta.retrieve('ta.redirectingPage')) {
return;
}
if (!requestNumber || !(requestNumber>=1) ) {
requestNumber = 1;
}
// only use the results of the last request sent
var myreqts = new Date().getTime();
lastreqts = myreqts; //will check for a match when the response comes in
var POLL_WAIT_TIME = 1000;
// get travel dates, etc
var travelData = false;
var reqData = false;
// for normal requests (forceAlsoIfDatesAreNotSet=false)
// we want to proceed only if dates are selected
// for other request (forceAlsoIfDatesAreNotSet=true)
// we want to proceed either with or without dates
travelData = ta.widgets.calendar.getCheckRatesData();
if(!travelData) {
if (forceAlsoIfDatesAreNotSet){
travelData = { "isDated": false };
} else {
return;
}
}
var metaParams = {};
if (ta.meta) {
metaParams = ta.commerce.meta.getMetaParams();
}
// We explicitly request to disable lazy loading because
// images are not loaded and the user is directly looking
// at this area, so it does not make sense to postpone
// loading the images
reqData = Object.merge(travelData, {
"detail": ""+placement.location_id,
"reqNum": (requestNumber > 1) ? "2" : "1",
"placementName": placement.name,
"servletClass": placement.servletClass,
"disableLazyLoading" : true
}, metaParams);
var queryString = Object.toQueryString(reqData);
if (!queryString || !queryString.trim()) {
ta.util.error.record(null, placement.name + " load - toQueryString failure", null, reqData);
return;
}
new Request({
"url": "/MetaPlacementAjax",
"data": reqData,
"evalScripts": true,
"onSuccess": function(responseText) {
if(lastreqts != myreqts) {
return;
}
//  SOL or Ready: display
//  NotReady: if it's the first try, wait a second and try again; if it's the second try, display
var ready = (requestNumber >=4) || responseText.contains('xsellHacStatusReady') || responseText.contains('xsellHacStatusSOL') || travelData.isDated === false;
if (!ready) {
setTimeout(function () { load(requestNumber + 1, forceAlsoIfDatesAreNotSet); }, POLL_WAIT_TIME);
} else {
updateEntirePlacement(responseText);
}
},
"onFailure": function() {
if(ta.retrieve('ta.redirectingPage')) {
return;
}
ta.util.error.record(null, placement.name + ' load - MetaPlacementAjax request failed');
}
}).send();
} catch(e) {
ta.util.error.record(e, placement.name + ' load');
}
}
