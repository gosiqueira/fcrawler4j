$(".nibbler-footer-popup-container").on('click', 'a', function(event){
var el = event.delegateTarget;
if (el.className.indexOf('nibbler--show--popup') >= 0) {
el.className = el.className.replace('nibbler--show--popup', '');
} else {
el.className += ' nibbler--show--popup';
var randomId = 'nibbler_' + ( ("" + (Math.random())).substr(2) );
$(document).on("click." + randomId, function (ev) {
if (!$.contains(el, ev.target)) {
el.className = el.className.replace('nibbler--show--popup', '');
}
if (el.className.indexOf('nibbler--show--popup') < 0) {
$(document).off("click." + randomId);
}
});
}
});
});
});
