<script id="popup-overlay-template" type="text/x-handlebars-template">
<div id="{{id}}-overlay" class="flights-popup-overlay {{id}}-overlay"></div>
</script>
<script id="popup-template" type="text/x-handlebars-template">
<div id="{{id}}" class="flights-popup {{id}} {{classes}} flights-popup-{{popupPosition}}"
{{#if priority}}data-priority="{{priority}}"{{/if}}
>
<div class="popup-border"></div>
<div class="popup-container">
{{#if title}}
<div class="popup-header"><h4>{{title}}</h4></div>
{{/if}}
<div class="popup-wrapper">
{{#if messageTitle}}
<div id="" class="core-message eva-message-{{#if messageType}}{{messageType}}{{/if}} " >
<div class="message-icon">
<span class="eva-icon-{{#if messageIcon}}{{messageIcon}}{{/if}}"></span>
</div>
<div class="message-close">
<i class="eva-icon-close"></i><!--[if IE 8]><span>&#215;</span><![endif]-->
</div>
<div class="message-text">
<h5 class="flights-message-title">{{messageTitle}}</h5>
<p>{{#if messageTitle}}{{messageDescription}}{{/if}}</p>
</div>
</div>
{{/if}}
{{#if showContent}}
<div class="popup-content"></div>
{{/if}}
</div>
</div>
{{#unless hideCloseIcon}}
<span class="popup-close-button popup-close">&times;</span>
{{/unless}}
{{#unless noPuntita}}
<span class="popup-arrow-border popup-arrow-{{indicatorPosition}}" style="display:none"> </span>
