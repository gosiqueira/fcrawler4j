    function pintarUltimasBusquedasCabecera(busquedas) {
        
        var count = Object.keys(busquedas).length;
        var textoHtml="";
        var textoTB ="";
             
        if (count > 0) {
         
            for (var i = 0; i < count; i++) {
                
                if(i==count-1)
                {
                    textoHtml=   "<div id='busqueda" + i + "' class='busqueda clearfix'><a id='busquedaLink"+i+"' href=\"" + busquedas[i]['Link'] + "\">";
                }else{
                    textoHtml=   "<div id='busqueda" + i + "' class='busqueda Bdashedgris clearfix'><a id='busquedaLink"+i+"' href=\"" + busquedas[i]['Link'] + "\">";
                }

                if(busquedas[i]['Image']!=null){
                    textoHtml += "<div class='imgBusqueda'><span class='"+busquedas[i]['Image']+"'></span></div>";
                    textoTB = "<div id='busquedaOculta" + i + "'><div  class='busqueda clearfix'><div class='imgBusqueda'><span class='"+busquedas[i]['Image']+"'></span></div>";
                }

                if(busquedas[i]['icoEspera']!=null){
                    textoTB = "<div id='busquedaOculta" + i + "'><div  class='busqueda clearfix'><div class='icoEsperaBusqueda'><img src='"+busquedas[i]['icoEspera']+"' /></div>";
                }
                
                textoHtml += "<div class='txtBusquedas'><span class='txt12azulbold'>" + busquedas[i]['Head'] +"</span>";
                textoTB += "<div class='txtBusquedas'><span class='txt12azulbold'>" + "Buscando" + " " + busquedas[i]['Head'] +"</span>";
                
                if(busquedas[i]['Line1']!=null){
                    textoHtml += "<span class='txt11negro'>"+ busquedas[i]['Line1'] +"</span>";
                    textoTB += "<span class='txt11negro'>"+ busquedas[i]['Line1'] +"</span>";
                    if(busquedas[i]['Line2']!=null){
                        textoHtml += "<span class='txt11negro'>"+ busquedas[i]['Line2'] +"</span>";
                        textoTB += "<span class='txt11negro'>"+ busquedas[i]['Line2'] +"</span>";
                    }
                }
                textoHtml += "</div><div class='clear'></div></a></div>";
                textoTB += "</div><div class='clear'></div></div></div>";
                
                $("#listaBusquedasOculto").append(textoTB);
                $("#listaBusquedas").append(textoHtml);

                $("#busqueda" + i).unbind();
                $("#busqueda" + i).bind("click",{id: i},function (event) {
                    $("#cFlotanteBusquedas").unbind();
                    $("#cFlotanteBusquedas").hide();
                    hacerBindBusquedas();
                    tb_show("", "#TB_inline?height=120&amp;width=440&amp;inlineId=busquedaOculta"+event.data.id+"&amp;modal=true");
                });

            }

            $("#listaBusquedas a").click(function(){
              
                _gaq.push(['_trackEvent', 'AccountHeader', "BusquedasClick", $(this).attr("href")]);
              
            });

      } else {
          var textoHtml = "<div class='busqueda'><span class='txt11negro'>Você não realizou nenhuma busca</span></div>";
          $("#listaBusquedas").append(textoHtml);
      }
        
  }


