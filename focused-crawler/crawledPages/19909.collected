// This function is invoked to load new hotels in the
// also-viewed module on the top right of /H_R.
function load(requestNumber, forceAlsoIfDatesAreNotSet) {
try {
if(ta.retrieve('ta.redirectingPage')) {
return;
}
if (!requestNumber || !(requestNumber>=1) ) {
requestNumber = 1;
}
// only use the results of the last request sent
var myreqts = new Date().getTime();
lastreqts = myreqts; //will check for a match when the response comes in
var POLL_WAIT_TIME = 1000;
// get travel dates, etc
var travelData = false;
var reqData = false;
// for normal requests (forceAlsoIfDatesAreNotSet=false)
// we want to proceed only if dates are selected
// for other request (forceAlsoIfDatesAreNotSet=true)
// we want to proceed either with or without dates
travelData = ta.widgets.calendar.getCheckRatesData();
if(!travelData) {
if (forceAlsoIfDatesAreNotSet){
travelData = { "isDated": false };
} else {
return;
}
}
var metaParams = {};
if (ta.meta) {
metaParams = ta.commerce.meta.getMetaParams();
}
// We explicitly request to disable lazy loading because
// images are not loaded and the user is directly looking
// at this area, so it does not make sense to postpone
// loading the images
reqData = Object.merge(travelData, {
"detail": ""+placement.location_id,
"reqNum": (requestNumber > 1) ? "2" : "1",
"placementName": placement.name,
"servletClass": placement.servletClass,
"disableLazyLoading" : true
}, metaParams);
var queryString = Object.toQueryString(reqData);
if (!queryString || !queryString.trim()) {
ta.util.error.record(null, placement.name + " load - toQueryString failure", null, reqData);
return;
}
new Request({
"url": "/MetaPlacementAjax",
"data": reqData,
"evalScripts": true,
"onSuccess": function(responseText) {
if(lastreqts != myreqts) {
return;
}
//  SOL or Ready: display
//  NotReady: if it's the first try, wait a second and try again; if it's the second try, display
var ready = (requestNumber >=4) || responseText.contains('xsellHacStatusReady') || responseText.contains('xsellHacStatusSOL') || travelData.isDated === false;
if (!ready) {
setTimeout(function () { load(requestNumber + 1, forceAlsoIfDatesAreNotSet); }, POLL_WAIT_TIME);
} else {
updateEntirePlacement(responseText);
}
},
"onFailure": function() {
if(ta.retrieve('ta.redirectingPage')) {
return;
}
ta.util.error.record(null, placement.name + ' load - MetaPlacementAjax request failed');
}
}).send();
} catch(e) {
ta.util.error.record(e, placement.name + ' load');
}
}
