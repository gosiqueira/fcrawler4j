sOrig = $(i+'Code').value;
}
else if ( i == 'aw_dest' ) {
sDest = $(i+'Code').value;
}
});
if(sOrig==sDest) {
alert("Please make sure that your origination and destination airports are different.");
return false;
}
}
$(name + "Fields").setStyle("display", "none");
$(name + "Edit").setStyle("display", "inline");
$(name + "Save").setStyle("display", "none");
vals.each(function(i) {
if ($(i + "Complete").value == "" || $(i + "Complete").value == $(i).value) {
if (name == 'awJoin') {
$(i + "Value").innerHTML = $(i + "Code").value;
} else {
$(i + "Value").innerHTML = $(i).value.replace(/ \- .*$/, '');
}
}
else {
$(i).value = $(i + "Complete").value;
}
});
return false;
}
function tamgEdit(name)
{
