<script type="text/javascript">
    if(window.innerWidth < 361){
        $('.item', '#sliderImg' ).each(function(i){
            var widthImg = this.getAttribute("data-src" ).replace("743x430","360x270");
            this.setAttribute('data-src', widthImg);
            if (i == 0) {
                $(this ).find("img").attr('src', widthImg);
            }
        });
    }


    var monthsArr = eval('[&quot;Janeiro&quot;, &quot;Fevereiro&quot;, &quot;Março&quot;, &quot;Abril&quot;, &quot;Maio&quot;, &quot;Junho&quot;, &quot;Julho&quot;, &quot;Agosto&quot;, &quot;Setembro&quot;, &quot;Outubro&quot;, &quot;Novembro&quot;, &quot;Dezembro&quot;]'.replace(/&quot;/g, '"'));

    if ($("#calendarSlider .calendar-slide").children().length > 1) {
        function showMonthsYear(thisStep) {
            var calLeft = $(thisStep).find("table").eq(0)[0],
                mLeft = parseInt(calLeft.getAttribute("data-month")) -1,
                yLeft = calLeft.getAttribute("data-year"),
                calRight = $(thisStep).find("table").eq(1)[0],
                mRight = parseInt(calRight.getAttribute("data-month")) -1,
                yRight = calRight.getAttribute("data-year");

            $("#txtMonthYearLeft").html(monthsArr[mLeft] + " " + yLeft);
            $("#txtMonthYearRight").html(monthsArr[mRight] + " " + yRight);
        }

        var calendarSlider = $("#calendarSlider").bxSlider({
            prevSelector: "#prevMonth",
            nextSelector: "#nextMonth",
            prevText: '&lsaquo;',
            nextText: '&rsaquo;',
            infiniteLoop: false,
            hideControlOnEnd: true,
            pager: false,
            adaptiveHeight: true,
            onSliderLoad: function(i){
                var thisStep = $("#calendarSlider").find(".calendar-slide").eq(i);
                showMonthsYear(thisStep);
            },
            onSlideBefore: function(e){
                showMonthsYear(e);
            }
        });

        $(".cal table").each(function(i) {
            var statusLeft = "",
                tableIndex = i,
                firstDayTd = true;

            $(this).find("td").each(function() {
                var td = this;

                if (td.getAttribute("data-status")) {
                    var tdStatus = td.getAttribute("data-status");

                    if (statusLeft != tdStatus) {
                        td.className = (statusLeft) ? statusLeft+"-left " + tdStatus : tdStatus;
                        statusLeft = tdStatus;
                    } else {
                        td.className = tdStatus;
                    }

                    if (firstDayTd && tableIndex) {
                        firstDayTd = false;

                        var prevTable = $(".cal table").eq($(td).closest("table").index("table") - 1),
                            firstClassLeft = prevTable.find("[data-status]:last").attr("data-status");

                        td.className = (firstClassLeft) ? firstClassLeft+"-left " + tdStatus : tdStatus;
                    }
                }
            });
        });

        
    } else {
        $(".box.availability").remove();
    }

    define("obj/texts", [], function() {
        return i18nTexts = {
            "days": "Domingo, Segunda, Terça, Quarta, Quinta, Sexta, Sábado",
            "daysShort": "Dom, Seg, Ter, Qua, Qui, Sex, Sab",
            "daysMin": "Do, Se, Te, Qu, Qu, Se, Sa",
            "months": "Janeiro, Fevereiro, Março, Abril, Maio, Junho, Julho, Agosto, Setembro, Outubro, Novembro, Dezembro",
            "monthsShorts": "Jan, Fev, Mar, Abr, Mai, Jun, Jul, Ago, Set, Out, Nov, Dez",
            "seeMore": "Ver mais",
            "seeLess": "Ver menos",
            "seeMoreAm": "Ver mais serviços",
            "seeLessAm": "Ver menos serviços"
        };
    });

    var reviews = 0;
    

    define("obj/rentalData", [], function() {
        return rentalData = {
            "latitude": -33.4646853,
            "longitude": -70.64694120000001,
            "markerIcon": 'http://br.staticontent.com/vacation-rentals/static/detail/2.2.8/images/map-marker-icons/apartment.png',
            "guestsQty": "1",
            "idRental": "565dfd6fe4b0fef0ad5deebf",
            "comments": reviews
        };
    });

    require(["detail"]);
</script>

