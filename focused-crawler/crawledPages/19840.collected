define(["widget"], function(widget) {
function getTrackingString ( element ) {
if (element && element.hasClass) {
if (element.hasClass('standard_pagination')) {
return 'STANDARD_PAGINATION';
} else if (element.hasClass('leaf_geo_pagination')) {
return 'LEAF_GEO_PAGINATION';
}
}
return "UNKNOWN_PAGINATION";
}
function getNumPages ( element ) {
var link = element && element.getElement('.pageNum.last');
return link && parseInt(link.getAttribute('data-page-number'), 10);
}
var paginationDiv = widget && widget.element && widget.element.getElement('.pagination');
var paginationClass = paginationDiv && ('.' + paginationDiv.get('class').replace(/\s/g, '.'));
var numPages = getNumPages(paginationDiv);
var trackingString = getTrackingString(paginationDiv);
var key = trackingString + '_loaded';
var firstInstance = !ta.has(key);
if (firstInstance && numPages) {
ta.store(key, true);
ta.queueForLoad(function(){
ta.util.element.trackWhenScrolledIntoView(paginationClass, [trackingString+'_VISIBLE', 'numPages', numPages, 0]);
}, "init /Hotels pagination tracking for widget: " + trackingString);
}
return {};
});})();
