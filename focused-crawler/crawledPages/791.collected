function generateContent(data) {

	var length = $.map(data, function(n, i) { return i; }).length;

	contentDiv.find('.ajax-content').remove();
	var title;
	if(length == 0){
		title = $('<header class="entry-header"><h1 class="entry-title"><span>Nenhuma Loja Encontrada</span></h1></header>');
	} else {
		title = $('<header class="entry-header"><h1 class="entry-title"><span>Lojas PrÃ³ximas</span></h1></header>');
	}

	var html;
	if(length > 0){
		html = $('<ul class="items"></ul>');
	}
	var limit = 30;
	if(limit > length) {
		limit = length;
	}
	var i = 0;
	for(var key in data){
		var thumbnailHtml;
		if(data[key].timthumbUrlContent){
			var thumbnailHtml = '<div class="thumbnail"><img src="'+data[key].timthumbUrlContent+'" alt="Item thumbnail" /><div class="comment-count">'+data[key].comment_count+'</div></div>';
		} else {
			thumbnailHtml = '';
		}
		var rating = '';
		if(data[key].rating){
			rating += '<span class="rating">';
			for (var i = 1; i <= data[key].rating['max']; i++) {
				rating += '<span class="star';
				if(i <= data[key].rating['val']) {
					rating += ' active';
				}
				rating += '"></span>';
			}
			rating += '</span>';
		}
		var descriptionHtml = '<div class="description"><h3><a href="'+data[key].link+'">'+data[key].post_title+'</a>'+rating+'</h3><a id="element-mobile" href="tel:'+data[key].optionsDir["telephone"]+'"><div id="btn-tel-amarelo-search"></div></a><div id="element-mobile" style="clear:both"></div><div>'+data[key].excerptDir+'</div><a href="'+data[key].link+'"><div id="btn-loja-search"></div></a><div class="divider-dash" style="clear:both"></div></div>';
		html.append('<li class="item clear">'+thumbnailHtml+descriptionHtml+'</li>');
		if(i <= limit){
			i++;
		} else {
			break;
		}
	};
	ajaxContent = $('<div class="ajax-content"></div>').html(title).append(html);
	contentDiv.find('>').hide();
	contentDiv.append(ajaxContent);
}

