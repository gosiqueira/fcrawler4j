<script type="text/javascript">
    (function()
    {
        $('.ofert a, .ofert-info-container a').css("pointer-events", "none");

        // Link de ofertas
        $('.ofert, .ofert-info-container').click(function(e){
            var link = $(this).find("a");
            if (e.which==1)
                window.location.href=link.attr("href");
            else if (e.which==2){
                window.open(link.attr("href"));
            }
        });


        // Para todos los comments...
        $('.ux-common-comment .ux-common-comment-type').each(function() {
            // Guardo comentario completo
            $comentarioCompleto = $.trim($(this).text());
            // Defino maximo de caracteres
            $maxLenght = 80;
            // Si el comentario supera la cantidad de caracteres...
            if ($comentarioCompleto.length > $maxLenght) {
                // Guardo el comentario corto y lo muestro junto con el elipsis y el link de ver mas.
                $comentarioCorto = $comentarioCompleto.substring(0, $maxLenght);
                $(this).text($comentarioCorto + "... ");
                $(this).append("<a href='#' data-tooltip='" + $comentarioCompleto + "'>Ver m&aacute;s &raquo;</a>");
            };
        });

        // Click en "ver mas" del comment para mostrar comentario completo
        $('.ux-common-comment .ux-common-comment-type a').click(function(e) {
            e.preventDefault();
            //Guarda una copia de referencia
            var $this = $(this);
            //Crea un tooltip
            var overlay = new Despegar.UX.Common.Modules.Overlay();
            overlay.open({
                //Especifica en donde abrir el tooltip
                trigger: $this.parents(".ux-common-comment-type"),
                //Obtiene la información a mostrar en el tooltip
                body: $this.data("tooltip"),
                //Clase para setear el ancho correspondiente
                className: "ux-common-comment-tooltip",
                //Posición
                position: "top-center"
            });
        });

    }());
</script>

<script type="text/javascript">
    $( ".comment").click(function() {
        if( !$(this).hasClass("item-opened")){
            $( ".comment" ).removeClass("item-opened");
            $( this ).toggleClass( "item-opened");
        }else{
            $( ".comment" ).removeClass("item-opened");
        }
    });

    // Reemplazamos las imagenes rotas de aerolineas
    $(".seo-words li a img").each(function(index) {
        $(this).error(function() {
            $(this).attr('src', 'http://media.despegar.com/davinci/logo/airline/50x50/default.png');
        });
        $(this).attr("src", $(this).attr("src"));
    });
</script>


