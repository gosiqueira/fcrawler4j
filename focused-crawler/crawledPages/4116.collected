        function fncLogonUnlogonButton() {
            var bt = document.getElementById(imgBtnLogUnlog);
            var str = bt.getAttribute('src');
            var entrar = fncGetAllAfterSimbol('/', str);

            if (str != '' && entrar == 'bt_entrar_top.png') {
                //Login...
                var oPopUp = new PopUp();
                oPopUp.openPopupWindow('newLogin.aspx?redirectUrlOpener=reload', 'Milevo', 700, 344);
                //return false;
            }

            return false;
        }

        function fncGetAllAfterSimbol(simbol, target) {
            var textGet = '';
            var ret = '';

            for (i = target.toString().length; i > 0; i--) {
                if (target.toString().charAt(i) == simbol)
                    break;

                textGet += target.toString().charAt(i);
            }

            for (j = textGet.length; j >= 0; j--)
                ret += textGet.toString().charAt(j);

            return ret;
        }

        function CallGA(value) {

            var ga = document.getElementById('idGAux');
            if (ga != null) {
                ga.src = "/ga/" + value;
            }
        }
