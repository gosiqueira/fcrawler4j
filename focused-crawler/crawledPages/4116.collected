<script type="text/javascript">
    $(document).ready(function() {

        try {
            $("ul.thumb li.itm-thumb7")[0].style.overflow = 'hidden';
        } catch (e) { }

        // hover no box7
        $("ul.thumb li.itm-thumb7").hover(function() {
            try {
                $("ul.thumb li.itm-thumb7")[0].style.overflow = 'visible';
            } catch (e) { }
        });

        $("ul.thumb li.itm-thumb7").mouseleave(function() {
            try {
                $("ul.thumb li.itm-thumb7")[0].style.overflow = 'hidden';
            } catch (e) { }
        });
        
        //Larger thumbnail preview

        $("ul.thumb li").hover(function() {
            $(this).find('.textoImagem').show();
            $(this).find('.moldura').css('background-color', '#ffffff');
            $(this).find('.moldura').css('border', 'solid 3px #ec862d');
            $(this).find('#moldura0').css('width', '195px');
            $(this).find('#moldura1').css('width', '195px');
            $(this).find('#moldura2').css('width', '195px');
            $(this).find('#moldura3').css('width', '195px');
            $(this).find('#moldura4').css('width', '195px');
            $(this).find('#moldura5').css('width', '195px');
            $(this).find('#moldura6').css('width', '195px');
            $(this).find('#moldura7').css('width', '195px');
            $(this).find('#moldura8').css('width', '195px');
            $(this).find('#moldura9').css('width', '195px');

            // alterna o tamanho da imagem
            //            debugger;
            var width = $(this).find('.defaultImage')[0].width;
            var height = $(this).find('.defaultImage')[0].height;
            //$(this).find('.defaultImage')[0].src = $(this).find('.defaultImage')[0].src.replace('?img=', '?big=');
            $(this).find('.defaultImage')[0].src = $(this).find('.defaultImage')[0].src.replace('vitral/', 'vitral/Big/');
            $(this).find('.defaultImage')[0].width = $(this).find('.bigImageWidth')[0].value;
            $(this).find('.defaultImage')[0].height = $(this).find('.bigImageHeight')[0].value;
            $(this).find('.bigImageWidth')[0].value = width;
            $(this).find('.bigImageHeight')[0].value = height;


            //$(this).find('.defaultImage').hide();
            //$(this).find('.bigImage').show();

            $(this).css({ 'z-index': '999' });
            $('.moldura').css('z-index', '999');
        }, function() {
            $(this).find('.textoImagem').hide();
            $(this).find('.moldura').css('background-color', 'transparent');
            $(this).find('.moldura').css('width', '0px');
            $(this).find('.moldura').css('border', '0px');

            // alterna o tamanho da imagem
            //            debugger;
            var width = $(this).find('.defaultImage')[0].width;
            var height = $(this).find('.defaultImage')[0].height;
            $(this).find('.defaultImage')[0].src = $(this).find('.defaultImage')[0].src.replace('vitral/Big/', 'vitral/');
            $(this).find('.defaultImage')[0].width = $(this).find('.bigImageWidth')[0].value;
            $(this).find('.defaultImage')[0].height = $(this).find('.bigImageHeight')[0].value;
            $(this).find('.bigImageWidth')[0].value = width;
            $(this).find('.bigImageHeight')[0].value = height;

            //$(this).find('.bigImage').hide();
            //$(this).find('.defaultImage').show();

            $(this).css({ 'z-index': '0' });
        });

        //Swap Image on Click
        $("ul.thumb li a").click(function() {

            _gaq.push(['_trackEvent', 'Foto do Vitral Clicada', 'Funcionalidade Acessada.']);

            var mainImage = $(this).attr("href"); //Find Image Name
            $("#main_view img").attr({ src: mainImage });
            var host = window.location.protocol + "//" + window.location.hostname;
            var port = window.location.port;
            if (port != null) {
                host = host + ":" + port;
            }
            var endereco = host + mainImage;
            window.location = endereco;
            return false;
        });
        $(".textoImagem").hide();
    });
    
</script>


