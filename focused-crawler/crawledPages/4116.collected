    <script language="javascript" type="text/javascript">

        function SetCalendar()
        {

            var dtIda = new Date();
            var dtVolta = new Date();

            dtIda.setDate(dtIda.getDate() + 7);
            dtVolta.setDate(dtVolta.getDate() + 10);


            $('#imgIda').datepicker({
                buttonImage: '/img/icon_cal.gif',
                buttonImageOnly: true,
                changeYear: false,
                numberOfMonths: 1,
                minDate: new Date(),
                dateFormat: 'dd/mm/yy',
                defaultDate: '0y'
            }).click(function () { $(this).datepicker('show'); });

            

            $('#imgVolta').datepicker({
                buttonImage: '/img/icon_cal.gif',
                buttonImageOnly: true,
                changeYear: false,
                numberOfMonths: 1,
                minDate: new Date()
            }).click(function() { $(this).datepicker('show'); });

            $('#txtIda').val(dtIda);
            $('#txtVolta').val(dtVolta);

            //$("#rdoIdaVolta").checked = true;
            var objIdaVolta = document.getElementById("rdoIdaVolta");
            if (objIdaVolta != null) {
                objIdaVolta.checked = true;
            }

            var datesviator = $("#txtIda, #txtVolta").datepicker({
                buttonImage: '/img/icon_cal.gif',
                buttonImageOnly: true,
                minDate: '+0',
                maxDate: '+1y',
                numberOfMonths: 1,
                changeYear: false,
                dateFormat: 'dd/mm/yy',
                //showAnim: 'fade', problema em homologacao
                showAnim: '',
                closeText: 'Fechar',
                currentText: 'Mês Atual',
                dayNamesMin: ['D', 'S', 'T', 'Q', 'Q', 'S', 'S'],
                monthNames: ['Janeiro', 'Fevereiro', 'Março', 'Abril', 'Maio', 'Junho', 'Julho', 'Agosto', 'Setembro', 'Outubro', 'Novembro', 'Dezembro'],
                onSelect: function (selectedDate) {
                    if (this.id == "txtIda") {
                        instance = $(this).data("datepicker"),
                                   date = $.datepicker.parseDate(
                                            instance.settings.dateFormat ||
                                            $.datepicker._defaults.dateFormat,
                                            selectedDate, instance.settings);
                        datesviator.not(this).datepicker("option", "minDate", date);
                        SetDatesCalendar();
                    }
                }
            });

            SetDatesCalendar();
        };
   
    </script>
