    var ctrllbErroSenha = "ctl00_ContentPlaceHolder1_login_lbErroSenha";
    var ctrltxtLogin = "ctl00_ContentPlaceHolder1_login_txtLogin";
    var ctrltxtSenha = "ctl00_ContentPlaceHolder1_login_txtSenha";
    var ctrlbtLogar = "ctl00_ContentPlaceHolder1_login_btEntrar";

    $("#" + "ctl00_ContentPlaceHolder1_login_txtLogin").focus();
    
//    $('#' + ctrlbtLogar).attr('src', '/img/bt_enter_login.png');
    function fncVerifyOk() {
        var mensagem = "";
        var objLogin = document.getElementById(ctrltxtLogin);
        var objSenha = document.getElementById(ctrltxtSenha);
        var objErroSenha = document.getElementById(ctrllbErroSenha);

        if (objLogin.value == "" && objSenha.value == "") {
            objErroSenha.innerHTML = "Preencha o login e a senha.";
            return false;
        }
        if (objLogin.value == "") {
            objErroSenha.innerHTML = "Preencha o login.";
            return false;
        }
        if (objSenha.value == "") {
            objErroSenha.innerHTML = "Preencha a senha.";
            return false;
        }

        // TrackEvent - Nova entrada no site.
        _gaq.push(['_trackEvent', 'Login [Entrar]', 'Login Efetuado.']);
        return true;
    }
    var isLoginFocus = false;
    function fncLoginFocus(obj) {
        isLoginFocus = true;
    }
    function fncLoginBlur(obj) {
        isLoginFocus = false;
    }
    function fncCheckEnter(obj, ev) {
        var key = 0;
        try {
            key = event.keyCode;
        }
        catch (e) {
            key = ev.which;
        }
        var objLogin = document.getElementById(ctrltxtLogin);
        var objSenha = document.getElementById(ctrltxtSenha);
        var objButton = document.getElementById(ctrlbtLogar);
        if (key == 13) {
            if (objLogin.value == "") {
                objLogin.focus();
                return false;
            }
            if (objSenha.value == "") {
                objSenha.focus();
                return false;
            }

            if (objLogin.value != "" && objSenha.value != "") {
                isLoginFocus = false;
                objButton.click();
                return false;
            }
        }
        return true;
    }

    function fncTrataCheck() {

        var cssclass = $("#dd_stay_logged").attr('class');
        if (cssclass == "checkoff") {
            $("#dd_stay_logged").attr('class', 'checkon');
            $("#ctl00_ContentPlaceHolder1_login_hd_stay_logged").val('checkon');
        }
        else {
            $("#dd_stay_logged").attr('class', 'checkoff');
            $("#ctl00_ContentPlaceHolder1_login_hd_stay_logged").val('checkoff');
        }
    }
    function DoDummyLogin() {
        var btnDummy = document.getElementById("btnDummyFB");
        if (btnDummy != null) {
            if (btnDummy.style.display != "none") {
                btnDummy.click();
            }
        }
    }
</script>


