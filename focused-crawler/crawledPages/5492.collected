define('performanceLogger', [], function() {
    var performanceLogger = {
        restart: function() {},
        time: function() {},
        timeEnd: function() {},
        publish: function() {}
    };

    if(window.performance && window.performance.timing && window.performance.now) {
        performanceLogger = {
            isFirstRequest: true,
            startTime: 0,
            events: {},
            restart: function() {
                this.isFirstRequest = false;
                this.events = [];
                this.startTime = this.getCurrentTime();
            },
            getCurrentTime: function() {
                return window.performance.now();
            },
            createEvent: function(id, startTime) {
                return {
                    id: id,
                    startTime: (startTime) ? startTime : this.startTime
                };
            },
            time: function(id) {
                var currentTime = this.getCurrentTime();
                if(id) {
                    this.events[id] = this.createEvent(id, currentTime);
                } else {
                    this.logError('ID must not be blank');
                }
            },
            timeEnd: function(id) {
                var event;
                if(id) {
                    event = this.events[id];
                    if(!event) {
                        event = this.createEvent(id);
                        this.events[id] = event;
                    }

                    if(!event.endTime) {
                        event.endTime = this.getCurrentTime();
                        event.elapsedTime = event.endTime - event.startTime;
                    }


                } else {
                    this.logError('ID must not be blank');
                }
            },
            publish: function() {
                var that = this;
                require('dctk', function(dctk) {
                    var event, params, key;
                    if(dctk.loggingAdapter) {
                        for(key in that.events) {
                            event = that.events[key];
                            if (event.elapsedTime) {
                                params = that.buildParamsForTrx(event);
                                dctk.loggingAdapter.logTrxEvent('navTiming', params);
                            }
                        }
                    }
                });
            },
            buildParamsForTrx: function(event) {
                var params = [],
                    eventName = event.id;

                if(this.isFirstRequest) {
                    eventName += "_first";
                } else {
                    eventName += "_subsequent";
                }

                params.push('clEventName01=' + eventName);
                params.push('clEventTime01=' + event.elapsedTime);

                return params;
            },
            logError: function(msg) {
                require('logger', function(logger){
                    logger.logMessage('[performanceLogger] Error ' + msg, []);
                });
            }
        };
    }

    return performanceLogger;
