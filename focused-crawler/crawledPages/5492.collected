require(['config', 'performanceLogger'], function(config, performanceLogger){
    "use strict";
    // json caching. attaching these to window object as we can't return after wrapping inside require
    window.timeToLive = 5 * 60 * 1000; // 5 minutes in milliseconds
    window.useSavedState = function (state) {
        if(!state) {
            return false;
        }
        var isValidTimeToLive = window.timeToLive > 0;
        var hasStateNotExpired = state.expirationTime && state.expirationTime > new Date().getTime();
        var isSameRelease = state.metaData && state.metaData.releaseVersion &&
            state.metaData.releaseVersion === window.releaseVersion;
        return (isValidTimeToLive && hasStateNotExpired && isSameRelease);
    };

    window.handleError = function(isFirstAjax, httpStatus, textStatus, responseText, error) {
        require('ajaxErrorHandler', function(ajaxErrorHandler) {
            ajaxErrorHandler.handleError(isFirstAjax, httpStatus, textStatus, responseText, error);
        });
    };

    window.isFirstXHR = true;

    window.firstXHR = function(window, XMLHttpRequest, ActiveXObject) {
        /*
        HORRIBLE HACK to fix an IE bug
        This replace refreshes the page and allows us to replace the URL fragment on the second page load without
        triggering a full page load. By doing it early we can at least prevent an additional AJAX request.
        */
        var isMSIE = /*@cc_on!@*/0;
        if (isMSIE && window.location.hash) {
            window.location.replace(window.location.hash); // in normal browsers this does not cause a full page refresh
        }
        /* end of HACK */

        // We don't have any fast back info. We need to setup & execute the AJAX call
        var xmlHttp,
            ajaxQuery = config.getHash(),
            headers = {
                "X-Requested-With": "XMLHttpRequest",
                "Accept": "application/json, text/javascript",
                "Content-type": "application/x-www-form-urlencoded"
            };

        if(!ajaxQuery) {
            ajaxQuery = "destination=";
        }
        
        ajaxQuery += "&hashParam=" + config.hashParam;

        try {
            xmlHttp = new XMLHttpRequest(); // Try creating the default XMLHTTPRequest
        } catch(e) {
            try {
                xmlHttp = new ActiveXObject("Microsoft.XMLHttp"); // Of course IE has to be different
            } catch(ex) {
                // will probably never run .. but just in case
                throw new Error('Unable to create XMLHTTP object');
            }
        }

        // Setup the callback function to handle the response
        xmlHttp.onreadystatechange = function() {
            if (this.readyState === 4) {
                var self = this;
                if(this.status >= 400) {
                    window.handleError(true, self.status, self.statusText, self.responseText);
                } else if (this.status === 0 || !this.responseText) {    // This happens when the user cancels the request (cancel button, refresh, back/forward, ect)
                    require('ajaxErrorHandler', function(ajaxErrorHandler) {
                        var responseText = (self.responseText) ? self.responseText : "";
                        ajaxErrorHandler.handleError(true, self.status, self.statusText, responseText, new Error("First XHR status 0 or no response"));
                    });
                } else {
                    require(['jquery', 'utils', 'presenter', 'config'], function($, utils, presenter, config){
                        performanceLogger.timeEnd('ajaxResponse');
                        // Parse JSON
                        try {
                            window.pageModel = $.parseJSON(xmlHttp.responseText);

                            if(window.pageModel) {
                                window.pageModel.request = utils.queryToObject(utils.getHash());
                            } else {
                                window.handleError(true, xmlHttp.status, xmlHttp.statusText, xmlHttp.responseText);
                            }
                        } catch(err) {
                            // Matching JQuery.ajax here by passing 'parseerror'
                            window.handleError(true, xmlHttp.status, 'error while parsing JSON', xmlHttp.responseText, err);
                        }

                        // Render the HTML
                        try {
                            require(['allModules'], function (allModules) {
                                presenter.registerModules(allModules);
                                presenter.render(window.pageModel);

                                // Hide the interstitial
                                require(['interstitialCtrl'], function(interstitialCtrl){
                                    try {
                                        if (window.pageModel === null || !window.pageModel || window.pageModel.handler !== "redirect") {
                                            interstitialCtrl.hideInterstitials();
                                            if(!config.isPerceivePageDisplay) {
                                                performanceLogger.timeEnd('resultsVisible');
                                            }
                                        }
                                    } catch(err) {
                                        window.handleError(true, xmlHttp.status, 'error while hiding the interstitial', xmlHttp.responseText, err);
                                    }
                                });

                                // After Page Render
                                try {
                                    if (config.delayAfterPageRender) {
                                        setTimeout(presenter.executeAfterPageRender, 50); // roll down the event queue
                                    } else {
                                        presenter.executeAfterPageRender();
                                    }
                                } catch(err) {
                                    window.handleError(true, xmlHttp.status, 'error while executing after page render. delay=' + config.delayAfterPageRender, xmlHttp.responseText, err);
                                }
                                });
                        } catch(err) {
                            window.handleError(true, xmlHttp.status, 'error while calling presenter.render()', xmlHttp.responseText, err);
                        }

                        // Save History
                        require(['formController', 'historyManager'], function(formController, historyManager){
                            try {
                                if(window.pageModel && window.pageModel.metaData && window.pageModel.metaData.isRetryRequest) {
                                    formController.updateHashFromForm();
                                }
                                historyManager.saveState(window.pageModel);
                            } catch(err) {
                                window.handleError(true, xmlHttp.status, 'error while saving history', xmlHttp.responseText, err);
                            }
                        });
                    });
                }
            }
        };

        // Execute the request. Use all the headers, and parameters
        xmlHttp.open("POST", config.dataUrl, true);

        for(var header in headers) {
            if (headers.hasOwnProperty(header)) {
                xmlHttp.setRequestHeader(header, headers[header]);
            }
        }
