define('ajaxErrorHandler', ['config'], function(config){
    var ajaxErrorHandler = {
        "handleError": function(isFirstAjax, httpStatus, textStatus, responseText, error) {
            var isHtml = false;
            if (!config.hashParam && httpStatus === 400) {
                require('interstitialCtrl', function(interstitialCtrl) {
                    interstitialCtrl.showAjaxInterstitial();
                });
                require('dctk', function(dctk){
                    dctk.loggingAdapter.logMessage('ajax auth failure', ['responsive=true']);
                });
                location.reload(true);
            }
            // First we want to display something to the user.
            if (/<!DOCTYPE html/.test(responseText)) {
                // the response from the server is an HTML document. Just write it out (this is likely a "We're Stuffed" page)
                document.close();
                document.write(responseText);
                document.close();
                isHtml = true;
            } else {
                // worst case is we don't know what to render, just render a generic error
                this.handleSoftError();
            }

            // Now we can log something to client side logs so that we know it happened and can fix it later
            require('logger', function(logger){
                var additionalParams = {
                    "isFirstAjax": isFirstAjax,
                    "httpStatus": httpStatus,
                    "textStatus": textStatus,
                    "location": encodeURIComponent(window.location.href),
                    "isHtml": isHtml
                };

                if(!error) {
                    error = new Error('Error while processing XHR'); // we need to add a dummy error so the DCT doesn't disallow us
                }

                logger.logError('Error while processing XHR', error, additionalParams);
            });
        },
        "handleSoftError": function() {
            require(['presenter', 'interstitialCtrl', 'uitk', 'config'], function(presenter, interstitialCtrl, uitk, config){
                window.pageModel = {
                    "error": {
                        "status": status,
                        "isErrorMessage": true
                    },
                    "wizard": {
                        "isDatelessSearch": false,
                        "isError": true,
                        "selRoomIndex": 1,
                        rooms: [{"adults":2, childAges:[]}]
