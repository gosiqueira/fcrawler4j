}

.flyingAirplaneConfirmation {
    margin-top: 55px;
    text-align: center;
}

.bestPrice,
.bestService,
.connectingCombining {
    margin: 40px auto 0 auto;
    display: table;
}

.bestPrice .group,
.bestService .group,
.connectingCombining .group {
    display: table-row;
}

.bestService .group div,
.bestPrice .group > div,
.connectingCombining .group > div {
    display: table-cell;
    vertical-align: top;
}

.connectingCombining .group > div > .title {
    font-family: Arial;
    font-size: 18px;
    color: #666;
    display: block;
    clear: both;
    padding-left: 0px;
}

#slidingCos {
    display: block;
    clear: both;
    width: 178px;
    height: 29px;
    background-image: url("/images/shared/new/WaitingVisor.png");
    text-align: center;
    margin-top: 10px;
    margin-left: 0px;
    font-family: Arial;
    font-size: 18px;
    color: #039;
    font-weight: bold;
    padding-top: 8px;
}

.connectingCombining .group .separator {
    width: 115px;
    display: table-cell;
}

.bestService,
.bestPrice {
    margin-top: 75px
}

.bestService .separator,
.bestPrice .separator {
    width: 145px;
    text-align: center;
}

.getBestService,
.thousandFlighsBooked,
.escape,
.getBestPrice {
    width: 300px;
    text-align: right;
    font-family: Arial;
    font-size: 20px;
    color: #666;
}

.thousandFlighsBooked,
.escape {
    text-align: left;
}

.thousandFlighsBooked .title,
.escape .title {
    font-size: 20px;
    color: #039;
    display: block;
    padding: 0;
    margin: 0;
    height: auto;
}

.thousandFlighsBooked .subtitle,
.escape .subtitle {
    font-size: 16px;
    color: #333;
    display: block;
}

.processingBooking {
    font-family: Arial;
    font-size: 20px;
    color: #999;
    text-align: center;
    margin-top: 70px;
}

.processingBookingConfirmation {
    font-family: Arial;
    font-size: 20px;
    color: #999;
    text-align: center;
    margin-top: 70px;
    margin-bottom: 50px;
}

.waitingAds {
