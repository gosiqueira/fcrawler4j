#waitScreen {
    font-family: arial;
    width: 960px;
    padding-bottom: 0;

}

#waitScreenContent {
    padding-top: 0;
}

#waitScreenContent .headerSections .headerMiddleSection,
#waitScreenContent .headerSections .headerMiddleSectionLeft,
#waitScreenContent .headerSections .headerMiddleSectionRight {
