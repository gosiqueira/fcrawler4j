    background: white;
    position: relative;
    height: 30px;
    display: block;
    padding: 0;
    margin: 0;
    top: 7px;
}

.waitSearchInfoPanel .separator{
    padding: 0 21px;
}

.flyingAirplane {
    margin-top: 35px;
