  var wrap = $(document);
  var box = $("[data-sidebar]");
  var boxTopPosition = box.offset().top;
  var articleFooter = $('.article-footer');

  var right = $('body').width() - (box.offset().left + box.width());

  wrap.on("scroll", function(e) {
   var scrollBottom = wrap.scrollTop();
   if (wrap.scrollTop() > boxTopPosition) {
     if (scrollBottom < articleFooter.offset().top){
       box.attr('style', 'position: fixed; top: 35px; right: ' + right + 'px;');
     } else {
       fixedTop = parseInt(articleFooter.offset().top) - parseInt(scrollBottom);
       box.attr('style', 'position: fixed; top: '+ fixedTop +'px; right: ' + right + 'px;');
     }
   } else {
     box.removeAttr('style');
   }
 });
