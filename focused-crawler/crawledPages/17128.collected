<script>
/* Inicializacoes        */
/*                       */

$(document).ready(function(){
	$('#calendar_frame').fadeOut(0); //some com calendario pra aparecer quando carregar

//Boxes pra escolher cidade e estado de acordo com localizacao inicial
	

  $("#estado").change(function(){
		if ($("#estado").val()=='ALL') {  //selecionou Todos
			$("#cidade").html('<option value="">Antes Escolha um Estado</option>'); 
			$('#calendar_frame').fadeOut(500, function(){
				$('#calendar_frame').attr('src','calendar/samples/03_extensions/meu.php?&ano=2015&estado=ALL');
			});
			$("#location_info").text('Todos Feriados Estaduais');
			createCookie('estado','ALL',360);	
			eraseCookie('cidade');		
		}
		else
		if (($("#estado").val()!='') && ($("#estado").val()!='NONE')) {
      $("#cidade").html('<option value="0">Carregando...</option>');
      $.post("cidades.php",{estado:$(this).val()}, function(valor){
         $("#cidade").html(valor);
			})
			$('#calendar_frame').fadeOut(500, function(){
				$('#calendar_frame').attr('src','calendar/samples/03_extensions/meu.php?&ano=2015&estado='+$("#estado").val());
			});
			$("#location_info").text($("#estado").val());
			$("#location_header").text('Feriados em ' + $("#estado").val());
			document.title = 'Feriados em ' + $("#estado").val();
			createCookie('estado',$("#estado").val(),360);
			//$('#cidade').load('cidades.php?estado='+$('#estado').val()); //via GET
		} else { //nenhum
			$("#cidade").html('<option value="">Antes Escolha um Estado</option>'); 
		}
	});
	
	$("#cidade").change(function(){
		if ($("#cidade").val()!='') {
			var est = $("#estado").val();
			var cid = $("#cidade").val();
			$('#calendar_frame').fadeOut(500, function(){
				$('#calendar_frame').attr('src','calendar/samples/03_extensions/meu.php?&ano=2015&estado='+est.toLowerCase()+'&cidade='+cid.toLowerCase());
			});
			$("#location_info").text(cid+" - "+est);
			$("#location_header").text('Feriados em '+cid+" - "+est);
			document.title = 'Feriados em '+cid+" - "+est;
			createCookie('estado',est,360);
			createCookie('cidade',cid,360);
		}
	});  
	
});
</script>

