<script type='text/javascript'>//<![CDATA[
var pauseShow	= 6000;
var speedIn		= 1500;
var speedOut	= 1500;
var imageDestination	= '#intro';

$(document).ready(function() {
	var slides 	= new Array(
		"http://lh3.googleusercontent.com/-yW4S94diG-I/VRLnDB8El_I/AAAAAAAAb2Y/pPS7t8jyKlI/s1200/Pancakes.jpg",
		"http://lh4.googleusercontent.com/-YgJRdhBxKjQ/VS6nbx_Co3I/AAAAAAAAdPU/cjBn50JIH30/s1200/DR%25201.jpg",
		"http://lh3.googleusercontent.com/-l8rSSyFHC-M/VRLnC-1NDFI/AAAAAAAAb2U/5giPkWZ1mUA/s1200/Veg%2520Salad.jpg",
		"http://lh5.googleusercontent.com/-fjFoFEAVZM8/VQsWSWYYeAI/AAAAAAAAbUw/Yn4VF9Obl-k/s1200/Dinner-Milanese.jpg",
		"http://lh6.googleusercontent.com/-mkLk46AxjM8/VQsV1BgqmwI/AAAAAAAAbUU/MqPw4J2jqQk/s1200/Starfish-2.jpg"
	);
	slides.forEach(function(entry) {
		var newImg = new Image;
		newImg.src = entry;
	});
	var slides_count = slides.length;
	var counter = 0;
	
	function slideshow_jump(index){
		var urlValue = 'url(' + slides[index] + ')';
		$(imageDestination).css('background-image',urlValue);
	}
	if(slides){
        var timer = window.setInterval(function() {
            slideshow_jump(counter);
			if (counter < slides_count){
				counter++;
				if (counter>= slides_count) counter = 0;
			} else counter = 0;
        }, pauseShow);
	}

	$('#leftbutton').click(function(){
		if (counter>0){
			counter--;
		} else counter = slides_count-1;
		slideshow_jump(counter);
		window.clearInterval(timer);
	});

	$('#rightbutton').click(function(){
		if (counter< slides_count-1){
			counter++;
		} else counter = 0;
		slideshow_jump(counter);
		window.clearInterval(timer);
	});
	
	(function() {
		var mX, mY, distance,
			$leftarrow  = $('#leftbutton');
			$rightarrow  = $('#rightbutton');
			$introarrow  = $('#introbutton');

		function calculateDistance(elem, mouseX, mouseY) {
			return Math.floor(Math.sqrt(Math.pow(mouseX - (elem.offset().left+(elem.width()/2)), 2) + Math.pow(mouseY - (elem.offset().top+(elem.height()/2)), 2)));
		}

		$(document).mousemove(function(e) {  
			mX = e.pageX;
			mY = e.pageY;
			distanceLeft = calculateDistance($leftarrow, mX, mY);
			distanceRight = calculateDistance($rightarrow, mX, mY);
			distanceIntro = calculateDistance($introarrow, mX, mY);
			
			if(distanceLeft <= 100){
				$leftarrow.addClass('animate');
			} else if (distanceLeft > 150) {
				$leftarrow.removeClass('animate');
			}
			if(distanceRight <= 100){
				$rightarrow.addClass('animate');
			} else if (distanceRight > 150) {
				$rightarrow.removeClass('animate');
			}
			if(distanceIntro <= 100){
				$introarrow.addClass('animate');
			} else if (distanceIntro > 150) {
				$introarrow.removeClass('animate');
			}
		});

	})();
	
	
});
//]]></script>

