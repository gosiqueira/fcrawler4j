        <script type="text/javascript">
            $(document).ready(function () {
                var time = Math.floor(new Date().getTime() / 1000);
                var hour = 60 * 60;
                var day = 24 * hour;
                var month = 30 * day;
                var expMonth = 3 * month;
                var expTime = time + expMonth;
                var expDate = new Date(expTime * 1000);
                doPopup();
                $("#accept-cookie").click(function () {
                    document.cookie = "fotaxi-cookie-accepted=1; expires=" + expDate.toString();
                    $("#cookie-popup").hide();
                })

                function checkAccepted() {
                    var cookies = document.cookie;
                    var cookiesArray = cookies.split(';');
                    var accepted = false;
                    for (var i = 0; i < cookiesArray.length; i++) {
                        var trimmed = cookiesArray[i].trim();
                        var cookieKeyPairs = trimmed.split("=");

                        if (cookieKeyPairs[0] == "fotaxi-cookie-accepted" && cookieKeyPairs[1] == 1) {
                            accepted = true;
                            break;
                        }
                    }
                    return accepted;
                }
                function doPopup() {
                    if (!checkAccepted()) {
                        $("#cookie-popup").show();
                    }
                }
            })
        </script>
