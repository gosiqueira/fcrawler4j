    <!-- A destacar -->
    <section id="puntosFuertes" class="puntosFuertes module mt-50">
        <h3 class="titularDestacado gris"><i class="fa fa-exclamation-circle"></i>In vetrina</h3>

            <div class="row">
                    <div class="col-md-6">
                        <div class="box contenidoADestacar">
