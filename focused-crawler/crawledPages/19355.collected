define(["placement"], function(placement) {
// 0 - small
// 1 - waiting for possible expansion
// 2 - expanding or expanded
var avafState = 0;
var avafTimer = false;
var avafMoreExpandedSize = 0;
/**
* Handles the onhover expansion of the Related to cross sell
*
* @param {Event} e The Event
* @param {Number} actionDelay How long to wait before expanding
*/
var relatedHoverOver = function(e, actionDelay) {
// Received mouseover: If quiescent small, start waiting.
try {
if (avafState !== 0) {
return;
}
if (actionDelay === 0) {
avafExpand();
} else {
avafWait((e) ? (e.target || e.srcElement) : null, actionDelay);
}
} catch (err) {
ta.util.error.record(err, 'also viewed - avafIn');
}
}; //end of relatedHoverOver
/**
* Handles the mouse out behavior on the Related to cross sell
*
* @param {Event} e The Event
*/
var relatedHoverOut = function(e) {
// Received mouseout
try {
if (avafState != 1 || !avafTimer) {
return;
}
if (!e) {
e = window.event;
}
if (!e) {
return;
}
// On mouseout, find the destination, i.e. the place we've gone out to.
var reltg = (e.relatedTarget) ? e.relatedTarget : e.toElement;
// Search up the tree to see whether the place we left for is (a) a child of our
// containing box, or (b) a child of BODY, i.e., outside the containing box.
// (If for some reason we got no destination, treat as case (b), which will mean
//  aborting the expansion; this is the safer course.)
var tg = ta.id(placement.id).getElement('.mainList');
while (reltg && reltg != tg && reltg.nodeName != 'BODY')
{
reltg = reltg.parentNode;
}
// In case (a), we're still really within the box, so ignore this event.
if (reltg == tg) return;
// In case (b), we've left the containing box, so abort the timer and the expansion.
clearTimeout(avafTimer);
avafTimer = false;
avafState = 0;
} catch (err) {
ta.util.error.record(err, 'also viewed - avafOut');
}
}; //end of relatedHoverOut
/**
* Handles the further expansion of the Related to cross sell,
* via the more/less toggle
*
* @param {Event} evt The Event
* @param {Boolean} more If more is true, we expand further
*/
var relatedExpand = function(evt, more) {
var placementElem
, whichElem
, moreElem
;
try {
ta.trk("AVAF_EXPAND_" + (more ? "MORE" : "LESS"), false, (evt) ? (evt.target || evt.srcElement) : null);
placementElem = ta.id(placement.id);
if (more) {
whichElem = placementElem.getElement(".hideableMore");
if(!whichElem) return;
whichElem.addClass("hidden");
whichElem = placementElem.getElement(".expand_av_on_more");
if(!whichElem) return;
whichElem.removeClass("hidden");
if (avafMoreExpandedSize === 0) {
whichElem.addClass("expanding");
avafMoreExpandedSize = whichElem.getSize().y;
whichElem.removeClass("expanding");
}
whichElem.addClass("show");
new Fx.Tween(whichElem, {
duration: 'long',
property: 'height'
}).start(0, avafMoreExpandedSize);
} else {
whichElem = placementElem.getElement(".expand_av_on_more");
if(!whichElem) return;
new Fx.Tween(whichElem, {
duration: 'long',
property: 'height'
}).start(whichElem.getSize().y, 0).chain(
function() {
whichElem.removeClass("show");
whichElem.addClass("hidden");
moreElem = placementElem.getElement(".hideableMore");
if(!moreElem) return;
moreElem.removeClass("hidden");
}
);
}
} catch (err) {
ta.util.error.record(err, 'also viewed - avafMore');
}
}; //end of relatedExpand
function avafWait(element, actionDelay) {
// Wait to expand
try {
avafState = 1;
avafTimer = setTimeout(function() {
avafExpand(element);
