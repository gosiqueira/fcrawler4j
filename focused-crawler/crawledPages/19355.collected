define(["widget"], function(widget) {
var carousel = widget.element.getElement('.carousel');
var carouselInner = carousel.getElement('.carouselInner');
var cells = carouselInner.getChildren();
var numPages = parseInt(carousel.getAttribute('data-num-pages'), 10);
var pageSize = parseInt(carousel.getAttribute('data-page-size'), 10);
var id = carousel.getAttribute('data-id');
var arrows = widget.element.getElements('.arrow');
var leftArrow = widget.element.getElement('.arrow.left');
var rightArrow = widget.element.getElement('.arrow.right');
var isRtl = !!document.getElement('.rtl');
function _setPage(page, element) {
carousel.setAttribute('data-page-number', page);
if (page == 1) {
leftArrow.addClass('disabled');
}
else {
leftArrow.removeClass('disabled');
}
if (page == numPages) {
rightArrow.addClass('disabled');
}
else {
rightArrow.removeClass('disabled');
}
var cell = cells[(page - 1) * pageSize];
var cellLeft = cell.getPosition(carouselInner).x;
var transform;
if (!element) { // not in response to a click
arrows.addClass('instant');
transform = 'setStyle';
} else {
arrows.removeClass('instant');
carousel.addClass('scrolling');
carouselInner.set('tween', {onComplete: function() {
carousel.removeClass('scrolling');
}});
transform = 'tween';
ta.fireEvent('prodp13n.carousel.setPage', id, page);
ta.trk('SetCarouselPage', false, element, ["N:" + page]);
}
if (isRtl) {
carouselInner[transform]('right', '-' + (carouselInner.getSize().x - (cellLeft + cell.getSize().x)) + 'px');
}
else {
carouselInner[transform]('left', '-' + cellLeft + 'px');
}
}
return {
left: function(element) {
var page = parseInt(carousel.getAttribute('data-page-number'), 10);
if (page > 1) {
_setPage(page - 1, element);
}
},
right: function(element) {
var page = parseInt(carousel.getAttribute('data-page-number'), 10);
if (page < numPages) {
_setPage(page + 1, element);
}
},
setPage: function(page) {
if (page < 1) {
page = 1;
}
if (page > numPages) {
page = numPages;
}
if (page != parseInt(carousel.getAttribute('data-page-number'), 10)) {
_setPage(page, true);
}
}
};
});})();
