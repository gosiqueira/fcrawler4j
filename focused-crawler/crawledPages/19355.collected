whichElem.removeClass("expanding");
whichElem.addClass("show");
var avaf = placementElem.getParent();
if (avaf) {
avaf.setStyle('height', 'auto');
}
var slide_up = new Fx.Tween(whichElem, {
duration: 'long',
property: 'height'
});
