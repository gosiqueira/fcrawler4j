        e.preventDefault();
        e.stopPropagation();

        var $target = $(e.target).closest('.alert-item');

        $target
            .find('.close-button')
            .remove();

        $target
            .find('.alert-message')
            .html('Nous vous enverrons les mises à jour du Scratchpad par courriel. Surveillez votre boîte de réception!');
        
