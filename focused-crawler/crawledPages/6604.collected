  window.alertMenuClickHandlers = {
    spSignIn: function(e, alertItemRef) {
        if (window.AlertsMenu) {
            AlertsMenu.sendeVar28('Header.Notif.SP.SignIn');
        }
    },
    spNudgeEnroll: function(e, alertItemRef) {
        e.preventDefault();
        e.stopPropagation();

        var $target = $(e.target).closest('.alert-item');

        $target
            .find('.close-button')
            .remove();

        $target
            .find('.alert-message')
            .html('We\'ll send Scratchpad updates to you by email. Watch your inbox!');
        
        if (Scratchpad.state.loggedInUser) {
            uitk.utils.liveAnnounce('We\'ll send Scratchpad updates to you by email. Watch your inbox!');
        }

        if (window.uitk 
        && window.uitk.utils 
        && window.uitk.utils.focusElement) {
            uitk.utils.focusElement($target);
        }

        if (window.AlertsMenu) AlertsMenu.sendeVar28('Header.Notif.SP.OptIn');
        if (window.Scratchpad) Scratchpad.enrollInNudge();
    },
    spBasicNews: function(e, alertItemRef) {
        if (window.AlertsMenu) {
            AlertsMenu.sendeVar28('Header.Notif.SP.News');
        }
    },
    spDetailedNews: function(e, alertItemRef) {
        if (window.AlertsMenu) {
            AlertsMenu.sendeVar28('Header.Notif.SP.LiveAlerts');
        }
    },
    addChromeExtension: function(e, alertItemRef)
    {
        function successCallback ()
        {
            dctk.loggingAdapter.logMessage("ChromeExpediaExtensionSuccessful")
            if(localStorage)
            {
                var date = new Date($.now());
                var month = date.getUTCMonth() + 1;
                var day = date.getUTCDate();
                var year = date.getUTCFullYear();
                var hours = date.getUTCHours();
                var minutes = date.getUTCMinutes();
                var seconds = date.getUTCSeconds();

                var logDate = year + "-" + month + "-" + day + " " + hours + ":" + minutes + ":" + seconds;
                localStorage.setItem("chromeExtensionInstallCTA", logDate);
            }
        }

        function failureCallback (error)
        {
            dctk.loggingAdapter.logError("ChromeExpediaExtensionFailed", error)
        }


         if (window.AlertsMenu)
            {
                AlertsMenu.sendeVar28('HP.notbell');
            }


        chrome.webstore.install("https://chrome.google.com/webstore/detail/cfbpcgmafclgfmpoelldpmiolgblgbel", successCallback, failureCallback);
    }
  };


  if (window.define && window.require)
