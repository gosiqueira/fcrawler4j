//Function Date //
function split_date()
{
	var d = $("#date_check_in").val();
	var tmp = d.split("/");
	$("#fromday").val(tmp[0]);
	$("#frommonth").val(tmp[1]);
	$("#fromyear").val(tmp[2]);
}


function show_current_date()
{
	var today = new Date();
	var dd = today.getDate();
	var mm = today.getMonth()+1;//January is 0!
	var yyyy = today.getFullYear();
	if(dd < 10)
	{
		dd = '0' + dd;
	}
	if(mm < 10)
	{
		mm = '0' + mm;
	}
	var set_today = dd+"/"+mm+"/"+yyyy;

	return set_today;
}

function show_max_date()
{
	var today = new Date();
	var yyyy = today.getFullYear() + 1;
	var set_today = "31/12/"+yyyy;

	return set_today;
}

function clear_date()
{
	var set_today = show_current_date();
	$("#date_check_in").val(set_today);
	split_date();
}
