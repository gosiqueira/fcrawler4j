	<script>

		$(function() {

			// grab the initial top offset of the navigation
			var sticky_navigation_offset_top = $('#page-header').offset().top;

			// our function that decides weather the navigation bar should have "fixed" css position or not.
			var sticky_navigation = function(){
				var scroll_top = $(window).scrollTop(); // our current vertical position from the top

				// if we've scrolled more than the navigation, change its position to fixed to stick to top, otherwise change it back to relative
				if (scroll_top > sticky_navigation_offset_top) {
					$('#page-header').css({ 'position': 'fixed', 'top':0, 'left':0, 'z-index':20000,'background': '#fff', 'width':'100%','height':'60px'});
					$('#topmenu').css({'background-color': '#fff', 'border-bottom':'1px soild #999;' });
					$('.menu-wrapper').css({'top':'60px'});
					$('#device-menu').css({'top':'-35px'});
					$('.imglogo img').css({'max-width':'120px','height':'auto'});
				} else {
					$('#page-header').css({ 'position': 'relative','height':'80px', 'z-index':9999});
					$('#topmenu').css({'background-color': 'rgba(255,255,255,0.5)', 'border-bottom':'none', });
					$('.menu-wrapper').css({'top':'80px'});
					$('#device-menu').css({'top':'0'});
					$('.imglogo img').css({'max-width':'180px','width':'100%'});
				}
			};

			// run our function on load
			sticky_navigation();

			// and run it again every time you scroll
			$(window).scroll(function() {
				 sticky_navigation();
			});


		});


	</script>



