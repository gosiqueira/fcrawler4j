countElem = " <div class='filterCount'>("+filter.count+")</div>";
}
var filterElem = new Element("div", {
"class": "filterItem filter" + (filter.selected ? " selected" : ""),
"data-name": filter.datakey,
"data-value": filter.datavalue,
"title": filter.name,
