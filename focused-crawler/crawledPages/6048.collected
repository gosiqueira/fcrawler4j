          <h4 class="item-title">{{hotelName}}</h4>
          <p class="item-date">{{checkInDate}} - {{checkOutDate}}</p>
          <p class="item-watches item-nested">{{viewership viewCount}}</p>
          <div class="item-price-urgency"><strong class="price">{{price}}</strong><p class="item-nested">{{lastSeenDate}}</p></div>
        </div>
      </a>
    </li>
</script>


