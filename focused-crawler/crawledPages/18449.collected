<script>
     $(document).ready(function() {
         //Initialize select2 components
         $("#countrySelect").select2({
            dropdownAutoWidth: true,
            minimumResultsForSearch: 10,
            containerCssClass: "form-control block-level input-country"
         });      

         $("#cityRegionSelect").select2({
             dropdownAutoWidth: true,
             minimumResultsForSearch: 10,
             enable: true, 
             containerCssClass: "form-control block-level input-city"
          });      

         //show once initialized
         $("#countryList").css("visibility", "visible");
         $("#cityRegionList").css("visibility", "visible");

         //"blacken" the text
         if ($("#countrySelect").attr("data-placeholder") != 'Selecionar um país') {
        	 $("#countryList > .select2-container > a").removeClass("select2-default");
         }
         if ($("#cityRegionSelect").attr("data-placeholder") != 'Selecionar uma região/cidade') {
        	 $("#cityRegionList > .select2-container > a").removeClass("select2-default");         
         }
         
         //event handler for select2 components  
         $("#countrySelect").change(function() {
            var selectedCountry = $("#countrySelect").val();
            if (selectedCountry != null &&
                selectedCountry != '') {
                var parameters = selectedCountry.split("|");
                changeCountry(parameters[0], parameters[1], parameters[2], parameters[3], 'LHN');
            }     
         });

         $("#cityRegionSelect").change(function() {
             var selectedCityRegion = $("#cityRegionSelect").val();
             if (selectedCityRegion != null &&
                 selectedCityRegion != '') {
                 var parameters = selectedCityRegion.split("|");
                 changeRegion(parameters[0], parameters[1], parameters[2]);
             }     
          });
         
     });
     
	function changeCountry(puid, countryId, countryName, countryNameEncoded, navLocation){

		//$("#spnCountry").text(countryName.substring(0, getCountryCitySize()));
		$("#destinationID").val(countryId);
		$("#destinationNameEnc").val(countryNameEncoded);
		
		var destDiv = "div[id='cityRegionList']"; 
        $.ajax({
            url: "/"+ "pt" + "/" + puid + "/pas-citiessearch?country="+countryId + "&navLocation="+navLocation,
            success: function(response, status, xhr) {
            	
           		if (status != "error") {
	                $(destDiv).html(response);
                }   	
            },
        });
	}    

	function changeRegion(regionId, regionName, regionNameEncoded){

		//$("#spnRegion").text(regionName.substring(0, getCountryCitySize()));
		$("#destinationID").val(regionId);
		$("#destinationNameEnc").val(regionNameEncoded);
	}    

	function pasCheck() {

		var myDestName = $("#destinationNameEnc").val();

        var alertCountry = $("<div/>").html("Selecione um país para pesquisa.").text();
        var alertFromDateEmpty = $("<div/>").html("Insira a partir da data.").text();
        var alertToDateEmpty = $("<div/>").html("Insira até a data.").text();
        var alertFromDate = $("<div/>").html("A data De selecionada deve ser anterior à data Até.").text();
        var alertInvalidDate = $("<div/>").html("Pesquisa de data não pode ser inferior à data atual.").text();

		if (myDestName == "/") { 
			alert(alertCountry);
			return false;
		}
		
		var haven = "/pt/"+"16218"+"/";

        myDestName = myDestName.replace(/-/g, " ").replace(/\s+/g, "-").replace(/&/g, "and");
		haven = haven + myDestName + "/d" + $("#destinationID").val();

		if (($("#fromDate").val() != '' || $("#toDate").val() != '') && $("#fromDate").val()  != undefined )  {
			
			var fromDate = $("#fromDate").val();
			var toDate = $("#toDate").val();
			
			if($.trim(fromDate) == ''){
				alert(alertFromDateEmpty);
				return false;
			}
			else if($.trim(toDate) == ''){
				alert(alertToDateEmpty);
				return true;
			}
			
			var language = "pt";
			
			var fromMonthName = fromDate.substring(3,6);
			var fromMonthNumber = $.fn.datepicker.dates[language].monthsShort.indexOf(toTitleCase(fromMonthName));
			
			var toMonthName = toDate.substring(3,6);
			var toMonthNumber = $.fn.datepicker.dates[language].monthsShort.indexOf(toTitleCase(toMonthName));
			
			var from = new Date(); 
			var to = new Date();       
			from.setFullYear(fromDate.substring(6,10),fromMonthNumber, fromDate.substring(0,2));
			to.setFullYear(toDate.substring(6,10), toMonthNumber, toDate.substring(0,2));

			var currentDate = new Date();
			if (to < currentDate || from < currentDate) {
				alert(alertInvalidDate);
				return false;
			}
			
			if (to<from) {
				alert(alertFromDate);
				return false;
			}
			
			fromMonthNumber = fromMonthNumber + 1;
			if(fromMonthNumber < 10){
				fromMonthNumber = "0" + fromMonthNumber;
			}
			toMonthNumber = toMonthNumber + 1;
			if(toMonthNumber < 10){
				toMonthNumber = "0" + toMonthNumber;
			}
			
			haven = haven + "-ttd/from/" + fromDate.substring(0,2) + "-" + fromMonthNumber + "-" + fromDate.substring(6,10) + "/to/" + toDate.substring(0,2)  + "-" + toMonthNumber + "-" + toDate.substring(6,10);
		}
		else {
			
			haven = haven + "-ttd?activities=all";
		}

		document.location.href = haven;
		return false;
	}        

	function getCountryCitySize(){
		var topNav = false ;
		var size = 20;
		if(topNav){
			size = 23;
		}
		return size;
	}
	
	function toTitleCase(str) {
	    return str.replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();});
	}
	
</script>
	
	




