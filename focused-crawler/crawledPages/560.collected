  <script language="JavaScript" type=text/javascript>
function validaForm(){
		
d = document.form1;
				
	



	//validar nome
		if (d.nome.value == ""){
			alert("O nome deve ser preenchido!");
			d.nome.focus();
			return false;
		}
		
		
		var filtro_mail = /^.+@.+\..{2,3}$/
if (!filtro_mail.test(d.email.value) || d.email.value=="") {
alert("Preencha o e-mail corretamente.");
d.email.focus();
return false;
}
		
		
		//validar mensagem
		if (d.mensagem.value == ""){
			alert("O campo da mensagem está em branco");
			d.mensagem.focus();
			return false;
		}
		
		return true;
}



</script>        
        
