}
.right-box-wrapper,
.left-box-wrapper {
float: left;
width: 23.73%;
}
.left-box, .right-box {
height: 190px;
}
.left-box:first-child {
margin-bottom: 1px;
}
.right-box:first-child {
margin-bottom: 1px;
}
.center-box {
position: relative;
width: 52.37288%;
height: 381px;
margin: 0 0.0847457%;
}
.slider2 .center-box {
margin-right: 0.0847457%;
