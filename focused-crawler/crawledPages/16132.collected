}
.bleft .content-item:hover h2 {
color: #fff;
}
.bleft .other-posts .post-title {
padding: 6% 0;
}
.bleft .content-item h2 {
border-bottom: 1px solid #bebebe;
padding: 6% 0;
}
.bleft .content-item:last-child h2 {
border: 0;
}
/* bsummary
