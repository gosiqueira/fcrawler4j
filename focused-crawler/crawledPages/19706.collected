(function(){
var define = ta.prwidgets.define.bind(ta.prwidgets,'hotels_p13n_attributes_on_location','handlers.js');
/**
* Javascript for p13n_attributes_on_location widget, merely loads in cross-sell urls
*/
define(["widget"], function(widget) {
var last_tag_width = { last_processed_property_tags_width: 0 };  // for pass by reference
// Load the mapping of tag id's to url's.  This widget is per listing, but we only
// do this one, and fire an event when done.
function loadCrossSellUrls() {
var payload = {};
payload.a = "getCrossSellUrls";
payload.g = ta.retrieve('page.geo');
new Request({
url: "/PersonalizationAjax",
onSuccess: function(responseText) {
if (responseText) {
var crossSellUrls = JSON.parse(responseText);
if (crossSellUrls) {
ta.store('ta.p13n.prodp13nCrossSellUrls', crossSellUrls);
ta.fireEvent('prodp13n.crossSellUrlsReady');
}
}
},
data: payload,
method: 'post'
}).send();
}
// For each tag on this widget, look up the url and replace the elemnt.  One sample neighborhood url is
// represented under id '12345', which is then swapped with the real neighborhood id.
function doTheSwap() {
widget.element.getElements('span.tag').each(function(elmt) {
var tagId = elmt.getAttribute('data-tag-id');
var url;
if (tagId.charAt(0) === 'd') {
url = ta.retrieve('ta.p13n.prodp13nCrossSellUrls')['d12345'].replace('zfn12345', 'zfn' + tagId.substring(1));
}
else {
url = ta.retrieve('ta.p13n.prodp13nCrossSellUrls')[tagId];
}
if (url) {
var newElmt = document.createElement('a');
newElmt.setAttribute('class', 'tag');
newElmt.setAttribute('href', url);
newElmt.setAttribute('target', '_blank');
newElmt.setAttribute('onclick', elmt.getAttribute('data-onclick'));
newElmt.innerHTML = elmt.innerHTML;
elmt.parentNode.replaceChild(newElmt, elmt);
}
});
}
ta.queueForLoad(function() {
// If there are tags to swap...
if (widget.element.getElements('span.tag').length > 0 ) {
// If the lookup has completed, proceed with the swap.
if (ta.has('ta.p13n.prodp13nCrossSellUrls')) {
ta.p13n.p13ncommon.hideTags(widget.element.getElement(".clickable_tags"),
widget.element.getElement(".clickable_tags").getElements("span.tag"),
last_tag_width,  true);
doTheSwap();
}
else {
// Unless a different widget instance has initiated the loading of the url's, do it now.
if (!ta.has('ta.p13n.prodp13nLoadingCrossSellUrls')) {
ta.store('ta.p13n.prodp13nLoadingCrossSellUrls', true);
loadCrossSellUrls();
}
// When the loading is complete, do the swap.
ta.on('prodp13n.crossSellUrlsReady', function() {
ta.p13n.p13ncommon.hideTags(widget.element.getElement(".clickable_tags"),
widget.element.getElement(".clickable_tags").getElements("span.tag"),
last_tag_width, true);
doTheSwap();
});
}
}
}, "initialize " + widget.name);
/**
* Public facade
*/
return {
};
});
})();
