activate: ta.overlays.ACTIVATE_CLICK_TOGGLE,
backdrop: ta.overlays.BACKDROP_NEVER,
backdropClickClose: true,
showArrow: false,
style: 'allDealsOverlay',
positionSource: details,
allowFlip: false,
remoteContent: url,
hideSpinner: true,
evalScripts: true,
onLoad: function() {
