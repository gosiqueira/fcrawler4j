    <script type="text/javascript">

    // EN EL DOCUMENT READY DE COTIZACION.JS ESTA EL EVENT LISTENER DE LOS CLICK !!!!!


    function InicializarPopUp() {
        //Variables redimencionables
        var wHijo = $("#cuerpo").children().width();
        var hHijo = $("#cuerpo").children().height();

        $("#PopUp").css("height", document.body.scrollHeight + "px");
        //Darle el alto y ancho
        $("#cuerpo").css("width", wHijo + "px");
        $("#cuerpo").css("height", hHijo + "px");

        $("#rellenarcampos").css("width", wHijo + "px");
        $("#rellenarcampos").css("height", hHijo + "px");

        var w = $(this).width();
        var h = $(this).height();

        //Centra el popup   
        w = (w / 2) - (wHijo / 2);
        h = (h / 2) - (hHijo / 2);
        $("#cuerpo").css("left", w + "px");
        $("#cuerpo").css("top", h + "px");

        $("#rellenarcampos").css("left", w + "px");
        $("#rellenarcampos").css("top", h + "px");

    }

    function MostrarPopUp() {
        InicializarPopUp()
        $("#PopUp").css("visibility", "visible");
        $("#cuerpo").css("visibility", "visible");
    }

    function MostrarPopUpRellenarCampos() {
        InicializarPopUp()
        $("#PopUp").css("visibility", "visible");
        $("#cuerpo").css("visibility", "hidden");
        $("#rellenarcampos").css("visibility", "visible");
        
    }

    function OcultarPopUp() {
        $("#PopUp").css("visibility", "hidden");
        $("#cuerpo").css("visibility", "hidden");
        $("#rellenarcampos").css("visibility", "hidden");
    }


    function ToTop() {
        $('html, body').animate({
            scrollTop: '0px'
        },
                 1500);

    }


    // onchange="validarEmail(this)" en el id=mail
    function validarEmail(valor) {
        if (/^w+([.-]?w+)*@w+([.-]?w+)*(.w{2,3})+$/.test(valor)) {
            alert("La dirección de email " + valor + " es correcta.")
            return (true)
        } else {
            alert("La dirección de email es incorrectaaa.");
            return (false);
        }
    }

    function looksLikeMail(str) {
        var lastAtPos = str.lastIndexOf('@');
        var lastDotPos = str.lastIndexOf('.');
        if (lastAtPos > 0 && lastDotPos > 0) { return 1; }
    }


    
    function URL_Completo(pagina) {
        var sRet = ControlId("CadenaURL").value + pagina;

        return sRet;
    }
    
    
    
    function AgregarContacto() {
        var Nombre = document.getElementById("nombre").value;
        var Mail = document.getElementById("mail").value;
        //alert('Nombre: ' + Nombre + ', Mail: ' + Mail);
        var mailcorrect = looksLikeMail(Mail);


        Nombre = Nombre.replace(/^\s+/g, '').replace(/\s+$/g, '')
        Mail = Mail.replace(/^\s+/g, '').replace(/\s+$/g, '')



        if (Nombre != "" && Mail != "" && mailcorrect == 1) {
            var ajax = new XMLHttpRequest();

            var url = "InterfazAjax.aspx?Comando=CONTACTO&Nombre=" + Nombre + "&Mail=" + Mail;
            url = URL_Completo(url);
            //url = "http://200.47.31.26/cotizacionBR/InterfazAjax.aspx?Comando=CONTACTO&Nombre=" + Nombre + "&Mail=" + Mail;
            ajax.open("GET", url, true);
            var vez = 0;
            ajax.onreadystatechange = function() {
                if (ajax.readyState == 4 && ajax.status == 200) {
                    //aca hago show con mensaje OK
                    MostrarPopUp();
                    //alert("Contato cadastrado corretamente");
                }
                else {
                    if (vez = 0) {
                        alert("oh god why");
                        vez = 1;
                    }


                }
            }
            document.getElementById("nombre").value = "";
            document.getElementById("mail").value = "";

            ajax.send(null); //Hacer la conección
        }
        else {
            MostrarPopUpRellenarCampos();
            
        }

    }




</script>

