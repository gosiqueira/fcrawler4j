for ( i = 0; i < leftOver.length; i++ ){
// if the old rec does not have a price, replace it with a new hotel
rec = oldRecsContainer[leftOver[i]];
for (recId in newRecsContainer){
if (newRecsContainer[recId].replaced === false){
showDivWithAnimation(rec.body,newRecsContainer[recId].body);
showDivWithAnimation(rec.priceBtn, newRecsContainer[recId].priceBtn);
newRecsContainer[recId].replaced = true;
rec.replaced = true;
break;
}
}
// do not have enough replacements
if (!rec.replaced){
return false;
}
}
return true;
}else{
//no rec has availability at all
return false;
}
}
