                    <ul class="cssn_form">
                                                                                                            <li class="box checked-box">
                                    <input name="frequency" id="frequency_weekly" value="weekly" type="radio"/>
                                    <label for="frequency_weekly">Hebdomadaire</label>
                                </li>
                                                            <li class="box checked-box">
                                    <input name="frequency" id="frequency_monthly" value="monthly" type="radio"/>
                                    <label for="frequency_monthly">Mensuelle</label>
                                </li>
                                                            <li class="box checked-box">
                                    <input name="frequency" id="frequency_both" value="both" type="radio" checked="checked"/>
                                    <label for="frequency_both">Les deux</label>
                                </li>
                                                                            <li class="box input-text">
                            <input class="input-text required-entry validate-email" name="mail-address" id="mail-address" type="text" placeholder="Adresse email"/>
                        </li>
                    </ul>
                    <div class="button-set-lb">
                        <button type="submit" class="button le-button" title="S'inscrire" name="send">S'inscrire</button>
                    </div>
                </form>
                <script>
                    var lightboxNewsletterForm = new VarienForm('newsletter-lightbox-form');
                    function lightboxNewsletterSubmit() {
                        if (lightboxNewsletterForm.validator.validate()) {
                            jQuery.ajax({
                                type: "post",
                                dataType: "json",
                                url: "http://www.colette.fr/newsletters-archives/index/post",
                                data: jQuery('#newsletter-lightbox-form').serialize(),
                                success: function (data) {
                                    manageAjaxMessage(data.content);
                                }
                            });
                        }
                        return false;
                    }
                </script>
        </div>
    </div>
</div>

</div>
</body>
</html>
