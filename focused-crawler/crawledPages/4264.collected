<script type="text/javascript">
(function(){
	
	//activar tab passagem
	
	$('.tabs .tab-flights').trigger('click');
	
	var _body = $("html, body");
	var _ctn_main_ofertas_class = ".catalogo";
	var _hidden_ofertas_class = ".ofertas-escondidas";
	var _btm_show_class  = '.btm-ver-ofertas';
	var _btm_show =  $(_btm_show_class);
	
	

	_btm_show.on('click',function(e){
		var $this = $(this);
		var _sibling_hidden_ofertas = $this.siblings(_hidden_ofertas_class);
		var _ctn_main = $this.closest(_ctn_main_ofertas_class); // container principal
		
		
		var _ctn_main_siblings_btm_show = _ctn_main.siblings(_ctn_main_ofertas_class).find(_btm_show_class);
		var _ctn_main_sblings_hidden_ofertas = _ctn_main.siblings(_ctn_main_ofertas_class).find(_hidden_ofertas_class);
		
		$this[0].style.display = 'none'; // desativa o btm clicado
		
		_sibling_hidden_ofertas.fadeIn(300);// ativa o container com as ofertas escondidas
		_ctn_main_sblings_hidden_ofertas.hide(); // desativa todas as caixas de ofertas escondidas que estao ativas
		
		_ctn_main_siblings_btm_show.show(); // ativa todos os btm que já foram escondidos
		


		var _offset_top_main = _sibling_hidden_ofertas.offset().top - 100;
		_body.animate({scrollTop:_offset_top_main},'1000');
		
		//console.log(_offset_top_main);
		});
		
	
	})(jQuery);
</script>

