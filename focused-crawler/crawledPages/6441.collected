			if (typeof s_exp_trackClick !== 'undefined') {
				s_exp_trackClick(this,'a','E3.CreateAccount.FB.Connect');
			}
			
			fbtk.login('email', function(response) {
                var options = {
                    provider: 'Facebook',
                    referrerUrl: referrerURL,
                    successUrl: successURL,
                    modal: 'modal'
                }
				authtk.showConnectModal(options);
			});
		};
	}

