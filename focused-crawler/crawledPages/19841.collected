var define = ta.prwidgets.define.bind(ta.prwidgets,'common_ratings_histogram','handlers.js');
define(["widget"], function(widget) {
function _areLabelsLong() {
var labels = ta.select('.row_label', widget.element);
for (var i = 0; i < labels.length; i++) {
if (labels[i].scrollWidth > labels[i].clientWidth) {
return true;
}
}
return false;
}
if (_areLabelsLong()) {
widget.element.addClass('long_labels');
}
return {
click: function(index, label) {
ta.setEvtCookie('Reviews_Summary', 'traveler_rating', label.toLowerCase(), 0, window.location.href);
document.forms.REVIEW_FILTER_FORM.filterRating.value=index;
document.forms.REVIEW_FILTER_FORM.submit();
}
};
});})();
if (ta.prwidgets) {
ta.prwidgets.initWidgets(document);
}
/* ]]> */
</script>
