        $(document).ready(function() {
           //$("a[id='nyroModalId']").nyroModal();
            checkPromoCodes();
        });
        
        
        
        function applyPromoCode(elem) {
        	
            if (Viator.submitted) {
                return false;
            }

            Viator.submitted = true;
            //$('#applyPromoCode').addClass('btn-inactive');

            var temp = document.getElementById('promo_code').value,
            $form = $(elem).closest("form");

            var filteredValue = filterString(temp).toUpperCase();
            document.getElementById('promo_code').value = filteredValue;

            $form.attr('action', '/booking/applyPromoCode.jspa');
            $form.submit();

            return false;
        }

        function removePromoCode(elem) {
            if (Viator.submitted) {
                return false;
            }

            Viator.submitted = true;

            var $form = $(elem).closest("form");

            $('#promo_code').prop('disabled',false);
            $('#promo_code').val("");

            $form.attr('action', '/booking/applyPromoCode.jspa');
            $form.submit();

            return false;
        }
        
        function filterString(strValue) {
            return strValue.replace(/[\+\/\?%&#\s]*/g,"");
        }
        
        function checkPromoCodes() {
            var $promoUrl = $("#promoUrl");
            if ($promoUrl.val() != "") {
                $.nmManual($promoUrl.val());
            }
        }        
    </script>

