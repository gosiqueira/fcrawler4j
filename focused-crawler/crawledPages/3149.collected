		var createCookieFromTrackerId = function(trackerId) {
			var date = new Date;
			date.setTime(date.getTime() + 31536000000); //one year un ms
            var expires = "; expires=" + date.toGMTString();
			document.cookie = "trackerid=" + trackerId + expires + "; path=/"
		};
		
		var getTrackerIdFromCookie = function() {
			var nameEQ = "trackerid=";
			var ca = document.cookie.split(";");
			for (var i = 0; i < ca.length; i++) {
				var c = ca[i];
				while (c.charAt(0) == " ") {
					c = c.substring(1, c.length)
				}
				if (c.indexOf(nameEQ) == 0) {
					return c.substring(nameEQ.length, c.length)
				}
			}
			return null
		};
		
		if(!getTrackerIdFromCookie()) {
			createCookieFromTrackerId("9b745aca-1ac1-43c8-b45a-ca1ac143c86f");
		}

	</script>



