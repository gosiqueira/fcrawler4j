        <link rel="stylesheet" href="//partner.vtrcdn.com/assets/css/skin/vml/viacon_url.css?puid=8328&v=release_TSM_2.12.0-20160118.005.3304d88c" media="all" type="text/css" rel="stylesheet"  />
        <script>
            // TSM-2915 : rewrite url for viacon for CORS - Font file should not be cached across different VML/Partners
            var styleSheets = document.styleSheets;
            var cssFontStr = '';
            var isIE8 = false;
            var hostUrl = "";

            for(var a in styleSheets){
                if(Object.prototype.hasOwnProperty.call(styleSheets,a) && styleSheets[a].href != null
                        && styleSheets[a].href.indexOf('viacon_url')>0){
                    var sSheets = styleSheets[a];
                    hostUrl = sSheets.href;
                    var cssRules = sSheets.cssRules? styleSheets[a].cssRules : sSheets.rules;
                    for(var b in cssRules) {
                        if(!!window.CSSFontFaceRule){
                            if(Object.prototype.hasOwnProperty.call(cssRules,b)
                                    && cssRules[b] instanceof CSSFontFaceRule){
                                cssFontStr = cssRules[b].cssText;
                                sSheets.deleteRule(b);
                            }
                        }else{
                            // This is IE < 8
                            isIE8 = true;
                            cssFontStr = sSheets.cssText;
                        }
                    }
                }
            }

            // IE returns CSS url as it is defined in viacon_url.css file, while modern browsers return with abs url.
            var replaceStr = '../../..';
            var reg = new RegExp(replaceStr, 'g');
            hostUrl = hostUrl.replace('http://', '').replace('https://', '').replace('//', '');
            hostUrl = hostUrl.substring(0, hostUrl.indexOf('/'));
            cssFontStr = cssFontStr.replace(reg,'//'+hostUrl+'/assets');

            // Add puid to all font file request urls.
            cssFontStr = cssFontStr.replace('.eot?#iefix','.eot?puid=8328#iefix');
            cssFontStr = cssFontStr.replace('.eot)','.eot?puid=8328)');
            cssFontStr = cssFontStr.replace('.woff','.woff?puid=8328');
            cssFontStr = cssFontStr.replace('.ttf','.ttf?puid=8328');
            cssFontStr = cssFontStr.replace('.svg#viacon','.svg?puid=8328#viacon');

            var css = document.createElement("style");
            css.type = "text/css";

            if(isIE8 && !!css.styleSheet){
                // IE8 doesn't have innerHTML for style node.
                css.styleSheet.cssText = cssFontStr;
            } else {
                css.innerHTML = cssFontStr;
                css.appendChild(document.createTextNode("")); // for webkit
            }

            document.head = document.head || document.getElementsByTagName('head')[0];
            document.head.appendChild(css);
        </script>
