<script type="text/html" id="new-alert-template" >
<div>
<div class="price-alert">
    <div class="popup-body">
			<div class="tooltip-container">
				<div class="tooltip-content">

	<div class="new-alert basic-form {{if product}}${product}-{{else}}generic-{{/if}}alert" id="alert-price-popup">
	{{if product }}
			{{if  typeof modify != "undefined" && modify == true}}

				{{if typeof renew != "undefined" && renew  == true}}
					<h3 class="typo-title-bg">Renovar meu alerta de {{if product == 'fl' }}voos{{else product == 'ho' }}hotéis {{else product == 'pkg' }}pacotes {{/if}}
				{{else}}
					<h3 class="typo-title-bg">Modificar meu alerta de {{if product == 'fl' }}voos{{else product == 'ho' }}hotéis {{else product == 'pkg' }}pacotes {{/if}}
				{{/if}}

			{{else}}
				<h3 class="typo-title-bg">Criar meu alerta de {{if product == 'fl' }}voos{{else product == 'ho' }}hotéis {{else product == 'pkg' }}pacotes {{/if}}
			{{/if}}

		<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
		<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
		{{if typeof adviceId != 'undefined' && adviceId == 'CUSTOM'}}
			<p class="nibbler-advice">${adviceText}</p>
		{{/if}}
	{{else typeof from != 'undefined' && from == "newsletter"  }}
	 	{{if typeof adviceId != 'undefined'}}
			{{if adviceId == 'UPDATED_SUBSCRIPTION'}}
				<h3 class="typo-title-bg">Já estava inscrito em nossas ofertas<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">O endereço de e-mail ${mail} já está inscrito em nossas ofertas.</p>

			{{else adviceId == 'ALREADY_SUBSCRIBED'}}
				<h3 class="typo-title-bg">Já estava inscrito em nossas ofertas<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">O endereço de e-mail ${mail} já está inscrito em nossas ofertas. Já revisou a Caixa de Spam?</p>

			{{else adviceId == 'SENT_CONFIRMATION'}}
				<h3 class="typo-title-bg">Obrigado por inscrever-se a decolar.com<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">Confirme sua inscrição pelo e-mail enviado a ${mail}. Se você não encontrar este e-mail verifique a Caixa de Spam.</p>
			{{else adviceId == 'SENT_THANKS'}}
				<h3 class="typo-title-bg">Obrigado por assinar a decolar.com!<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
				<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
					<p class="nibbler-advice">A partir de agora você receberá as melhores ofertas exclusivas em sua caixa ${mail}</p>
			{{/if}}
		{{else}}
			<h3 class="typo-title-bg">Obrigado por assinar a decolar.com!<img src="http://br.staticontent.com/nibbler-versioned/1.5.35/Nibbler/PopupAlert/img/alert-bell.png" alt="" class="title-icon" /></h3>
			<div class="alert-container {{if allOptions}}empty{{/if}} {{if exactDays}}exact{{else}}aprox{{/if}}-dates-option">
		{{/if}}

