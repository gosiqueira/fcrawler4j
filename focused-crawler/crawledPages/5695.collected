<script id="expired-hotel-template" type="text/x-handlebars-template">
  <li id="{{domId}}" class="history-item-wrapper hotel-detail">  
    <article class="history-item ">
      <figure class="aspect-ratio4-3 image card-photo" data-media-type="image" data-lazy="true" data-late="false" data-downgrade="false" data-src="{{imagePath}}" data-alt="{{imageDescription}}" data-aspect-ratio="4-3" data-class="tile-media">
        <img alt="{{imageDescription}}" class="tile-media" src="{{imagePath}}">
      </figure>

      <div class="item-body">
        <div class="item-details-container ">
          <div class="item-info">
            <h4 class="item-title truncate">{{itemTitle}}</h4>
            <strong class="star-rating rating-secondary star rating hide-600 small-hotel-card">
              <span class="visuallyhidden">{{starsDecimal}}out of 5</span>
              <span class="icon icon-stars{{starsDash}} stars-black value-title" aria-hidden="true"></span>
            </strong>
            <div class="card-details city-name hide-600">{{cityName}}</div>
          </div>
          <div class="card-price-details priced">
            <div class="card-price old">
              <span class="small"></span><span class="amount">{{lastPrice}}</span>
            </div>
            <div class="card-details price-age">{{lastViewedDate}}</div>
          </div>
        </div>
      </div>

      <a href="{{detailsClickthroughURL}}" class="item-link">
        <span class="visuallyhidden">{{linkDesciption}}</span>
      </a>

      <button type="button" data-control="remove" data-target-id="{{domId}}" class="btn-close btn-primary" id="delete-{{domId}}" >
        <span class="icon icon-close" aria-hidden="true"></span><span class="alt">{{removeButtonDescription}}</span>
      </button>

    </article>

    <div class="below has-notes">
      <div class="notes-container pre-filled" data-itemid="{{domId}}" >
        <div class="new-notes-wrapper">
          <button type="button" class="btn-utility btn-primary add-notes-button">
            <span class="btn-label"><span class="visuallyhidden">{{addNoteButtonDescription}}</span><span aria-hidden="true">{{addNoteButtonString}}</span></span>
          </button>
        </div>
        <div class="notes-display-wrapper">
          <div class="notes-text">{{noteValue}}</div>
          <button type="button" class="btn-utility btn-primary edit-notes-button">
            <span class="btn-label"><span class="visuallyhidden">{{editNoteButtonDescription}}</span><span aria-hidden="true">{{editNoteButtonString}}</span></span>
          </button>
        </div>
        <div class="notes-edit-wrapper">
          <label class="notes-label text">
            <span class="label visuallyhidden">{{noteEntryDescription}}</span>
            <textarea placeholder="{{addNotePlaceholder}}" class=" notes-entry" maxlength="1200">{{noteValue}}</textarea>
          </label>
          <button type="button" class="btn-sub-action btn-secondary save-notes-button">
            <span class="btn-label"><span class="visuallyhidden">{{saveNoteButtonDescription}}</span><span aria-hidden="true">{{saveNoteButtonString}}</span></span>
          </button>
          <button type="button" class="btn-action btn-primary cancel-notes-button">
            <span class="btn-label"><span class="visuallyhidden">{{cancelButtonDescription}}</span><span aria-hidden="true">{{cancelButtonString}}</span></span>
          </button>
          <div class="notes-messages">
            <div class="chars-remaining"></div>
          </div>
        </div>
      </div>
    </div>
  </li>
</script>
