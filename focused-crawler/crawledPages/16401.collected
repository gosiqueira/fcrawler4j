<script type="text/javascript" language="javascript">

    $(document).ready(function () {
        var form = $('#loginCustomer');

        form.validate({
            ignore: [],
            onkeyup: false,
            onfocusout: false,
            focusCleanup: false,
            rules: {
                SSOCustomerAccountLoginView$TextBoxUserID : "required",
                SSOCustomerAccountLoginView$PasswordFieldPassword: "required"
            },
            messages: {
                SSOCustomerAccountLoginView$TextBoxUserID: "Inserisci il tuo Username (E-mail di registrazione)",
                SSOCustomerAccountLoginView$PasswordFieldPassword : "Inserisci la tua password",

            },
            errorPlacement: function (error, element) {
                if (!element.hasClass('valid')){
                    // qTip call
                    $(element).parent().addClass('error-qtip-field-sitecore').qtip({
                        show: {
                            ready: true
                        },
                        content: eval(error),
                        position: {
                            my: 'bottom center',
                            at: 'top center'
                        },
                        style: {
                            classes: 'qtip-error'
                        }
                    });
                }
            },
            showErrors: function () {
                this.defaultShowErrors();
                removeErrorValidElement();
                //setTimeout(function () { removeErrorValidElement(); }, 1);
                setTimeout(function () { removeErrorAllElement(); }, 4000);
            },
            success: function (error) {
                // Hide tooltips on valid elements
                //setTimeout(function () {
                //    //form.find('.valid').parent().removeClass('error-qtip-field-sitecore').qtip('destroy');
                //    removeErrorValidElement();
                //}, 1);
            },
            submitHandler: function (form) {
                openPleaseWaitBox();
                form.submit();
            }
        });
    });

</script>



