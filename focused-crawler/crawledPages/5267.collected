        e.preventDefault();
        e.stopPropagation();

        var $target = $(e.target).closest('.alert-item');

        $target
            .find('.close-button')
            .remove();

        $target
            .find('.alert-message')
            .html('Lähetämme Scratchpad-päivitykset sähköpostiisi. Tarkista saapuneet kansiosi!');
        
        if (Scratchpad.state.loggedInUser) {
