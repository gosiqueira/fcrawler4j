        <div class="form-column" id="location-arr">
          <label for="arrCity-flight">Destino</label>
          <input name="arrCity" id="arrCity-flight" type="text" class="highlight disabled over" size="20" maxlength="50" disabled="disabled">
          <span class="ajax-loader dep"><img src="/pictures/ajax-loader.gif"></span> </div>
        <div class="clearer"></div>
        </fieldset>
        <!-- Dates -->
        <fieldset class="dates">
        <div id="calendar-dep-flight" class="form-column w45">
          <label>Ida</label>
          <input name="depDate" type="text" class="highlight disabled" readonly="readonly" value="dd/mm/aaaa" size="16" maxlength="10" id="depDate-flight" disabled="disabled">
