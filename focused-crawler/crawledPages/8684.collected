        <div id="calendar-ret-flight" class="form-column w45">
          <label>Volta</label>
          <input name="retDate" type="text" class="highlight disabled" readonly="readonly" value="dd/mm/aaaa" size="16" maxlength="10" id="retDate-flight" disabled="disabled">
          <span class="calicon-link" id="showCalRet-flight"> <img src="/pictures/icoCalendar.gif" width="18" height="17" border="0" alt="Elige una fecha de Retorno"> </span> </div>
        </fieldset>
        <div class="clearer"></div>
