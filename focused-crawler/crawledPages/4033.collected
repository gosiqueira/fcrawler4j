/**
* The overlays may be shown before the ad loads.
* The ad height may change, causing the page to shift down and the overlays to be in the wrong place.
* Handle this case.
*/
function adjustJFYOverlayPositionsOnAdLoad() {
if (window.googletag && googletag.cmd) {
googletag.cmd.push(function(){
googletag.pubads().addEventListener('slotRenderEnded', function(evnt) {
// there won't be any visible overlays if the namespace isn't loaded
if (ta.overlays) ta.overlays.Factory.adjustJFYOverlayPositions(evnt);
});
});
}
}
/**
* show/hide the full just for you reason
*/
function toggleJfy(event, elmt) {
var shortReason = elmt.getParent('.reason .short');
var fullReason = elmt.getParent('.reason .full');
if (shortReason && fullReason) {
if (elmt.hasClass('more')) {
// more
shortReason.addClass('hidden');
fullReason.removeClass('hidden');
}
else {
// less
shortReason.removeClass('hidden');
fullReason.addClass('hidden');
}
}
}
