// figuring out where to put the imperfect header gets very complicated in after vr rap sorting/reshuffling
// use js after the fact to figure out where it should go and inject it there
function placeImperfectHeader() {
var imperfectHeader = ta.select('.p13n_other_hotels_wrapper')[0];
if(!imperfectHeader) {
return;
}
var allListings = ta.select('#ACCOM_OVERVIEW .listing');
var nPerfects = ta.select('#ACCOM_OVERVIEW .listing .sprite-match_icon_v2').length;
// if we have some perfect listings, but also some that aren't put the imperfect header after the last perfect
