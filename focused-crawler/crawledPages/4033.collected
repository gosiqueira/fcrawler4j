var key = trackingString + '_loaded';
var firstInstance = !ta.has(key);
if (firstInstance && numPages) {
ta.store(key, true);
ta.queueForLoad(function(){
ta.util.element.trackWhenScrolledIntoView(paginationClass, [trackingString+'_VISIBLE', 'numPages', numPages, 0]);
}, "init /Hotels pagination tracking for widget: " + trackingString);
}
return {};
});})();
