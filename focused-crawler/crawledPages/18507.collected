<head>
    <title>Appelez l'international avec votre carte SIM |Lebara Mobile</title> 
<meta name="title" content="Appelez l'international avec votre carte SIM |Lebara Mobile"/> 
<meta name="keywords" content="appels internationaux, carte SIM, Lebara, pre-payé, appels nationaux"/> 
<meta name="description" content="Appels Nationaux et Internationaux à prix low cost avec la carte SIM Lebara Mobile. Profitez de nos offres dès aujourd'hui."/> 

	
 




<script type="text/javascript">
var isTopupInfoExists =  'false';

var price='';
var pageNameFromURL= 'http://www.lebara.fr/pages/responsive/pt_li_generic_empty_main_responsive.jsp';
var splitPageName=pageNameFromURL.split("/");
var pageLabel='';
var pageName=splitPageName[splitPageName.length-1];
var orderValue = '';
var productPrice = '';
var amount = '0';
var navigationType = '';
var MerchantReferenceNumber = '';
var isocode = 'FR'
if(isocode == 'GB')
{
	if(price!=null || price!="") {
		var currencySymbolRemoved= price.substr(1,price.length);
		orderValue = currencySymbolRemoved;
		productPrice = currencySymbolRemoved;
	}
}
if(isocode == 'DE' || isocode == 'ES' || isocode == 'FR' || isocode == 'NL')
{
	if(price!=null || price!="") {
		var currencySymbolRemoved= price.substr(0,price.length-1);
		orderValue = currencySymbolRemoved;
		productPrice = currencySymbolRemoved;
	}
}
if(isocode == 'PL')
{
	if(price!=null && price!="") {
		var currencySymbolRemoved= price.substr(0,price.length-2);
		orderValue = currencySymbolRemoved;
		productPrice = currencySymbolRemoved;
	}
}
if(isocode == 'DK')
{
	if(price!=null && price!="") {
		var currencySymbolRemoved= price.substr(0,price.length-2);
		orderValue = currencySymbolRemoved;
		productPrice = currencySymbolRemoved;
	}
}
var pageURL = location.href;
var productId = '';
var productSKU = '';
var productName = '';
var simSelected = 	'';
if(isocode == 'FR' || isocode == 'GB');
{
	if( productName == 'Topup')
	{
	 	productName = 'Registered';		
	}
	else if (simSelected.toString() == 'true' && productName == 'UnregisteredTopup') 
	{
		productName = 'PaidSim'	;
	}
}
var orderNumber = '';                                        
var productCategory = '';
var quantity = '1';
var searchTerm = '';
var topupType = '';
var loggedIn;
var customerAccountId =''; //email id
var voucherCode =''; // promotion code
var taxAmount = ''; // always empty for lebara
var purchaseType=''; // not yet decided
var voucherCode = '';

if(customerAccountId == null || customerAccountId ==''){
	loggedIn = "no";
}else{
	loggedIn = "yes";
}

if(isocode == 'DK') {
	/*DK Specific Parameters Free SIM*/
 	orderNumber = '';
 	
 	/*Check for other orderNumber in PRCC */
 	if(!(orderNumber != "")) {
 		orderNumber='';
 	}
 	
 	/*DK Specific Parameters Free SIM*/
 	if('false'=='true'){
 	if('false'=='true' && 'false'=='true') {
 	   if(''!='FREE'){
 	 	  productCategory = 'paid_sim';
 	 	  productPrice = '';
 	 	  orderValue = '';
 	 	  productId='';
 	 	 productName='';
 	 	} 
 	   else{
 	      productCategory = 'free_sim';
 	 	  productPrice = '0';
 	 	  orderValue = '0';
 	 	  productId='sim_2';
 	 	  productName='Free';
 	   }
 	}
 	}

 	  else if(''=="instantTopup"){
 	      productCategory = 'instantTopup';
 	 	  productPrice = '';
 	 	  orderValue =  '';
 	 	 productId= '';
 	 	 productName='';
 	   }
 	  else if(''=="autoTopup"){
 	     productCategory = 'autoTopup';
 	    productPrice = '';
	 	  orderValue =  '';
	 	 productId= '';
	 	 productName='';
 	  }
 	  else  if(''!=null && ''=="MBB"){
 	 	    productCategory = 'data';
 	 	    productPrice = '49';
 		 	  orderValue =  '49';
 		 	 productId= '49_lebaraMobileData';
 		 	 productName='49_lebaraMobileData';
 	 	 }
 	 else  if(''!=null && ''=="TakeAway"){
	 	    productCategory = 'Take_Away';
	 	    productPrice = '49';
		 	  orderValue =  '49';
		 	 productId= '49_lebaraMobileData';
		 	 productName='49_lebaraMobileData';
	 	 }
 	 else  if(''!=null && ''=="Roaming"){
	 	    productCategory = 'Roaming';
	 	    productPrice = '';
		 	orderValue =  '';
		    productId= 'roaming_49';
		 	productName='roaming_49';
		 	orderNumber='';
	 	 }
	else {
		 
		 }
 	
 	/*Check for other productPrice in DK PRCC */
	if(productPrice == "") {
		orderValue = amount;
		productPrice = amount;
	}
	voucherCode = '';
	quantity = '1' ;
	
	
	var talkCountryCode = '';
	var talkCurrencyCode = '';
	
}
