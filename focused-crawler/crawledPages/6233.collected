        e.preventDefault();
        e.stopPropagation();

        var $target = $(e.target).closest('.alert-item');

        $target
            .find('.close-button')
            .remove();

        $target
            .find('.alert-message')
            .html('Nous vous enverrons les mises à jour de Scratchpad par e-mail. Consultez votre boîte e-mail !');
        
        if (Scratchpad.state.loggedInUser) {
            uitk.utils.liveAnnounce('Nous vous enverrons les mises à jour de Scratchpad par e-mail. Consultez votre boîte e-mail !');
        }

        if (window.uitk 
        && window.uitk.utils 
        && window.uitk.utils.focusElement) {
            uitk.utils.focusElement($target);
        }

        if (window.AlertsMenu) AlertsMenu.sendeVar28('Header.Notif.SP.OptIn');
        if (window.Scratchpad) Scratchpad.enrollInNudge();
