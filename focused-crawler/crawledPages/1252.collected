            function updateQueryStringParameter(uri, key, value) {
                var re = new RegExp("([?|&])" + key + "=.*?(&|$)", "i");
                separator = uri.indexOf('?') !== -1 ? "&" : "?";
                if (uri.match(re)) {
                    return uri.replace(re, '$1' + key + "=" + value + '$2');
                }
                else {
                    return uri + separator + key + "=" + value;
                }
            }

            this.prepareStartSearchDetail = function () {
        
                var searchEngine = $("#search_engine");                
                searchEngine.searchEngine("setValidationInputDestination", true);

                if (searchEngine.searchEngine("validate")) {

                    var data = searchEngine.searchEngine("getData");
                    
                    var location = data.Hotel.Location.removeDiacritics().split("-")[0];
                    var locationId = data.Hotel.LocationId;
                    var dateCheckIn = data.Hotel.CheckIn;
                    var dateCheckOut = data.Hotel.CheckOut;
                    var rooms = data.Hotel.RoomsRequest.length;
                    var adults = '';    
                    var children = '';
                    var childrenAges = '';                    

                    for (var i = 0; i < data.Hotel.RoomsRequest.length; i++) {
                        var room = data.Hotel.RoomsRequest[i];
                        
                        if(adults != '')
                            adults = adults + ',';
                        if(children != '')
                            children = children + ',';

                        adults = adults + room.Adults;                        
                        children = children + room.ChildAges.length;                         

                        var childrenAgeByRoom = '';
                        for (var j = 0; j < room.ChildAges.length; j++) {
                            if(childrenAgeByRoom != '')
                                childrenAgeByRoom = childrenAgeByRoom + ',';

                            childrenAgeByRoom = childrenAgeByRoom + room.ChildAges[j];
                        }   
                                         
                        childrenAges = childrenAges + childrenAgeByRoom;               
                        childrenAges = childrenAges + ';';       
                    }

                    childrenAges = childrenAges.substring(0, childrenAges.length - 1);

                    var lastUrl = Global.URLSite + "travel/resultado-hoteis.aspx?searchtype=Hotel&Location=&LocationId=&CheckIn=&CheckOut=&IsSimplifiedSearch=false&Rooms=&Adults=&Children=&ChildAges=&OrderBy=&FindHotelName=false&DataConsulta=&Nav=";
                    
                    var queryString = lastUrl;
                    queryString = updateQueryStringParameter(queryString, 'Location', location);
                    queryString = updateQueryStringParameter(queryString, 'LocationId', locationId);
                    queryString = updateQueryStringParameter(queryString, 'CheckIn', dateCheckIn.Day + '/' + dateCheckIn.Month + '/' + dateCheckIn.Year);
                    queryString = updateQueryStringParameter(queryString, 'CheckOut', dateCheckOut.Day + '/' + dateCheckOut.Month + '/' + dateCheckOut.Year);
                    queryString = updateQueryStringParameter(queryString, 'Rooms', rooms);
                    queryString = updateQueryStringParameter(queryString, 'Adults', adults);
                    queryString = updateQueryStringParameter(queryString, 'Children', children);
                    queryString = updateQueryStringParameter(queryString, 'ChildAges', childrenAges);                    
                    queryString = updateQueryStringParameter(queryString, 'DataConsulta', new Date());
                    queryString = updateQueryStringParameter(queryString, 'Nav', this._getBrowser());
                    window.location.replace(queryString);

                }
            };

            this._getBrowser = function() {
                try {
                    var ua = navigator.userAgent, tem,
                    M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
                    if (/trident/i.test(M[1])) {
                        tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
                        return 'IE ' + (tem[1] || '');
                    }
                    if (M[1] === 'Chrome') {
                        tem = ua.match(/\bOPR\/(\d+)/)
                        if (tem != null) return 'Opera ' + tem[1];
                    }
                    M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
                    if ((tem = ua.match(/version\/(\d+)/i)) != null) M.splice(1, 1, tem[1]);
                    return M.join(' ');
                }
                catch (ex) {
                    return "";
                }
            }

