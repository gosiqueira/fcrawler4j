                var roomContainer = $('.search-engine-roomContainer', motor);

                var rooms = $(roomContainer).length ? $(roomContainer).addClass('hide') : $(roomContainer).addClass('hide');

                for (var i = 0; i < roomsQuantity; i++) {
                    rooms.eq(i).removeClass('hide');

                    var adultQuantity = data.Hotel.RoomsRequest[i].Adults;

                    var childsAges = data.Hotel.RoomsRequest[i].ChildAges;

                    //Atualiza a quantidade de adultos por quarto.
                    var drpAdults = $("#search2_drpAdultsQtdRoom" + i);

                    drpAdults.find('span').text(adultQuantity);

                    utilFunctions.refreshValuesMotor($(drpAdults.find('a')), adultQuantity);

                    var drpChildrens = $("#search2_drpChildrenQtdRoom" + i);

                    $(drpChildrens).find('span').text(childsAges.length);

                    utilFunctions.refreshValuesMotor($(drpChildrens.find('a')), childsAges.length);

                    var childrens = drpAdults.closest('li').find('.child-ages').find('div');
                    //Esconde todas as crianças para mostrar apenas as pedidas
                    childrens.each(function () {
                        $(this).addClass("hide");
                    });

                    drpAdults.closest('li').find('.child-ages').find('span').addClass("hide");

                    if (childsAges.length > 0) {

                        drpAdults.closest('li').find('.child-ages').find('span').removeClass("hide");

