        hotelDetail.calculateTotal = function() {

            var roomCangooroo = $(".radioRoomCangooroo:checked");
            var totalValue = 0;
            var roomRateCode = "";

            roomCangooroo.each ( function(){
                var totalStr = $(this).attr("valueTotalDays");
                totalStr = totalStr.replace(/\./g, '').replace(/\,/g, '.');
                totalValue = totalValue + parseFloat(totalStr);
                
                //console.log(totalStr);           
                    
                if(roomRateCode == "")
                    roomRateCode = $(this).attr("roomRateCode");
                else
                    roomRateCode = roomRateCode + ',' + $(this).attr("roomRateCode");
            });
           
            $('.room_button_buy').attr("roomratecode", roomRateCode);
            $('.price-total').text(formatter.currency(totalValue, true, 2));

            var valorparcelado = totalValue/ 1;
            $('.payment span').text(formatter.currency(valorparcelado, true, 2));

        };

        

        hotelDetail.buy = function(event) {           
            hotelDetail.current.SourceOrigin = "CVC";
            hotelDetail.current.ExternalProperties = JSON.parse("[{\"Key\":\"LAND\",\"Value\":\"LAND\"},{\"Key\":\"destinationLocationMasterCode\",\"Value\":\"" + hotelDetail.getDestinationLocationMasterCode(hotelDetail.current) + "\"}]");
            hotelDetail.current.HotelIdSources = JSON.parse("[{\"Key\":33,\"Value\":\"" + hotelDetail.current.Id + "\"}]");

            var choosedRoomRateCode = $(this).attr('roomratecode').split(',');
            var choosedRooms = new Array;
            var callPreorder = true;
            for (var i = 0; i < choosedRoomRateCode.length; i++) {
                choosedRooms.addRange(hotelDetail.current.Rooms.where(function(room) { return hotelDetail.getRoomRateCode(room) == choosedRoomRateCode[i] }));
            }   		    

            if(!hotelDetail.current.Name)
                hotelDetail.current.Name = "";

            if(!hotelDetail.current.LocationId)
                hotelDetail.current.LocationId = 1198336;

