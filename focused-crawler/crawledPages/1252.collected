        var queryStringFunctions = new (function () {

            var thisFunctions = this;

            this.getQueryString = function (data) {

                var querystring = "";
                var querystringAir = Global.URLSite + "passagens/resultado.aspx";
                var querystringPackage = Global.URLSite + "pacotes/resultado.aspx";
                var querystringHotel = Global.URLSite + "hoteis/resultado.aspx";

                if(Global.SearchType=="3")
                    Global.SearchType="Hotel";

                switch (Global.SearchType) {
                    case "Air": querystring = querystringAir + "?" + thisFunctions.getQueryStringAir(data) + "&SearchType=Air"; break;
                    case "Hotel": querystring = querystringHotel + "?" + thisFunctions.getQueryStringHotel(data) + "&SearchType=Hotel"; break;
                    case "AirHotel": querystring = querystringPackage + "?" + thisFunctions.getQueryStringAirHotel(data) + "&SearchType=AirHotel"; break;
                }

                return querystring.removeDiacritics();
            };

            this.getQueryStringAir = function (data) {

                var querystring = "";

                querystring = querystring + thisFunctions.getQueryStringAirDestination(data);

                querystring = querystring + "&NumADT=" + (data.Air.Adults ? data.Air.Adults : 0);
                querystring = querystring + "&NumCHD=" + (data.Air.Children ? data.Air.Children : 0);
                querystring = querystring + "&NumINF=" + (data.Air.Babies ? data.Air.Babies : 0);

                if (data.Air.RequestData.CityPairsRequest[0] != undefined && data.Air.RequestData.CityPairsRequest[0].EndTime != undefined)
                    querystring = querystring + "&Hora=" + (data.Air.RequestData.CityPairsRequest[0].EndTime - 1).toString();
                else
                    querystring = querystring + "&Hora=";


                if (data.Air.RequestData.CityPairsRequest[1] != undefined && data.Air.RequestData.CityPairsRequest[1].EndTime != undefined)
                    querystring = querystring + "&Hora=" + (data.Air.RequestData.CityPairsRequest[1].EndTime - 1).toString();
                else
                    querystring = querystring + "&Hora=";


                querystring = querystring + "&Multi=false";
                querystring = querystring.replace("undefined", "");
                querystring = querystring.replace("null", "");

                return querystring;
            };

            this.getQueryStringHotel = function (data) {

                var querystring = "";

                if(data.Hotel)
                {
                    data = data.Hotel;
                }

                querystring += "&Location=" + $('#departureLocation').getSelectedLocationName().removeDiacritics().split("-")[0];
                querystring += "&LocationId=" + data.LocationId;
                querystring += "&CheckIn=" + utilFunctions.GetDatePattern(data.CheckIn);
                querystring += "&CheckOut=" + utilFunctions.GetDatePattern(data.CheckOut);
                querystring += "&Rooms=" + data.RoomsRequest.length;
                querystring += "&Adults=" + utilFunctions.GetAdults(data.RoomsRequest);
                querystring += "&Children=" + utilFunctions.GetChildren(data.RoomsRequest);
                querystring += "&ChildAges=" + utilFunctions.GetChildAges(data.RoomsRequest);

                return querystring;
            };

            this.getQueryStringAirHotel = function (data) {

                var querystring = "";

                querystring = querystring + thisFunctions.getQueryStringAirDestination(data);

                if (data.Air.RequestData.CityPairsRequest[0] != undefined && data.Air.RequestData.CityPairsRequest[0].EndTime != undefined)
                    querystring = querystring + "&HoraIda=" + (data.Air.RequestData.CityPairsRequest[0].EndTime - 1).toString();
                else
                    querystring = querystring + "&HoraIda=";

                if (data.Air.RequestData.CityPairsRequest[1] != undefined && data.Air.RequestData.CityPairsRequest[1].EndTime != undefined)
                    querystring = querystring + "&HoraVolta=" + (data.Air.RequestData.CityPairsRequest[1].EndTime - 1).toString();
                else
                    querystring = querystring + "&HoraVolta=";

                querystring = querystring + "&NumADT=" + (data.Air.Adults ? data.Air.Adults : 0);
                querystring = querystring + "&NumCHD=" + (data.Air.Children ? data.Air.Children : 0);
                querystring = querystring + "&NumINF=" + (data.Air.Babies ? data.Air.Babies : 0);

                querystring += thisFunctions.getQueryStringHotel(data);
                querystring += "&RoundTrip=" + (data.Air.IsRoundTrip == true ? "1" : "0");
                querystring += "&NonStop=" + (data.Air.NonStop == true ? "1" : "0");
                querystring += "&AnotherDate=" + (data.Hotel.AnotherDate == true ? "true" : "false");

                querystring += "&Cabine=" + data.CabinFilter;
                querystring = querystring.replaceAll("undefined", "");
                querystring = querystring.replaceAll("null", "");

                return querystring;
            };

            this.getQueryStringAirDestination = function (data) {

                var querystring = "";

                if (data.Air.Destinations) {
                    for (var i = 0; i < data.Air.Destinations.length; i++) {
                        querystring = querystring + "&Origem=" + data.Air.Destinations[i].Origin.removeDiacritics().replaceAll(" ", "%20").replaceAll("(", "%28").replaceAll(")", "%29");
                        querystring = querystring + "&Destino=" + data.Air.Destinations[i].Destination.removeDiacritics().replaceAll(" ", "%20").replaceAll("(", "%28").replaceAll(")", "%29");

                        if (data.Air.IsRoundTrip) {
                            querystring = querystring + "&Origem=" + data.Air.Destinations[i].Destination.removeDiacritics();
                            querystring = querystring + "&Destino=" + data.Air.Destinations[i].Origin.removeDiacritics();
                        }

                        var proximityId = $('.txtDestination').getSelectedLocationIdProximity();
                        var proximityName = $('.txtDestination').getSelectedLocationNameProximity().removeDiacritics();

                        querystring = querystring + "&Proximity=" + proximityName
                        querystring = querystring + "&ProximityId=" + (proximityName ? proximityName : 0);
                        querystring = querystring + "&Data=" + utilFunctions.GetDatePattern(data.Air.Destinations[i].OutboundDate);

                        if (data.Air.Destinations[i].InboundDate)
                            querystring = querystring + "&Data=" + utilFunctions.GetDatePattern(data.Air.Destinations[i].InboundDate);

                        if (data.Air.Destinations[i].CiaCodes.length > 0)
                            querystring = querystring + "&Cia=" + data.Air.Destinations[i].CiaCodes[0];

                        querystring = querystring + "&SomenteDireto=" + data.Air.Destinations[i].NonStop;
                    }
                }

                return querystring;
            };
        });

