                        $('#search_engine').find('#txtCheckIn').animate({ backgroundColor: "#0c83d9", color: "white" }, "slow");
                        $('#search_engine').find('#txtCheckOut').animate({ backgroundColor: "#0c83d9", color: "white" }, "slow");
                        $('#search_engine').find('#txtCheckIn').animate({ backgroundColor: "#fff", color: "#666" }, "slow");
                        $('#search_engine').find('#txtCheckOut').animate({ backgroundColor: "#fff", color: "#666" }, "slow");
                    }
