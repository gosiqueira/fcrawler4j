                hotelDetail.removeLoaderPriceBox();
                hotelDetail.showMessageCancellationPolicies();
            }   
        };

        hotelDetail.showMessageCancellationPolicies = function () {            
            var opt_data = {            
            };            
            var htmlBind = HotelsDetails.infoIndisponibilidade(opt_data);
