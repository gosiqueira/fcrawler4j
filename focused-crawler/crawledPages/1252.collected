            });
            
            $.scrollTo($('#info-indisponibilidade .info'), { axis: 'y', duration: thisFunctions.scrollToTimeSpan });

        };

        hotelDetail.postPage = function(data){
            if(data && data.Hotel && data.Hotel.SourceOrigin != 'CVC')
                Global.Hotel = data.Hotel;

            Global.InterfaceEnvelope.Hotel = Global.Hotel;
            Global.SearchType = "Hotel";
            var continueForm = $("#continueForm");
            var protocol = 'https://';           
            //if(window.location.hostname)
            //    continueForm.attr("action", protocol + window.location.hostname + "/travel/checkout-hoteis.aspx");
            //else
            //    continueForm.attr("action", "/travel/checkout-hoteis.aspx");

            continueForm.attr("action", Global.CheckOutProtocol + window.location.hostname +'/travel/finalizar-compra?product='+Global.SearchType+'&order=' + Util.newGuid());


            $("#InterfaceEnvelope", continueForm).val(Util.stringifyJSON(Global.InterfaceEnvelope));
            $("#Culture", continueForm).val(Global.Culture);
            $("#SearchType", continueForm).val(Global.SearchType);
            $("#ExchangeRate", continueForm).val(Global.ExchangeRate);
            $("#SelectedCurrency", continueForm).val(Global.selectedCurrency);
            $("#QueryString", continueForm).val(Global.QueryString);
            continueForm.submit();
        };
        
        hotelDetail.getReqToPreOrder = function () {
            var numberOfADTs = 0;
            var numberOfCHDs = 0;
            var numberOfINFs = 0;

            if (Global.InterfaceEnvelope.Summary) {
                numberOfADTs = Global.InterfaceEnvelope.Summary.NumberOfADTs ? Global.InterfaceEnvelope.Summary.NumberOfADTs : 0;
                numberOfCHDs = Global.InterfaceEnvelope.Summary.NumberOfCHDs ? Global.InterfaceEnvelope.Summary.NumberOfCHDs : 0;
                numberOfINFs = Global.InterfaceEnvelope.Summary.NumberOfINFs ? Global.InterfaceEnvelope.Summary.NumberOfINFs : 0;
            }

            var req = {
                Culture: Global.Culture,
                FlightGroups: Global.InterfaceEnvelope.Air ? Global.InterfaceEnvelope.Air.FlightGroups : null,
                Hotel: Global.Hotel,
                GiftListQuotes: Global.InterfaceEnvelope.GiftList,
                AirHotelPairRecommendation: Global.InterfaceEnvelope.Recommendation,
                Package: Global.InterfaceEnvelope.Package,
                AffiliatedId: Global.InterfaceEnvelope.affiliatedId,
                SessionId: Global.InterfaceEnvelope.SessionId,
                TravelCardValue: Global.InterfaceEnvelope.TravelCard,
                NumberOfADTs: numberOfADTs,
                NumberOfCHDs: numberOfCHDs,
                NumberOfINFs: numberOfINFs
            };

            return req;
        };

        hotelDetail.getDestinationLocationMasterCode = function(hotel)
        {
            if (hotel.ExternalProperties.destinationLocationMasterCode)
            {
                return hotel.ExternalProperties.destinationLocationMasterCode;
            }
            else
            {
                if (hotel.ExternalProperties && hotel.ExternalProperties.length)
                {
                    return hotel.ExternalProperties.where(function(e){ return e.Key == "destinationLocationMasterCode" })[0].Value;
                }
            }
            return null;
        };

        hotelDetail.getRoomRateCode = function(room) {
            if (room.SelectedRoomOption.ExternalProperties.roomRateCode)
            {
                return room.SelectedRoomOption.ExternalProperties.roomRateCode;
            }
            else
            {
                if (room.SelectedRoomOption.ExternalProperties && room.SelectedRoomOption.ExternalProperties.length)
                {
                    for(var i = 0; i < room.SelectedRoomOption.ExternalProperties.length; i++)
                    {
                        if (room.SelectedRoomOption.ExternalProperties[i].Key == "roomRateCode")
                            return room.SelectedRoomOption.ExternalProperties[i].Value;
                    }
                }
            }
            return null;
        };

        hotelDetail.backToPreviousSearch = function() {            
            var url = '';

            
            url = Util.stringformat(Global.URLSite + 'hoteis/index.aspx');
            
            window.location = url;
        };

        hotelDetail.getHotelData = function(event) {         

            var isFeb = false;

            if($(this).hasClass("feb"))
                var isFeb= true;

            var hotelHashKey = Global.Hotel.HashKey;
            var hotel =  Global.Hotel;
            hotel['Feb'] = hotelIsFeb(hotel);

            if (Util.QueryString('isFeb') == 'true')
                hotel['Feb'] = Util.QueryString('isFeb');

            if ((hotel.Feb) && (Util.getCookie('AgencyId') != 0)) 
                alert("Este Hotel está indisponível, favor entrar em contato com a sua filial de atendimento pra solicitar reserva!");
            
            else {
                var choosedRoomRateCode = $(this).attr('roomratecode').split(',');
                var choosedRooms = new Array;
                for (var i = 0; i < choosedRoomRateCode.length; i++) {
                    choosedRooms.addRange(hotelDetail.current.Rooms.where(function(room) { return hotelDetail.getRoomRateCode(room) == choosedRoomRateCode[i] }));
                }   		    

                var roomHashKeys = [];

                for (var i = 0; i < choosedRooms.length; i++) {
                    roomHashKeys.push(choosedRooms[i].HashKey);
                }

                hotel = _getHotel_WithNewValues(hotelHashKey, roomHashKeys);
                prepareHotelToRender(hotel,isFeb);
            }
        }

        function hotelIsFeb(hotel){
            for (var i = 0; i < arrayOfObjectsSelectedRoomOptionExternalProperties.length; i++) {
                if(arrayOfObjectsSelectedRoomOptionExternalProperties[i].Key == "feb")
                    return arrayOfObjectsSelectedRoomOptionExternalProperties[i].Value == 'S' ?  true : false;
            }
        }

        function prepareHotelToRender(hotel,isFeb) { 
          
            if (!hotel.ExternalProperties.length){
                var externalProperties=[];
           
                for(var propertyName in hotel.ExternalProperties)
                    externalProperties.add({ Key: propertyName, Value:hotel.ExternalProperties[propertyName] });               

                hotel.ExternalProperties=externalProperties;
            }
           
            hotel.SelectedRooms.forEach(function (r) {

                if (!r.SelectedRoomOption.ExternalProperties.length){
                    externalProperties=[];

                    var hasFeb = false;

                    for(var propertyName in r.SelectedRoomOption.ExternalProperties) {
                    

                        if (propertyName == 'feb') {
                            var value = isFeb ? 'S' : r.SelectedRoomOption.ExternalProperties[propertyName];
                        
                            externalProperties.add({ Key: propertyName, Value: value });               
                        
                        } else {
                            externalProperties.add({ Key: propertyName, Value:r.SelectedRoomOption.ExternalProperties[propertyName] });               
                        }
                    }

                    r.SelectedRoomOption.ExternalProperties=externalProperties;
                }
            });

            //Carrega HotelIdSource para efetuar o checkout sem erros
            hotel.HotelIdSources=[]
            hotel.HotelIdSources.add({ Key: hotel.Source, Value: hotel.Id })
           
            hotel.CityNameExtracted = decodeURIComponent(Util.QueryString('Destination'));

            params = {
                item: Util.Clone(hotel),
                ctrl: $('.room_button'),
                interfaceEnvelope: Global.InterfaceEnvelope
            };
                  
            Global.SearchType="Hotel";
                     
            EstimateBox.View.AddHotel(params);
        }

        function _getHotel_WithNewValues(hotelHashKey, roomsHashKeys) {
            var totalAmount = 0;

            var hotel =Global.Hotel;

            _setHotelRooms(hotel, roomsHashKeys);       

            hotel.SelectedRooms.forEach(function (r) {
                totalAmount += r.SelectedRoomOption.TotalAmount;
            });

            hotel.TotalAmount = totalAmount / hotel.TotalGuests;

            if (hotel.SelectedInstallment) {
                hotel.SelectedInstallmentValue = hotel.TotalAmount / hotel.SelectedInstallment.TotalInstallments;
            }
           
            return hotel;
        }

        function _setHotelRooms(hotel, roomsHashKeys) {

            var rooms = [];

            for (var i = 0; i < roomsHashKeys.length; i++) {

                var room = hotel.Rooms.where(function (r) {
                    return r.HashKey == roomsHashKeys[i];
                });

                rooms.push(room[0]);
            }

            hotel.SelectedRooms = rooms;

            return hotel;
        };

    </script>

  
    
 
    

