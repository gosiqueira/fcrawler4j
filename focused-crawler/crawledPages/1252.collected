        hotelDetail.preOrderCompleted = function(data){
        
            hotelDetail.postPage(data);
        
        };

        hotelDetail.preOrderError = function(data){            
            Global.MessagesController.notifyHideMessages();
            if (data.Hotel) {
                //caso a disponibilidade do hotel tenha sido confirmada direciona para checkout
                var package = {
                    Recommendation: thisFunctions.selectedAir,
                    Hotel: thisFunctions.selectedHotel
                }
                thisFunctions.onBuyPackage(package);
            }
            else {
                _gaq.push(['_trackEvent', 'Hotel', 'pre_calculo', 'checkout']);
