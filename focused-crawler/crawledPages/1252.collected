        hotelDetail.changeValueCangooroo = function() {
            var radio = $(this);
            var indexRow = radio.attr("indexRow");
            var groupKey = radio.attr("groupKey");            

            $('.groupKey' + groupKey).removeClass("current").removeClass("last").removeClass("first");
