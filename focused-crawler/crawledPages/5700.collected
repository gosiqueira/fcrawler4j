<div class="activitydeals clearfix" data-destination="<%=deal.Destination%>">
    <figure><a href="<%=deal.Url%>">
        <img class="activitythumbimage" src="<%=deal.picture%>"></a></figure>
    <div class="activityinfo">
        <p><a href="<%=deal.Url%>" class="h4"><%=deal.Title%></a>
        <% if (deal.ImageCaption) { %>
		<br><span class="hot"><%=deal.ImageCaption%></span>
		<% } %>
        <br><span class="h4"><%=deal.Destination%></span></p>
        <p><span class="small"><%=deal.Description%></span>
		 <% if (/^\d{4}-\d{2}-\d{2}$/.test(deal.Date1) && /^\d{4}-\d{2}-\d{2}$/.test(deal.Date2)) { %>
		</br>Para viagem entre <span class="bold"><%=moment(deal.Date1, "YYYY-MM-DD").format("D [de] MMMM") %> e <%=moment(deal.Date2, "YYYY-MM-DD").format("D [de] MMMM") %></span>.
		 <% } %>
        </p>
        <% _.each(deal.items, function(item) { %>
			<h4><a href="<%=deal.Url%>"><%=item.name.replace("diasApenas","dias<br/>Apenas").replace(/([a-zA-Z])(\d)/gi, "$1" + "<br />" + "$2").replace(/([a-z])([A-Z])/g, "$1 $2")%></a></h4>
