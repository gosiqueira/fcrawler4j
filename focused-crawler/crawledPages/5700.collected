<script>
      var trivagoRedirectionEnabled = true;
      var oldPreserveTA = publishing.wizard.preserveTA;

      var trackAction = function (linkID) {
          if(s_exp && s_exp.tl) {
              s_exp.linkDelay=10;
              s_exp.linkTrackVars='prop16,eVar28,eVar18';
              s_exp.linkTrackEvents='None';
              s_exp.prop16=linkID;
              s_exp.eVar28=linkID;
              s_exp.tl(true,'o','RFRR Action Link');
              s_exp.linkDelay=500;
          }	
          return;
      }

      publishing.wizard.preserveTA = function(ci)
      {
          //the cd node may contain a URL.  If it does, when you submit your form, you will get redirected to the location in cd.
          publishing.wizard.searchFormRedirectURL = ci.selectedItem.data.cd;
          if ($(publishing.WIZARD_CONTAINER_ID + " div.lobForm-hotelOnly").css("display") == "block" && ci.selectedItem.data.cd) {
                trackAction('LTC.HLP.ItemSelected');
          }
          oldPreserveTA(ci);    
      };

      var CubaFormSubmitOverride = publishing.wizard.ON_FORM_SUBMIT;

      var addAdultsAndChildrenParams = function() {
          var rooms = parseInt($('#H-NumRoom').val());
          if (rooms == 9) {
              return '&adults=2,2,2,2,2,2,2,2,2';
          } else {
              var adults = [];
              var children = [];
              for (var i = 0; i < rooms; i ++) {
                  adults[i] = $('#H-NumAdult' + (i + 1)).val();
                  var childCount = parseInt($('#H-NumChild' + (i + 1)).val());
                  for (var j = 0; j < childCount; j++) {
                      children.push((i + 1) + '_' + $('#H-Rm' + (i + 1) + 'Child' + (j + 1) + 'Age').val());
                  }
              }
              return "&adults=" + adults.join(',') + "&children=" + children.join(',');
          }
      }

      publishing.wizard.ON_FORM_SUBMIT = function(ev, opts) {
          //searchFormRedirectURL will be undefined if the data.cd node is not in the XML or has no value
          if (typeof publishing.wizard.searchFormRedirectURL != 'undefined') {
              if (publishing.wizard.searchFormRedirectURL.length > 0) {
                  if (trivagoRedirectionEnabled && $(ev.target).attr('class') == 'hotelOnly') {
                      trackAction('LTC.HLP.SearchClicked');
                      var url = "/Trivago-Redirection?" + "startDate=" + encodeURIComponent($('#H-fromDate').val()) +
                          "&endDate=" + encodeURIComponent($('#H-toDate').val()) + addAdultsAndChildrenParams() +
                          "&redirectionURL=" + encodeURIComponent(publishing.wizard.searchFormRedirectURL);
                      window.open(url, '_blank');
                      return false;
                  } else if (publishing.wizard.siteId == "4") {
                      window.location = publishing.wizard.searchFormRedirectURL;
                      return false;
                  } else {
                      return CubaFormSubmitOverride(ev,opts);
                  }
              }
          } else {
              return CubaFormSubmitOverride(ev,opts);
          }
      }

</script>                            </div>
