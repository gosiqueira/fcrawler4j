		    dados   = {};		    
		    dados['act']  	 =  "getAmountComments";
		    dados['site_id'] =  list_site_id;
		    
		    if(list_idcmt.length > 0)
		    {
		    	dados['keys'] =  list_idcmt;
		    
		    	$.ajax({
					url      : "http://live.gazetadopovo.com.br/",
					data     : dados,
					dataType : "jsonp",
					async    : false,
					success  : function(data){
						 $.each( data.result, function( key, val ) 
						 {						 		
						 		if(val.cont_comentario != "0")
						 		{
						 			// adiciona a quantidade de comentários de cada matéria
									$("a[data-idcmt='"+val.refid+"']").html("<span></span> " + val.cont_comentario);
						 		}
						 });
					},
					error: function(jqXHR, textStatus, errorThrown) {
				        console.log("error");
				        console.log(jqXHR.error());
				    }					
				});
		    }
		    
		    if(list_idcmt_article.length > 0)
		    {
		    	dados['keys'] =  list_idcmt_article;
		    
		    	$.ajax({
					url      : "http://live.gazetadopovo.com.br/",
					data     : dados,
					dataType : "jsonp",
					async    : false,
					success  : function(data){
						 $.each( data.result, function( key, val ) 
