    addChromeExtension: function(e, alertItemRef)
    {
        function successCallback ()
        {
            dctk.loggingAdapter.logMessage("ChromeExpediaExtensionSuccessful")
            if(localStorage)
            {
                var date = new Date($.now());
                var month = date.getUTCMonth() + 1;
                var day = date.getUTCDate();
                var year = date.getUTCFullYear();
                var hours = date.getUTCHours();
                var minutes = date.getUTCMinutes();
                var seconds = date.getUTCSeconds();

                var logDate = year + "-" + month + "-" + day + " " + hours + ":" + minutes + ":" + seconds;
                localStorage.setItem("chromeExtensionInstallCTA", logDate);
            }
        }

        function failureCallback (error)
        {
            dctk.loggingAdapter.logError("ChromeExpediaExtensionFailed", error)
        }


         if (window.AlertsMenu)
            {
                AlertsMenu.sendeVar28('HP.notbell');
            }


        chrome.webstore.install("https://chrome.google.com/webstore/detail/cfbpcgmafclgfmpoelldpmiolgblgbel", successCallback, failureCallback);
    }
  };


